<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################################################################################
  #Edit mode																		  #
  ###################################################################################
  -->
  <UIEditModeConfiguration>
    <label>
      <selectTemplate>Sélection d'un modèle</selectTemplate>
      <select-repository>Sélection d'un dépôt de données</select-repository>
      <actionInput>UIEditModeConfiguration</actionInput>
      <workspaceName>Sélection d'un espace de travail</workspaceName>
      <savedPath>Sauvegarder vers</savedPath>
      <repositoryName>Veuillez sélectionner un dépôt de données</repositoryName>
    </label>
    <msg>
      <save-successfully>Les préférences de la portlet ont été sauvegardées</save-successfully>
      <item-not-found>Ce répertoire est invalide !</item-not-found>
      <fileType-empty>Veuillez sélectionner un type de fichier.</fileType-empty>
      <ws-empty>Vous devez choisir un espace de travail.</ws-empty>
    </msg>
    <action>
      <Save>#{word.save}</Save>
    </action>
  </UIEditModeConfiguration>
  <UIPopupWindow>
    <title>
      <UIOneNodePathSelector>Sélectionner un emplacement</UIOneNodePathSelector>
    </title>
  </UIPopupWindow>
  <UIFastContentCreatortForm>
    <msg>
      <name-not-allowed>Ce nom n'est pas permis.</name-not-allowed>
      <in-versioning>Veuillez d'abord effectuer un check-out avant d'ajouter un document</in-versioning>
      <cannot-save>Sauvegarde impossible. Certaines contraintes ne sont pas respectées</cannot-save>
      <saved-successfully>Le document a été sauvegardé avec succès à cet emplacement : '{0}'</saved-successfully>
      <not-support>Impossible de charger le formulaire de '{0}'.</not-support>
      <not-add-folder>Seuls des documents de type 'nt:file' peuvent ôtre ajoutés dans ce répertoire</not-add-folder>
      <access-denied>Accès interdit! Vous n'avez pas les droits suffisant pour accéder à '{0}'</access-denied>
      <path-not-found>Le répertoire '{0}' a été suppimé ou est invalide</path-not-found>
      <node-locked>Le document '{0}' est verrouillé</node-locked>
      <node-isExist>Ce document existe déjà dans '{0}'</node-isExist>
    </msg>
    <action>
      <Save>Sauver</Save>
      <Cancel>Annuler</Cancel>
    </action>
  </UIFastContentCreatortForm>
  <EmptyFieldValidator>
    <msg>
      <empty-input>Valeur manquante dans le champ '{0}'</empty-input>
    </msg>
  </EmptyFieldValidator>
  <UITreeList>
    <info>
      <child-not-found>Ce document n'a pas de sous document</child-not-found>
    </info>
    <header>
      <name>Nom</name>
      <action>Action</action>
    </header>
  </UITreeList>
  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Confirmez-vous la suppression ?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>
  <UIFastContentCreatorPortlet>
    <msg>
      <re-config>Un problème est survenu, veuillez vérifier votre configuration.</re-config>
    </msg>
  </UIFastContentCreatorPortlet>
  <DateTimeValidator>
    <msg>
      <Invalid-input>Le champ '{0}' dont la valeur est '{1}' est invalide</Invalid-input>
    </msg>
  </DateTimeValidator>
  <!--
  ###################################################################################
  # org.exoplatform.ecm.jcr.ECMNameValidator																			  #
  ###################################################################################
  -->
  <ECMNameValidator>
    <msg>
      <Invalid-char>Caractères invalides détectés dans le champ '{0}'.</Invalid-char>
      <empty-input>Valeur manquante dans le champ '{0}' !</empty-input>
    </msg>
  </ECMNameValidator>
  <!--
  ###################################################################################
  # JCR Browser																			  #
  ###################################################################################
  -->
  <UIWorkspaceList>
    <label>
      <workspaceName>Espace de travail</workspaceName>
      <rootNodeInfo>Racine</rootNodeInfo>
    </label>
  </UIWorkspaceList>
  <UITabPane>
    <title>
      <UICategoriesSelectPanel>Sélectionner des catégories</UICategoriesSelectPanel>
      <UISelectedCategoriesGrid>Catégories référencées</UISelectedCategoriesGrid>
    </title>
  </UITabPane>
  <PopupComponent>
    <title>
      <UICategoriesSelector>Ajouter des catégories</UICategoriesSelector>
    </title>
  </PopupComponent>
  <UICategoriesSelector>
    <action>
      <close>Fermer</close>
    </action>
  </UICategoriesSelector>
  
    <PopupComponent>
	  <title>
	    <UIOneTaxonomySelector>Ajouter des catégories</UIOneTaxonomySelector>
	  </title>
  </PopupComponent>
  
  <UITreeTaxonomyList>
    <label>
      <taxonomyTree>Arbre de taxonomies</taxonomyTree>
    </label>
  </UITreeTaxonomyList>
  
  <UIDocumentForm>
  	<msg>
  		<not-support>Impossible de charger le template '{0}'.</not-support>
  		<do-not-permission>Vous n'avez pas accès au template '{0}'.</do-not-permission>
  	</msg>
  </UIDocumentForm>
  <Taxonomy>
		<msg>
			<AccessDeniedException>You don't have permission to access in these taxonomies</AccessDeniedException>		 	
		</msg>
 </Taxonomy>
 <UIFastContentCreatortForm>
    <msg>
      <constraintviolation-exception>Can not save document. Data input is missed or invalid!</constraintviolation-exception>
    </msg>
  </UIFastContentCreatortForm>
</bundle>

