<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #############################################################################
  #			org.exoplatform.workflow.webui.component.monitor											#
  #############################################################################
  -->
  <UIWorkflowAdministrationPortlet>
    <tab>
      <label>Administration</label>
      <text>Admin Workflow Management</text>
    </tab>
    <msg>
      <refresh-session-success>The session was refreshed successfully.</refresh-session-success>
    </msg>
    <label>
      <refresh>Refresh</refresh>
    </label>
  </UIWorkflowAdministrationPortlet>
  <UITabPane>
    <title>
      <UIMonitor>Process Monitor</UIMonitor>
						<UICompletedProcesses>Completed</UICompletedProcesses>
						<UIRunningProcesses>Processing</UIRunningProcesses>
    </title>
  </UITabPane>
  <UIMonitor>
    <header>
      <id>Process Id</id>
      <name>Process Name</name>
      <version>Process Version</version>
      <action>Action</action>
    </header>
    <msg>
      <empty>No business process to monitor</empty>
    </msg>
  </UIMonitor>
  <UIAdministrationManager>
    <tab>
      <label>
        <UITimers>Timers</UITimers>
        <UIMonitor>Monitor</UIMonitor>
      </label>
    </tab>
    <msg>
      <confirm-delete-process>Are you sure to delete this process model?</confirm-delete-process>
    </msg>
    <action>
      <UploadProcess>Upload Process</UploadProcess>
      <title>
        <View>View</View>
        <Delete>Delete</Delete>
      </title>
    </action>
  </UIAdministrationManager>
  <UITimers>
    <header>
      <id>Id</id>
      <name>Name</name>
      <dueDate>Due Date</dueDate>
    </header>
    <msg>
      <empty>No timer set</empty>
    </msg>
  </UITimers>
  <AdministrationPopup>
    <title>
      <UIProcessDetail>Process Detail</UIProcessDetail>
    </title>
  </AdministrationPopup>
  <UploadProcessPopup>
    <title>
      <UIUploadProcess>Upload Process</UIUploadProcess>
    </title>
  </UploadProcessPopup>
  <UIUploadProcess>
    <label>
      <name>Name</name>
      <upload>Select File</upload>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <fileName-error>The file path is not found.</fileName-error>
      <fileName-invalid>The file name contains some invalid characters. Please retype the name or upload another file.</fileName-invalid>
      <process-successful>Upload successfully!</process-successful>
      <data-invalid>Upload failed. Maybe, the process definition file coul not be found or data is invalid.</data-invalid>
    </msg>
  </UIUploadProcess>
  <UIProcessDetailGrid>
    <header>
      <processInstanceId>Process Instance Id</processInstanceId>
      <processId>Process Id</processId>
      <processName>Process Name</processName>
      <startDate>Start Date</startDate>
      <endDate>End Date</endDate>
      <action>Action</action>
    </header>
    <msg>
      <empty>Please select a process that you want to view the detail.</empty>
    </msg>
  </UIProcessDetailGrid>
  <UIProcessDetail>
    <msg>
      <confirm-delete-process>Are you sure to delete this process instance?</confirm-delete-process>      
    </msg>
  </UIProcessDetail>    
  <UITasksGrid>
    <header>
      <id>Id</id>
      <taskName>Task Name</taskName>
      <actorId>Actor Id</actorId>
      <end>End</end>
    </header>
  </UITasksGrid>
  <UIProcessDetail>
    <msg>
      <confirm-delete-completed-process>Are you sure to delete these completed processes?</confirm-delete-completed-process>
      <result-delete-completed-process>There is not any completed process to delete.</result-delete-completed-process>
    </msg>
    <action>
      <FlushAll>Flush All</FlushAll>
      <Cancel>Cancel</Cancel>
    </action>  
  </UIProcessDetail>
  <TaskListOfProcessPopup>
    <title>
      <UITaskListOfProcess>List Tasks of Instance</UITaskListOfProcess>
    </title>
    <tab>
      <label>
        <UITasksGrid>Task</UITasksGrid>
      </label>
    </tab>    
  </TaskListOfProcessPopup>
  <UIRunningProcesses>
    <header>
      <processInstanceId>Process Instance Id</processInstanceId>
      <processId>Process Id</processId>
      <processName>Process Name</processName>
      <startDate>Start Date</startDate>
      <endDate>End Date</endDate>
      <action>Action</action>
    </header>
    <msg>
      <empty>There are no processes to view details.</empty>
    </msg>
    <action>
      <Cancel>Cancel</Cancel>
    </action>
  </UIRunningProcesses>
  <UICompletedProcesses>
    <header>
      <processInstanceId>Process Instance Id</processInstanceId>
      <processId>Process Id</processId>
      <processName>Process Name</processName>
      <startDate>Start Date</startDate>
      <endDate>End Date</endDate>
      <action>Action</action>
    </header>
    <msg>
      <empty>There are no processes to view details.</empty>
    </msg>
    <action>
      <FlushAll>Flush All</FlushAll>
      <Cancel>Cancel</Cancel>
    </action>				
  </UICompletedProcesses>
  <UITaskListOfProcess>    
    <tab>
      <label>
        <UITasksGrid>List Tasks</UITasksGrid>
      </label>
    </tab>
    <action>      
      <Cancel>Cancel</Cancel>
    </action>    
  </UITaskListOfProcess>
	<UIProcessDetail>
    <action>
    	<title>
    		<View>View</View>
				<Delete>Delete</Delete>
    	</title>
    </action>  
  </UIProcessDetail>
</bundle>

