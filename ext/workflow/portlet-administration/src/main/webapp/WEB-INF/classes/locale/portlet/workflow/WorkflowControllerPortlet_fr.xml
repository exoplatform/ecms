<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<bundle>
  <!--
  #############################################################################
  #      org.exoplatform.workflow.webui.component.controller                    #
  #############################################################################
  -->
  <UIWorkflowControllerPortlet>
    <tab>
      <label>Contrôleur</label>
      <text>Administration des Workflows</text>
    </tab>
    <msg>
      <refresh-session-success>La session a été rafraichie avec succès.</refresh-session-success>
    </msg>
    <label>
      <refresh>Rafraîchir</refresh>
    </label>
  </UIWorkflowControllerPortlet>
  <ControllerPopup>
    <title>
      <UITaskManager>Gestion des tâches</UITaskManager>
    </title>
  </ControllerPopup>
  <UITabPane>
    <title>
      <UITaskList>Contrôleur des Tâches</UITaskList>
      <UIBPDefinition>Gestion des Définitions de Processus Métier</UIBPDefinition>
      <UIDocumentContent>Afficher le contenu</UIDocumentContent>
      <UIDocumentForm>Modifier le contenu</UIDocumentForm>
      <UITask>Tâche</UITask>
    </title>
  </UITabPane>
  <UITaskList>
    <header>
      <id>ID Tâche</id>
      <processName>Nom Processus</processName>
      <taskName>Nom de la Tâche</taskName>
      <actor>Acteur</actor>
      <description>Description</description>
      <actorId>Id Acteur</actorId>
      <taskVariables>Variables de la Tâche</taskVariables>
      <processDateStart>Date de début du Processus</processDateStart>
      <action>Action</action>
    </header>
    <label>
      <managerState>Gérer</managerState>
    </label>
    <msg>
      <task-not-found>Vous ne pouvez pas gérer cette tâche. Cette tâche a peut-être été géré par un autre utilisateur.</task-not-found>
      <task-change>La tâche a été modifiée ou supprimée.</task-change>
    </msg>
  </UITaskList>
  <UITaksList>
    <msg>
      <no-task>Aucune tâche assignée</no-task>
    </msg>
  </UITaksList>
  <UITask>
    <label>
      <hide-process-link>Cacher Schéma Processus</hide-process-link>
      <show-process-link>Montrer Schéma Processus</show-process-link>
      <process-diagram-not-available>Schéma du processus n'est pas disponible</process-diagram-not-available>
    </label>
    <title>
      <Delegate>Choisir un utilisateur</Delegate>
      <state-image>Image de l'État</state-image>
    </title>
    <msg>
      <has-not-got-delegate>Veuillez choisir un utilisateur à qui déléguer.</has-not-got-delegate>
      <user-not-exist>Cet utilisateur délégué n'existe pas.</user-not-exist>
    </msg>
  </UITask>
  <PopupDelegatedSelectorId>
    <title>
      <UIUserSelectContainer>Choisir un utilisateur</UIUserSelectContainer>
    </title>
  </PopupDelegatedSelectorId>
  <UIBPDefinition>
    <header>
      <id>Id</id>
      <name>Nom</name>
      <version>Version</version>
      <action>Action</action>
    </header>
    <msg>
      <empty>Aucun Processus Métier à lancer</empty>
    </msg>
  </UIBPDefinition>
  <UIControllerManager>
    <action>
      <title>
        <ManageStart>Gestion du lancement</ManageStart>
      </title>
    </action>
  </UIControllerManager>
  <UIDocumentForm>
    <msg>
      <cannot-save>Une erreur s'est produite lors de l'enregistrement du document.</cannot-save>
      <in-versioning>Veuillez faire un Check Out sur le noeud courant avant d'en ajouter un nouveau.</in-versioning>
      <not-support>Impossible de charger le formulaire '{0}'.</not-support>
      <value-invalid>'{0}' est une valeur requise.</value-invalid>
      <confirm-close>Les changements que vous avez faits seront perdus si vous fermez ce formulaire.</confirm-close>
      <confirm-navigate>Les changements que vous avez fait seront perdus si vous quittez cette page.</confirm-navigate>
    </msg>
  </UIDocumentForm>
  <PopupComponent>
    <title>
      <UIOneTaxonomySelector>Ajouter des Catégories</UIOneTaxonomySelector>
      <UIOneNodePathSelector>Choisir un chemin</UIOneNodePathSelector>
    </title>
  </PopupComponent>
  <UITreeTaxonomyList>
    <label>
      <taxonomyTree>Arbre de catégories</taxonomyTree>
      <rootNodePath>Racine de l'arborescence</rootNodePath>
      <rootNodeInfo>UITreeTaxonomyList</rootNodeInfo>
    </label>
  </UITreeTaxonomyList>
     <UIWorkspaceList>
    <label>
      <workspaceName>Espace de travail</workspaceName>
      <rootNodeInfo>UIWorkspaceList</rootNodeInfo>
      <rootNodePath>Racine de l'arborescence</rootNodePath>
    </label>
  </UIWorkspaceList>
  <rootNodeInfo>
    <title>
      <AddRootNode>Ajouter un noeud racine</AddRootNode>
    </title>
  </rootNodeInfo>
  <PositiveNumberValidator>
    <msg>
      <invalid-input>Le champ « {0} » avec la valeur « {1} » n'est pas valide. Un nombre positif est requis.</invalid-input>
    </msg>
  </PositiveNumberValidator>
</bundle>
