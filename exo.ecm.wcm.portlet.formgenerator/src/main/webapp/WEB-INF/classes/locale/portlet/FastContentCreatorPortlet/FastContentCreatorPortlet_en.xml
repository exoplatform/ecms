      <UIFCCTemplateField>Template options</UIFCCTemplateField>
      <UIFCCLocationFormInputAction>Location to save</UIFCCLocationFormInputAction>
      <UIFCCLocationFormStringInput></UIFCCLocationFormStringInput>
      <UIFCCTemplateFormSelectBox>Select template</UIFCCTemplateFormSelectBox>
      <UIFCCSaveFormStringInput>Custom save button</UIFCCSaveFormStringInput>
      <UIFCCMessageFormTextareaInput>Custom save message</UIFCCMessageFormTextareaInput>
      <UIFCCRedirectFormCheckboxInput>Redirect</UIFCCRedirectFormCheckboxInput>
      <UIFCCRedirectPathFormStringInput>Redirect path</UIFCCRedirectPathFormStringInput>
      <UIFCCActionField>Actions</UIFCCActionField>
