      <save-successfully>Saved to portlet preferences successfully</save-successfully>
      <item-not-found>This path is invalid!</item-not-found>
      <fileType-empty>You have to choose file type before save this configuration.</fileType-empty>
      <ws-empty>You have to choose one workspace.</ws-empty>
      <action-empty>You have to choose a folder with an action</action-empty>
   <get-template>Error when get template.</get-template>
      <set-template-option>Error when set template option.</set-template-option>
      <do-select>Error when do select.</do-select>
      </msg>
    <action>
      <Save>Save</Save>
    </action>
  </UIFCCConfig>

  <UIFCCLocationFormInputAction>
    <title>
      <SelectPath>Select path</SelectPath>
    </title>
  </UIFCCLocationFormInputAction>

  <UIFCCActionList>
    <header>
      <name>Name</name>
      <description>Description</description>
      <instanceOf>Instance of</instanceOf>
      <action>Action</action>
    </header>
    <action>
      <Add>Add</Add>
    </action>
    <msg>
      <empty>No action</empty>
      <remove-popup-first>You must close edit popup before delete this node.</remove-popup-first>
      <confirm-delete-action>Do you really want to delete this action?</confirm-delete-action>
      <template-empty>Template of '{0}' is null or empty</template-empty>
      <template-null>Template is null or you haven't got permission to access this template.</template-null>
      <access-denied>Access denied! You haven't got enough permission to do this action.</access-denied>
    </msg>
  </UIFCCActionList>

  <UIFCCSelectorPopupWindow>
    <title>
      <UIOneNodePathSelector>Select location</UIOneNodePathSelector>
    </title>
  </UIFCCSelectorPopupWindow>

  <UIWorkspaceList>
    <label>
      <workspaceName>Workspace</workspaceName>
      <rootNodePath>Root path</rootNodePath>
      <rootNodeInfo></rootNodeInfo>
    </label>
  </UIWorkspaceList>

  <rootNodeInfo>
    <title>
      <AddRootNode>Add root node</AddRootNode>
    </title>
  </rootNodeInfo>

  <UITreeList>
    <info>
      <child-not-found>This node hasn't got any child</child-not-found>
    </info>
    <header>
      <name>Name</name>
      <action>Action</action>
    </header>
