  <AddMetadata>
    <dialog>
      <label>
        <isDeep>Recursiu</isDeep>
        <nodetypes>Tipus de node</nodetypes>
        <affectedNodeTypeNames>Tipus de Node afectats</affectedNodeTypeNames>
      </label>
    </dialog>
  </AddMetadata>

  <EnableVersioning>
    <dialog>
      <label>
        <isDeep>Recursiu</isDeep>
        <nodetypes>Tipus de node</nodetypes>
        <affectedNodeTypeNames>Tipus de Node afectats</affectedNodeTypeNames>
      </label>
    </dialog>
  </EnableVersioning>
  
  <AutoVersioning>
    <dialog>
      <label>
        <isDeep>Recursiu</isDeep>
        <nodetypes>Tipus de node</nodetypes>
        <affectedNodeTypeNames>Tipus de Node afectats</affectedNodeTypeNames>
      </label>
    </dialog>
