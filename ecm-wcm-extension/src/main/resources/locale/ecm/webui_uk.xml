<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Ім’я</name>
      <action>Add</action>
    </header>
    <info>
      <child-not-found>Дитина не знайдено!</child-not-found>
    </info>
    <action>
      <title>
        <select>Виберіть</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Ім'я категорії</categoryName>
      <categoryPath>Шлях до категорії</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Категорії не збережено.</cannot-save>
      <have-saved>Категорії збережені успішно</have-saved>
      <non-categories>Невірні категорії. Будь ласка, виберіть відповідні категорії.</non-categories>
      <confirm-delete>Видалити цю категорію?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Виберіть користувача</UIPermissionUser>
      <UIPermissionMembership>Вибрати роль</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Вибрати роль</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Виберіть користувача</selectUser>
      <selectGroup>Перегляд і вибір групи</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Вибрати роль</selectUser>
      <selectGroup>Перегляд і вибір групи</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Виберіть групу</selectGroup>
      <selectMembership>Вибрати роль</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Селектор сторінок</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Додати кореневий вузол</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Будь-яке право доступа</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
