    <dialog>
      <label>
        <title>Назва</title>
        <summary>#{word.summary}</summary>
      </label>
    </dialog>
  </RssEnable>

  <!--
  ###################### resoucebundle mix:versionable dialogs template ####
  -->
  <Versionable>
    <dialog>
      <label>
        <version-history>Історія версій</version-history>
        <base-version>Базова версія</base-version>
        <is-checked-out>Видана</is-checked-out>
        <predecessors>Попередники</predecessors>
        <merge-failed>Не вдалося виконати злиття</merge-failed>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Versionable>

  <!--
  ##################### resoucebundle for resouce container dialogs template#####
  -->
  <ResouceContainer>
    <dialog>
      <label>
        <name>Ім’я</name>
        <autoload>Автозавантаження</autoload>
        <minetype>Mime тип</minetype>
        <content>Контент</content>
      </label>
    </dialog>
  </ResouceContainer>
  <!--
  ###################### resoucebundle for validator ####
  -->
  <EmailAddressValidator>
    <msg>
      <Invalid-input>Адреса електронної пошти недійсна.</Invalid-input>
    </msg>
  </EmailAddressValidator>
  <EmptyFieldValidator>
    <msg>
      <empty-input>Поле "{0}" обов'язкове. Будь ласка, введіть ваше значення.</empty-input>
    </msg>
  </EmptyFieldValidator>
  <NumberFormatValidator>
    <msg>
      <Invalid-number>Поле "{0}" є неприпустимим.</Invalid-number>
    </msg>
  </NumberFormatValidator>
