 <!-- New Document i18n -->
  <UIActionBar>
    <tooltip>
      <NewDocument>Nuevo documento</NewDocument>
    </tooltip>
  </UIActionBar>
  
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  <UITabForm>
    <label>
      <newDocument>Nuevo documento</newDocument>
    </label>
  </UITabForm>
  <!-- For new document popup window -->
  <UIPopupWindow>
    <title>
      <UINewDocumentForm>Nuevo documento</UINewDocumentForm>
    </title>
  </UIPopupWindow>
   <!-- For new document form -->
  <UINewDocumentForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
      <Save>Crear documento</Save>
    </action>
    <label>
      <titleTextBox>Nombre del documento:</titleTextBox>
      <typeSelectBox>Tipo de documento:</typeSelectBox>
    </label>
    <msg>
      <name-invalid>Introduce el nombre del documento.</name-invalid>
      <constraint-violation>No puede añadir un documento de tipo '{0}' aquí.</constraint-violation>
      <repository-exception>Ocurrió un error inesperado. Póngase en contacto con su administrador para obtener más detalles.</repository-exception>
      <repository-exception-permission>No tienes permiso para añadir nuevos documentos aquí.</repository-exception-permission>
      <numberformat-exception>No se puede guardar este documento. El nombre proporcionado debe estar en formato numérico.</numberformat-exception>
      <!--DMS 2.4.x; ECM-3553-->
      <not-allow-sameNameSibling>No puedes guardar el documento. Ya existe un elemento con el mismo nombre.</not-allow-sameNameSibling>
    </msg>
  </UINewDocumentForm>
  
</bundle>
