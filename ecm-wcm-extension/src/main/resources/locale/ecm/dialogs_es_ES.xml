      <label>
        <isDeep>Es profundo</isDeep>
        <nodetypes>Tipos de nodo</nodetypes>
        <affectedNodeTypeNames>Tipos de nodo afectados</affectedNodeTypeNames>
      </label>
    </dialog>
  </AddMetadata>

  <EnableVersioning>
    <dialog>
      <label>
        <isDeep>Es profundo</isDeep>
        <nodetypes>Tipos de nodo</nodetypes>
        <affectedNodeTypeNames>Tipos de nodo afectados</affectedNodeTypeNames>
      </label>
    </dialog>
  </EnableVersioning>
  
  <AutoVersioning>
    <dialog>
      <label>
        <isDeep>Es profundo</isDeep>
        <nodetypes>Tipos de nodo</nodetypes>
        <affectedNodeTypeNames>Tipos de nodo afectados</affectedNodeTypeNames>
      </label>
    </dialog>
  </AutoVersioning>

  <AccessibleMedia>
  <dialog>
  <label>
  <captions>Leyendas</captions>
<audioDescription>Descripción audio</audioDescription>
<alternativeText>Texto alternativo</alternativeText>
</label>
</dialog>
<help>
<CaptionsHelp>Las leyendas deben proporcionar el texto del cuadro de diálogo y los sonidos importantes</CaptionsHelp>
<AudioDescriptionHelp>Una descripción audio es una pista narrada del vídeo</AudioDescriptionHelp>
<AlternativeTextHelp>Proporcionar una alternativa accesible de presentar la información</AlternativeTextHelp>
</help>
