<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Navn</name>
      <action>Add</action>
    </header>
    <info>
      <child-not-found>Underkategori ikke funnet!</child-not-found>
    </info>
    <action>
      <title>
        <select>Velg</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Kategorinavn</categoryName>
      <categoryPath>Kategori sti</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Kategorier ble ikke lagret.</cannot-save>
      <have-saved>Kategorier ble lagret</have-saved>
      <non-categories>Kategoriene er feil. Velg riktig kategorier.</non-categories>
      <confirm-delete>Vil du slette denne kategorien?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Velg bruker</UIPermissionUser>
      <UIPermissionMembership>Velg rolle</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Velg rolle</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Velg bruker</selectUser>
      <selectGroup>Bla gjennom og velge en gruppe</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Velg rolle</selectUser>
      <selectGroup>Bla gjennom og velge en gruppe</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Velg gruppe</selectGroup>
      <selectMembership>Velg rolle</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Sidevelger</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Add Root</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Noen tillatelser</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
