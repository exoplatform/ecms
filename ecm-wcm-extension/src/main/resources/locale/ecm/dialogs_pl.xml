<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################### global word for ECM dialogs resoucebundle ####
  -->
  <Ecm>
    <dialog>
      <label>
        <end-time>Czas zakończenia</end-time>
        <start-time>Czas rozpoczęcia</start-time>
        <lifecycle>Cykl życiowy</lifecycle>
        <mimeType>Typ MIME</mimeType>
        <repeat-count>Liczba powtórzeń</repeat-count>
        <time-interval>Odstęp czasu</time-interval>
        <query-items>Elementy zapytania</query-items>
        <schedule-type>Typ harmonogramu</schedule-type>
        <remove>Usuń element</remove>
        <add>Dodaj element</add>
        <expand>Rozwiń to</expand>
      </label>
    </dialog>
  </Ecm>
  
  <UIDocumentForm>
    <label>
      <title>#{word.title}</title>
      <link>Link</link>
      <contentHtml>#{word.content}</contentHtml>
      <content>#{word.content}</content>
      <contentPlain>#{word.content}</contentPlain>
      <name>Nazwa</name>
    <categories>Kategorie</categories>
    </label>
  </UIDocumentForm>
 
  <UICommentForm>
    <label>
      <placeholder>Use @ to mention someone in your comment</placeholder>
    </label>
  </UICommentForm>
 
  <!--
  ###################### resoucebundle for HTMLFile dialogs template ####
  -->
  <HtmlFile>
    <dialog>
      <label>
        <name>Nazwa</name>
        <content>Zawartość</content>
      </label>
    </dialog>
  </HtmlFile>

  <!--
  ###################### resoucebundle for nt:file dialogs template ####
  -->
  <File>
    <dialog>
      <label>
        <name>#{word.name}</name>
        <categories>Kategorie</categories>
        <mimeType>#{Ecm.dialog.label.mimeType}</mimeType>
        <width>Szerokość</width>
        <height>Wysokość</height>
        <content>#{word.content}</content>
        <re-upload>Plik już istnieje. Możesz usunąć i wgrać inny plik.</re-upload>
      </label>
    </dialog>
  </File>

  <UIActionForm>
    <label>
      <description>#{word.description}</description>
      <port>Port</port>
      <userName>#{word.userName}</userName>
      <password>Hasło</password>
      <link>Link</link>
      <to>Wyślij e-mail do</to>
      <actionName>#{word.name}</actionName>
    </label>
  </UIActionForm>
  <EditFormAction>
    <label>
      <description>#{word.description}</description>
      <port>Port</port>
      <userName>#{word.userName}</userName>
      <password>Hasło</password>
      <link>Link</link>
      <to>Wyślij e-mail do</to>
      <actionName>#{word.name}</actionName>
    </label>
  </EditFormAction>

  <!--
  ###################### resoucebundle for exo:addMetadataAction dialogs template ####
  -->
  <AddMetadata>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <metadata>Metadane</metadata>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AddMetadata>

  <!--
  ###################### resoucebundle for exo:autoVersioning dialogs template ####
  -->
  <AutoVersioning>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Wyrażenie Cron</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AutoVersioning>
  <!--
  ###################### resoucebundle for exo:enableVersiong dialogs template ####
  -->
  <EnableVersioning>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Cron Expression</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </EnableVersioning>
  <!--
  ###################### resoucebundle for dc:elementSet dialogs template ####
  -->
  <ElementSet>
    <dialog>
      <label>
        <title>#{word.title}:</title>
        <creator>Twórca:</creator>
        <subject>#{word.subject}:</subject>
        <source>Źródło:</source>
        <contributor>Współtwórca:</contributor>
        <resource-type>Typ zasobu:</resource-type>
        <date>#{word.date}:</date>
        <publisher>Wydawca:</publisher>
        <description>#{word.description}:</description>
        <rights>Prawa:</rights>
        <relation>Relacja:</relation>
        <format>#{word.format}:</format>
        <coverage>Zasięg:</coverage>
        <identifier>Identyfikator:</identifier>
        <language>Język:</language>
      </label>
    </dialog>
  </ElementSet>

  <!--
  ###################### resoucebundle for exo:actionable dialogs template ####
  -->
  <Actionable>
    <dialog>
      <label>
        <primary-type>Typ podstawowy</primary-type>
        <mixin-types>Typy wstawek</mixin-types>
      </label>
    </dialog>
  </Actionable>

  <!--
  ###################### resoucebundle for exo:move dialogs template ####
  -->
  <Move>
    <dialog>
      <label>
        <destination-workpsace>Docelowa obszar roboczy</destination-workpsace>
        <destination-path>Ścieżka docelowa</destination-path>
        <primary-type>Typ podstawowy</primary-type>
        <mixin-types>Typy wstawek</mixin-types>
      </label>
    </dialog>
  </Move>
  <!--
  ###################### resoucebundle for exo:published dialogs template ####
  -->
  <Published>
    <dialog>
      <label>
        <start-publication>Zacznij publikację</start-publication>
        <end-publication>Koniec publikacji</end-publication>
      </label>
    </dialog>
  </Published>
 
  <!--
  ###################### resoucebundle mix:referenceable dialogs template ####
  -->
  <Referenceable>
    <dialog>
      <label>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Referenceable>
  <!--
  ###################### resoucebundle exo:relationable dialogs template ####
  -->
  <Relationable>
    <dialog>
      <label>
        <relation>Relacja</relation>
      </label>
    </dialog>
  </Relationable>
  <!--
  ###################### resoucebundle exo:rss-enable dialogs template ####
  -->
  <RssEnable>
    <dialog>
      <label>
        <title>Tytuł</title>
        <summary>#{word.summary}</summary>
      </label>
    </dialog>
  </RssEnable>

  <!--
  ###################### resoucebundle mix:versionable dialogs template ####
  -->
  <Versionable>
    <dialog>
      <label>
        <version-history>Historia wersji</version-history>
        <base-version>Wersja bazowa</base-version>
        <is-checked-out>Sprawdzono</is-checked-out>
        <predecessors>Przodkowie</predecessors>
        <merge-failed>Scalanie nie powiodło się</merge-failed>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Versionable>

  <!--
  ##################### resoucebundle for resouce container dialogs template#####
  -->
  <ResouceContainer>
    <dialog>
      <label>
        <name>Nazwa</name>
        <autoload>Auto obciążenia</autoload>
        <minetype>Typ MIME</minetype>
        <content>Zawartość</content>
      </label>
    </dialog>
  </ResouceContainer>
  <!--
  ###################### resoucebundle for validator ####
  -->
  <EmailAddressValidator>
    <msg>
      <Invalid-input>Adres e-mail jest nieprawidłowy.</Invalid-input>
    </msg>
  </EmailAddressValidator>
  <EmptyFieldValidator>
    <msg>
      <empty-input>Pole '{0}' jest wymagane. Wprowadź wartość.</empty-input>
    </msg>
  </EmptyFieldValidator>
  <NumberFormatValidator>
    <msg>
      <Invalid-number>Pole '{0}' jest nieprawidłowe.</Invalid-number>
    </msg>
  </NumberFormatValidator>
  <!--
  #####Since version 2.4#######
  ###################### resoucebundle exo:taxonomyAction dialogs template ####
  -->
  <ScriptAction>
    <dialog>
      <label>
        <id>Id</id>
        <name>Nazwa</name>
        <lifecycle>Cykl życiowy</lifecycle>
        <homePath>Główna ścieżka</homePath>
        <targetWorkspace>Docelowy obszar roboczy</targetWorkspace>
        <targetPath>Ścieżka docelowa</targetPath>
      </label>
    </dialog>
  </ScriptAction>
  <UIDialogForm>
   <form>
   <DialogFormException>Nie można zapisać tego dokumentu. Może być problem z ochroną przed odzyskaniem.</DialogFormException>
   </form>
  </UIDialogForm>

