 <!-- New Document i18n -->
  <UIActionBar>
    <tooltip>
      <NewDocument>Yeni Belge</NewDocument>
    </tooltip>
  </UIActionBar>
  
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  <UITabForm>
    <label>
      <newDocument>Yeni Belge</newDocument>
    </label>
  </UITabForm>
  <!-- For new document popup window -->
  <UIPopupWindow>
    <title>
      <UINewDocumentForm>Yeni Belge</UINewDocumentForm>
    </title>
  </UIPopupWindow>
   <!-- For new document form -->
  <UINewDocumentForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
      <Save>Belge Oluştur</Save>
    </action>
    <label>
      <titleTextBox>Belge Adı:</titleTextBox>
      <typeSelectBox>Belge Türü:</typeSelectBox>
    </label>
    <msg>
      <name-invalid>Lütfen belge adını girin.</name-invalid>
      <constraint-violation>Geçerli düğüm '{0}' düğüm türünü eklemenize izin vermez.</constraint-violation>
      <repository-exception>Beklenmeyen bir hata oluştu. Daha fazla ayrıntı için yöneticinize başvurun.</repository-exception>
      <repository-exception-permission>Buraya yeni bir belge ekleme izniniz yok.</repository-exception-permission>
      <numberformat-exception>Bu eylem kaydedemezsiniz. Verilen adı sayısal biçiminde olması gerekir.</numberformat-exception>
      <!--DMS 2.4.x; ECM-3553-->
      <not-allow-sameNameSibling>Belgeyi kaydedemezsiniz. Aynı ada sahip bir öğe zaten var.</not-allow-sameNameSibling>
    </msg>
  </UINewDocumentForm>
  
