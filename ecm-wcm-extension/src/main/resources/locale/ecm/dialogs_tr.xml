<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################### global word for ECM dialogs resoucebundle ####
  -->
  <Ecm>
    <dialog>
      <label>
        <end-time>Bitiş Saati</end-time>
        <start-time>Başlangıç Saati</start-time>
        <lifecycle>Ömrü</lifecycle>
        <mimeType>MIME türü</mimeType>
        <repeat-count>Tekrar Say</repeat-count>
        <time-interval>Zaman Aralığı</time-interval>
        <query-items>Sorgu Öğeleri</query-items>
        <schedule-type>Zamanlama Türü</schedule-type>
        <remove>Öğeyi Kaldır</remove>
        <add>Öğe Ekle</add>
        <expand>Öğeyi Genişlet</expand>
      </label>
    </dialog>
  </Ecm>
  
  <UIDocumentForm>
    <label>
      <title>#{word.title}</title>
      <link>Bağlantı</link>
      <contentHtml>#{word.content}</contentHtml>
      <content>#{word.content}</content>
      <contentPlain>#{word.content}</contentPlain>
      <name>Ad</name>
    <categories>Kategoriler</categories>
    </label>
  </UIDocumentForm>
 
  <UICommentForm>
    <label>
      <placeholder>Yorumlarınızda birinden bahsetmek için @ kullanın</placeholder>
    </label>
  </UICommentForm>
 
  <!--
  ###################### resoucebundle for HTMLFile dialogs template ####
  -->
  <HtmlFile>
    <dialog>
      <label>
        <name>Ad</name>
        <content>İçerik</content>
      </label>
    </dialog>
  </HtmlFile>

  <!--
  ###################### resoucebundle for nt:file dialogs template ####
  -->
  <File>
    <dialog>
      <label>
        <name>#{word.name}</name>
        <categories>Kategoriler</categories>
        <mimeType>#{Ecm.dialog.label.mimeType}</mimeType>
        <width>Genişlik</width>
        <height>Yükseklik</height>
        <content>#{word.content}</content>
        <re-upload>Dosya zaten var. Dosyayı silin ve başka bir dosya yükleyin.</re-upload>
      </label>
    </dialog>
  </File>

  <UIActionForm>
    <label>
      <description>#{word.description}</description>
      <port>Port</port>
      <userName>#{word.userName}</userName>
      <password>Şifre</password>
      <link>Bağlantı</link>
      <to>E-Posta gönder</to>
      <actionName>#{word.name}</actionName>
    </label>
  </UIActionForm>
  <EditFormAction>
    <label>
      <description>#{word.description}</description>
      <port>Port</port>
      <userName>#{word.userName}</userName>
      <password>Şifre</password>
      <link>Bağlantı</link>
      <to>E-Posta gönder</to>
      <actionName>#{word.name}</actionName>
    </label>
  </EditFormAction>

  <!--
  ###################### resoucebundle for exo:addMetadataAction dialogs template ####
  -->
  <AddMetadata>
    <dialog>
      <label>
        <id>Kimlik</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <metadata>Metaveri</metadata>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AddMetadata>

  <!--
  ###################### resoucebundle for exo:autoVersioning dialogs template ####
  -->
  <AutoVersioning>
    <dialog>
      <label>
        <id>Kimlik</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Cron ifade</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AutoVersioning>
  <!--
  ###################### resoucebundle for exo:enableVersiong dialogs template ####
  -->
  <EnableVersioning>
    <dialog>
      <label>
        <id>Kimlik</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Cron İfadesi</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </EnableVersioning>
  <!--
  ###################### resoucebundle for dc:elementSet dialogs template ####
  -->
  <ElementSet>
    <dialog>
      <label>
        <title>#{word.title}:</title>
        <creator>Hazırlayan:</creator>
        <subject>#{word.subject}:</subject>
        <source>Kaynak:</source>
        <contributor>Katılımcı:</contributor>
        <resource-type>Kaynak Türü:</resource-type>
        <date>#{word.date}:</date>
        <publisher>Yayımcı:</publisher>
        <description>#{word.description}:</description>
        <rights>Haklar:</rights>
        <relation>İlişki:</relation>
        <format>#{word.format}:</format>
        <coverage>Kapsam:</coverage>
        <identifier>Tanımlayıcı:</identifier>
        <language>Dil:</language>
      </label>
    </dialog>
  </ElementSet>

  <!--
  ###################### resoucebundle for exo:actionable dialogs template ####
  -->
  <Actionable>
    <dialog>
      <label>
        <primary-type>Birincil Türü</primary-type>
        <mixin-types>Mixin Türü</mixin-types>
      </label>
    </dialog>
  </Actionable>

  <!--
  ###################### resoucebundle for exo:move dialogs template ####
  -->
