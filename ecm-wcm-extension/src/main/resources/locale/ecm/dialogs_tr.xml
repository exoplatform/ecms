    <dialog>
      <label>
        <destination-workpsace>Hedef Çalışma Alanı</destination-workpsace>
        <destination-path>Hedef Yolu</destination-path>
        <primary-type>Birincil Türü</primary-type>
        <mixin-types>Mixin Türü</mixin-types>
      </label>
    </dialog>
  </Move>
  <!--
