    <dialog>
      <label>
        <name>Ad</name>
        <autoload>Otomatik Yükleme</autoload>
        <minetype>MIME Türü</minetype>
        <content>İçerik</content>
      </label>
    </dialog>
  </ResouceContainer>
  <!--
  ###################### resoucebundle for validator ####
  -->
  <EmailAddressValidator>
    <msg>
      <Invalid-input>E-posta adresi geçerli değil.</Invalid-input>
    </msg>
  </EmailAddressValidator>
  <EmptyFieldValidator>
    <msg>
      <empty-input>'{0}' Alanı gereklidir. Lütfen bir değer girin.</empty-input>
    </msg>
  </EmptyFieldValidator>
  <NumberFormatValidator>
    <msg>
      <Invalid-number>'{0}' Alanı geçersiz.</Invalid-number>
    </msg>
  </NumberFormatValidator>
  <!--
