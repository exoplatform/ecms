<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Nazwa</name>
      <action>Dodaj</action>
    </header>
    <info>
      <child-not-found>Nie znaleziono!</child-not-found>
    </info>
    <action>
      <title>
        <select>Wybierz</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Nazwa kategorii</categoryName>
      <categoryPath>Ścieżka kategorii</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Kategorie nie zostały zapisane.</cannot-save>
      <have-saved>Kategorie zostały pomyślnie zapisane</have-saved>
      <non-categories>Kategorie są błędne. Proszę wybrać odpowiednie kategorie.</non-categories>
      <confirm-delete>Czy chcesz usunąć tę kategorię?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Wybierz użytkownika</UIPermissionUser>
      <UIPermissionMembership>Wybierz rolę</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Wybierz rolę</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Wybierz użytkownika</selectUser>
      <selectGroup>Przeglądaj i wybierz grupę</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Wybierz rolę</selectUser>
      <selectGroup>Przeglądaj i wybierz grupę</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Wybierz grupę</selectGroup>
      <selectMembership>Wybierz rolę</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Selektor strony</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Dodaj węzeł główny</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Każda licencja</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
