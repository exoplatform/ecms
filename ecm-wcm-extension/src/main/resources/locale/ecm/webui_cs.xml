<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Jméno</name>
      <action>Přidat</action>
    </header>
    <info>
      <child-not-found>Potomek nebyl nalezen!</child-not-found>
    </info>
    <action>
      <title>
        <select>Vybrat</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Název kategorie</categoryName>
      <categoryPath>Cesta kategorie</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Kategorie nebyly uloženy.</cannot-save>
      <have-saved>Kategorie byly úspěšně uloženy</have-saved>
      <non-categories>Kategorie jsou nesprávné. Prosím vyberte příslušné kategorie.</non-categories>
      <confirm-delete>Přejete si odstranit tuto kategorii?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Vybrat uživatele</UIPermissionUser>
      <UIPermissionMembership>Select Role</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Select Role</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Vybrat uživatele</selectUser>
      <selectGroup>Projít a vybrat skupinu</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Select Role</selectUser>
      <selectGroup>Projít a vybrat skupinu</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Vybrat skupinu</selectGroup>
      <selectMembership>Select Role</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Výběr stránky</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Přidat kořenový uzel</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Jakékoliv oprávnění</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
