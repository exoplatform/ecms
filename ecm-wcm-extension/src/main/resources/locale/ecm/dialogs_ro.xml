 <!-- New Document i18n -->
  <UIActionBar>
    <tooltip>
      <NewDocument>New Document</NewDocument>
    </tooltip>
  </UIActionBar>
  
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  <UITabForm>
    <label>
      <newDocument>New Document</newDocument>
    </label>
  </UITabForm>
  <!-- For new document popup window -->
  <UIPopupWindow>
    <title>
      <UINewDocumentForm>New Document</UINewDocumentForm>
    </title>
  </UIPopupWindow>
   <!-- For new document form -->
  <UINewDocumentForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
      <Save>Create Document</Save>
    </action>
    <label>
      <titleTextBox>Document Name:</titleTextBox>
      <typeSelectBox>Document Type:</typeSelectBox>
    </label>
    <msg>
      <name-invalid>Please enter the document name.</name-invalid>
      <constraint-violation>Modulul curent nu vă permit să adăugaţi modul de tip "{0}".</constraint-violation>
      <repository-exception>Eroare neașteptată. Vă rugăm să contactaţi administratorul pentru mai multe detalii.</repository-exception>
      <repository-exception-permission>You don't have the permission to add a new document here.</repository-exception-permission>
      <numberformat-exception>Imposibil de salvat acest document. Numele furnizat trebuie să fie în format numeric.</numberformat-exception>
      <!--DMS 2.4.x; ECM-3553-->
      <not-allow-sameNameSibling>You can't save the document. An item with the same name already exists.</not-allow-sameNameSibling>
    </msg>
  </UINewDocumentForm>
  
</bundle>
