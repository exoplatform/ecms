 <!-- New Document i18n -->
  <UIActionBar>
    <tooltip>
      <NewDocument>Nuovo Documento</NewDocument>
    </tooltip>
  </UIActionBar>
  
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  <UITabForm>
    <label>
      <newDocument>Nuovo Documento</newDocument>
    </label>
  </UITabForm>
  <!-- For new document popup window -->
  <UIPopupWindow>
    <title>
      <UINewDocumentForm>Nuovo Documento</UINewDocumentForm>
    </title>
  </UIPopupWindow>
   <!-- For new document form -->
  <UINewDocumentForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
      <Save>Crea Documento</Save>
    </action>
    <label>
      <titleTextBox>Nome del Documento:</titleTextBox>
      <typeSelectBox>Tipo di Documento:</typeSelectBox>
    </label>
    <msg>
      <name-invalid>Sei pregato di inserire il nome del documento.</name-invalid>
      <constraint-violation>Il nodo non consente di aggiungere il tipo di nodo '{0}'.</constraint-violation>
      <repository-exception>Errore sconosciuto. Contatta l'amministratore per maggiori dettagli.</repository-exception>
      <repository-exception-permission>Non hai il permesso di aggiungere qui un nuovo documento.</repository-exception-permission>
      <numberformat-exception>Impossible salvare questo documento. Il nome fornito deve essere in formato numerico.</numberformat-exception>
      <!--DMS 2.4.x; ECM-3553-->
      <not-allow-sameNameSibling>Non puoi salvare il documento. Un elemento con lo stesso nome esiste già.</not-allow-sameNameSibling>
    </msg>
  </UINewDocumentForm>
  
