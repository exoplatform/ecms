<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################### global word for ECM dialogs resoucebundle ####
  -->
  <Ecm>
    <dialog>
      <label>
        <end-time>Tempo Fine</end-time>
        <start-time>Tempo Inizio</start-time>
        <lifecycle>Ciclo di Vita</lifecycle>
        <mimeType>Tipo Mime</mimeType>
        <repeat-count>Conteggio Ripetizione</repeat-count>
        <time-interval>Intervallo Tempo</time-interval>
        <query-items>Oggetti Query</query-items>
        <schedule-type>Tipo Schedulato</schedule-type>
        <remove>Rimuovi Oggetto</remove>
        <add>Aggiungi Oggetto</add>
        <expand>Espandere questo</expand>
      </label>
    </dialog>
  </Ecm>
  
  <UIDocumentForm>
    <label>
      <title>#{word.title}</title>
      <link>Collegamento</link>
      <contentHtml>#{word.content}</contentHtml>
      <content>#{word.content}</content>
      <contentPlain>#{word.content}</contentPlain>
      <name>Nome</name>
    <categories>Categorie</categories>
    </label>
  </UIDocumentForm>
 
  <UICommentForm>
    <label>
      <placeholder>A cosa stai lavorando? Utilizzare @ per citare qualcuno</placeholder>
    </label>
  </UICommentForm>
 
  <!--
  ###################### resoucebundle for HTMLFile dialogs template ####
  -->
  <HtmlFile>
    <dialog>
      <label>
        <name>Nome</name>
        <content>Contenuto</content>
      </label>
    </dialog>
  </HtmlFile>

  <!--
  ###################### resoucebundle for nt:file dialogs template ####
  -->
  <File>
    <dialog>
      <label>
        <name>#{word.name}</name>
        <categories>Categorie</categories>
        <mimeType>#{Ecm.dialog.label.mimeType}</mimeType>
        <width>Larghezza</width>
        <height>Altezza</height>
        <content>#{word.content}</content>
        <re-upload>Il file esiste già. Può eliminarlo o caricare un'altro file.</re-upload>
      </label>
    </dialog>
  </File>

  <UIActionForm>
    <label>
      <description>#{word.description}</description>
      <port>Porta</port>
      <userName>#{word.userName}</userName>
      <password>Password</password>
      <link>Collegamento</link>
      <to>Invia Mail A</to>
      <actionName>#{word.name}</actionName>
    </label>
  </UIActionForm>
  <EditFormAction>
    <label>
      <description>#{word.description}</description>
      <port>Porta</port>
      <userName>#{word.userName}</userName>
      <password>Password</password>
      <link>Collegamento</link>
      <to>Invia Mail A</to>
      <actionName>#{word.name}</actionName>
    </label>
  </EditFormAction>

  <!--
  ###################### resoucebundle for exo:addMetadataAction dialogs template ####
  -->
  <AddMetadata>
    <dialog>
      <label>
        <id>ID</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <metadata>Vedi Metadati</metadata>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AddMetadata>

  <!--
  ###################### resoucebundle for exo:autoVersioning dialogs template ####
  -->
  <AutoVersioning>
    <dialog>
      <label>
        <id>ID</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Espressione Cron</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AutoVersioning>
  <!--
  ###################### resoucebundle for exo:enableVersiong dialogs template ####
  -->
  <EnableVersioning>
    <dialog>
      <label>
        <id>ID</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
