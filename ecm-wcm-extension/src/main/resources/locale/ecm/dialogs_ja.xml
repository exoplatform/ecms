<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################### global word for ECM dialogs resoucebundle ####
  -->
  <Ecm>
    <dialog>
      <label>
        <end-time>終了時刻</end-time>
        <start-time>開始時刻</start-time>
        <lifecycle>ライフサイクル</lifecycle>
        <mimeType>Mime タイプ</mimeType>
        <repeat-count>繰り返し回数</repeat-count>
        <time-interval>インターバル</time-interval>
        <query-items>アイテムを検索</query-items>
        <schedule-type>スケジュールタイプ</schedule-type>
        <remove>アイテムを削除</remove>
        <add>アイテムを追加</add>
        <expand>展開する</expand>
      </label>
    </dialog>
  </Ecm>
  
  <UIDocumentForm>
    <label>
      <title>#{word.title}</title>
      <link>リンク</link>
      <contentHtml>#{word.content}</contentHtml>
      <content>#{word.content}</content>
      <contentPlain>#{word.content}</contentPlain>
      <name>名前</name>
    <categories>カテゴリー</categories>
    </label>
  </UIDocumentForm>
 
  <UICommentForm>
    <label>
      <placeholder>コメントに誰かをメンションするには@ を使用します。</placeholder>
    </label>
  </UICommentForm>
 
  <!--
  ###################### resoucebundle for HTMLFile dialogs template ####
  -->
  <HtmlFile>
    <dialog>
      <label>
        <name>名前</name>
        <content>コンテンツ</content>
      </label>
    </dialog>
  </HtmlFile>

  <!--
  ###################### resoucebundle for nt:file dialogs template ####
  -->
  <File>
    <dialog>
      <label>
        <name>#{word.name}</name>
        <categories>カテゴリ</categories>
        <mimeType>#{Ecm.dialog.label.mimeType}</mimeType>
        <width>幅</width>
        <height>高さ</height>
        <content>#{word.content}</content>
        <re-upload>ファイルが既に存在します。再度アップロードすることで置き換えることができます。</re-upload>
      </label>
    </dialog>
  </File>

  <UIActionForm>
    <label>
      <description>#{word.description}</description>
      <port>ポート</port>
      <userName>#{word.userName}</userName>
      <password>パスワード</password>
      <link>リンク</link>
      <to>メール送信</to>
      <actionName>#{word.name}</actionName>
    </label>
  </UIActionForm>
  <EditFormAction>
    <label>
      <description>#{word.description}</description>
      <port>ポート</port>
      <userName>#{word.userName}</userName>
      <password>パスワード</password>
      <link>リンク</link>
      <to>メール送信</to>
      <actionName>#{word.name}</actionName>
    </label>
  </EditFormAction>

  <!--
  ###################### resoucebundle for exo:addMetadataAction dialogs template ####
  -->
  <AddMetadata>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <metadata>メタデータ</metadata>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AddMetadata>

  <!--
  ###################### resoucebundle for exo:autoVersioning dialogs template ####
  -->
  <AutoVersioning>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>表現のクローン</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AutoVersioning>
  <!--
  ###################### resoucebundle for exo:enableVersiong dialogs template ####
  -->
  <EnableVersioning>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>表現のクローン</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </EnableVersioning>
  <!--
  ###################### resoucebundle for dc:elementSet dialogs template ####
  -->
  <ElementSet>
    <dialog>
      <label>
        <title>#{word.title}</title>
        <creator>作成者</creator>
        <subject>#{word.subject}</subject>
        <source>ソース</source>
        <contributor>コントリビュータ</contributor>
        <resource-type>リソースタイプ</resource-type>
        <date>#{word.date}</date>
        <publisher>公開者</publisher>
        <description>#{word.description}</description>
        <rights>権限</rights>
        <relation>関連</relation>
        <format>#{word.format}</format>
        <coverage>範囲</coverage>
        <identifier>識別子</identifier>
        <language>言語</language>
      </label>
    </dialog>
  </ElementSet>

  <!--
  ###################### resoucebundle for exo:actionable dialogs template ####
  -->
  <Actionable>
    <dialog>
      <label>
        <primary-type>基本タイプ</primary-type>
        <mixin-types>ミックスイン・タイプ</mixin-types>
      </label>
    </dialog>
  </Actionable>

  <!--
  ###################### resoucebundle for exo:move dialogs template ####
  -->
  <Move>
    <dialog>
      <label>
        <destination-workpsace>移動先ワークスペース</destination-workpsace>
        <destination-path>移動先パス</destination-path>
        <primary-type>基本タイプ</primary-type>
        <mixin-types>ミックスイン・タイプ</mixin-types>
      </label>
    </dialog>
  </Move>
  <!--
  ###################### resoucebundle for exo:published dialogs template ####
  -->
  <Published>
    <dialog>
      <label>
        <start-publication>公開開始</start-publication>
        <end-publication>公開終了</end-publication>
      </label>
    </dialog>
  </Published>
 
  <!--
  ###################### resoucebundle mix:referenceable dialogs template ####
  -->
  <Referenceable>
    <dialog>
      <label>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Referenceable>
  <!--
  ###################### resoucebundle exo:relationable dialogs template ####
  -->
  <Relationable>
    <dialog>
      <label>
        <relation>関連</relation>
      </label>
    </dialog>
  </Relationable>
  <!--
  ###################### resoucebundle exo:rss-enable dialogs template ####
  -->
  <RssEnable>
    <dialog>
      <label>
        <title>タイトル</title>
        <summary>#{word.summary}</summary>
      </label>
    </dialog>
  </RssEnable>

  <!--
  ###################### resoucebundle mix:versionable dialogs template ####
  -->
  <Versionable>
    <dialog>
      <label>
        <version-history>バージョン履歴</version-history>
        <base-version>ベースバージョン</base-version>
        <is-checked-out>チェックアウト</is-checked-out>
        <predecessors>先行オペレーション</predecessors>
        <merge-failed>マージ失敗</merge-failed>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Versionable>

  <!--
  ##################### resoucebundle for resouce container dialogs template#####
  -->
  <ResouceContainer>
    <dialog>
      <label>
        <name>名前</name>
        <autoload>オートロード</autoload>
        <minetype>Mime タイプ</minetype>
        <content>コンテンツ</content>
      </label>
    </dialog>
  </ResouceContainer>
  <!--
  ###################### resoucebundle for validator ####
  -->
  <EmailAddressValidator>
    <msg>
      <Invalid-input>Eメールアドレスは無効です！</Invalid-input>
    </msg>
  </EmailAddressValidator>
  <EmptyFieldValidator>
    <msg>
      <empty-input>フィールド '{0}' は必須です。</empty-input>
    </msg>
  </EmptyFieldValidator>
  <NumberFormatValidator>
    <msg>
      <Invalid-number>フィールド '{0}' が不正です。</Invalid-number>
    </msg>
  </NumberFormatValidator>
  <!--
  #####Since version 2.4#######
  ###################### resoucebundle exo:taxonomyAction dialogs template ####
  -->
  <ScriptAction>
    <dialog>
      <label>
        <id>Id</id>
        <name>名前</name>
        <lifecycle>ライフサイクル</lifecycle>
        <homePath>ホームパス</homePath>
        <targetWorkspace>ターゲットワークスペース</targetWorkspace>
        <targetPath>ターゲットパス</targetPath>
      </label>
    </dialog>
  </ScriptAction>
  <UIDialogForm>
   <form>
   <DialogFormException>ドキュメントが保存できません。保存前のインターセプタに問題があるかもしれません。</DialogFormException>
   </form>
  </UIDialogForm>

