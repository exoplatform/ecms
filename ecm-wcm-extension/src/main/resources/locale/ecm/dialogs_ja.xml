  <AddMetadata>
    <dialog>
      <label>
        <isDeep>Is deep</isDeep>
        <nodetypes>ノードタイプ</nodetypes>
        <affectedNodeTypeNames>影響を受けるノードタイプ</affectedNodeTypeNames>
      </label>
