<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################### global word for ECM dialogs resoucebundle ####
  -->
  <Ecm>
    <dialog>
      <label>
        <end-time>Pabaigos laikas</end-time>
        <start-time>Pradžios laikas</start-time>
        <lifecycle>Gyvenimo ciklas</lifecycle>
        <mimeType>MIME tipas</mimeType>
        <repeat-count>Kartojimų skaičius</repeat-count>
        <time-interval>Laiko intervalas</time-interval>
        <query-items>Užklausos elementai</query-items>
        <schedule-type>Tvarkaraščio tipas</schedule-type>
        <remove>Pašalinti elementą</remove>
        <add>Pridėti elementą</add>
        <expand>Išplėsti šį</expand>
      </label>
    </dialog>
  </Ecm>
  
  <UIDocumentForm>
    <label>
      <title>#{word.title}</title>
      <link>Nuoroda</link>
      <contentHtml>#{word.content}</contentHtml>
      <content>#{word.content}</content>
      <contentPlain>#{word.content}</contentPlain>
      <name>Pavadinimas</name>
    <categories>Kategorijos</categories>
    </label>
  </UIDocumentForm>
 
  <UICommentForm>
    <label>
      <placeholder>Use @ to mention someone in your comment</placeholder>
    </label>
  </UICommentForm>
 
  <!--
  ###################### resoucebundle for HTMLFile dialogs template ####
  -->
  <HtmlFile>
    <dialog>
      <label>
        <name>Pavadinimas</name>
        <content>Turinys</content>
      </label>
    </dialog>
  </HtmlFile>

  <!--
  ###################### resoucebundle for nt:file dialogs template ####
  -->
  <File>
    <dialog>
      <label>
        <name>#{word.name}</name>
        <categories>Kategorijos</categories>
        <mimeType>#{Ecm.dialog.label.mimeType}</mimeType>
        <width>Width</width>
        <height>Height</height>
        <content>#{word.content}</content>
        <re-upload>Failas jau egzistuoja. Jūs galėtumėte jį ištrinti ir įkelti kitą failą.</re-upload>
      </label>
    </dialog>
  </File>

  <UIActionForm>
    <label>
      <description>#{word.description}</description>
      <port>Jungtis</port>
      <userName>#{word.userName}</userName>
      <password>Slaptažodis</password>
      <link>Nuoroda</link>
      <to>Siųsti laišką</to>
      <actionName>#{word.name}</actionName>
    </label>
  </UIActionForm>
  <EditFormAction>
    <label>
      <description>#{word.description}</description>
      <port>Jungtis</port>
      <userName>#{word.userName}</userName>
      <password>Slaptažodis</password>
      <link>Nuoroda</link>
      <to>Siųsti laišką</to>
      <actionName>#{word.name}</actionName>
    </label>
  </EditFormAction>

  <!--
  ###################### resoucebundle for exo:addMetadataAction dialogs template ####
  -->
  <AddMetadata>
    <dialog>
      <label>
        <id>ID</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <metadata>Metaduomenys</metadata>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AddMetadata>

  <!--
  ###################### resoucebundle for exo:autoVersioning dialogs template ####
  -->
  <AutoVersioning>
    <dialog>
      <label>
        <id>ID</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Cron išraiška</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AutoVersioning>
  <!--
  ###################### resoucebundle for exo:enableVersiong dialogs template ####
  -->
  <EnableVersioning>
    <dialog>
      <label>
        <id>ID</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Cron Expression</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </EnableVersioning>
  <!--
  ###################### resoucebundle for dc:elementSet dialogs template ####
  -->
  <ElementSet>
    <dialog>
      <label>
        <title>#{word.title}:</title>
        <creator>Kūrėjas:</creator>
        <subject>#{word.subject}:</subject>
        <source>Šaltinis:</source>
        <contributor>Talkininkas:</contributor>
        <resource-type>Išteklių tipas:</resource-type>
        <date>#{word.date}:</date>
        <publisher>Leidėjas:</publisher>
        <description>#{word.description}:</description>
        <rights>Teisės:</rights>
        <relation>Ryšys:</relation>
        <format>#{word.format}:</format>
        <coverage>Aprėptis:</coverage>
        <identifier>Identifikatorius:</identifier>
        <language>Kalba:</language>
      </label>
    </dialog>
  </ElementSet>

  <!--
  ###################### resoucebundle for exo:actionable dialogs template ####
  -->
  <Actionable>
    <dialog>
      <label>
        <primary-type>Pagrindinis tipas</primary-type>
        <mixin-types>"Mixin" tipas</mixin-types>
      </label>
    </dialog>
  </Actionable>

  <!--
  ###################### resoucebundle for exo:move dialogs template ####
  -->
  <Move>
    <dialog>
      <label>
        <destination-workpsace>Paskirties darbo sritis</destination-workpsace>
        <destination-path>Paskirties kelias</destination-path>
        <primary-type>Pagrindinis tipas</primary-type>
        <mixin-types>"Mixin" tipas</mixin-types>
      </label>
    </dialog>
  </Move>
  <!--
  ###################### resoucebundle for exo:published dialogs template ####
  -->
  <Published>
    <dialog>
      <label>
        <start-publication>Pradėti paskelbimą</start-publication>
        <end-publication>Baigti paskelbimą</end-publication>
      </label>
    </dialog>
  </Published>
 
  <!--
  ###################### resoucebundle mix:referenceable dialogs template ####
  -->
  <Referenceable>
    <dialog>
      <label>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Referenceable>
  <!--
  ###################### resoucebundle exo:relationable dialogs template ####
  -->
  <Relationable>
    <dialog>
      <label>
        <relation>Ryšys</relation>
      </label>
    </dialog>
  </Relationable>
  <!--
  ###################### resoucebundle exo:rss-enable dialogs template ####
  -->
  <RssEnable>
    <dialog>
      <label>
        <title>Pavadinimas</title>
        <summary>#{word.summary}</summary>
      </label>
    </dialog>
  </RssEnable>

  <!--
  ###################### resoucebundle mix:versionable dialogs template ####
  -->
  <Versionable>
    <dialog>
      <label>
        <version-history>Versijų istorija</version-history>
        <base-version>Bazinė versija</base-version>
        <is-checked-out>Yra išregistruotas</is-checked-out>
        <predecessors>Pirmtakai</predecessors>
        <merge-failed>Sulieti nepavyko</merge-failed>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Versionable>

  <!--
  ##################### resoucebundle for resouce container dialogs template#####
  -->
  <ResouceContainer>
    <dialog>
      <label>
        <name>Vardas</name>
        <autoload>Automatinis krovimas</autoload>
        <minetype>MIME tipas</minetype>
        <content>Turinys</content>
      </label>
    </dialog>
  </ResouceContainer>
  <!--
  ###################### resoucebundle for validator ####
  -->
  <EmailAddressValidator>
    <msg>
      <Invalid-input>Elektroninio pašto adresas yra neteisingas.</Invalid-input>
    </msg>
  </EmailAddressValidator>
  <EmptyFieldValidator>
    <msg>
      <empty-input>'{0}' yra reikalingas laukas. Įveskite savo reikšmę.</empty-input>
    </msg>
  </EmptyFieldValidator>
  <NumberFormatValidator>
    <msg>
      <Invalid-number>Laukas '{0}' yra netinkamas.</Invalid-number>
    </msg>
  </NumberFormatValidator>
  <!--
  #####Since version 2.4#######
  ###################### resoucebundle exo:taxonomyAction dialogs template ####
  -->
  <ScriptAction>
    <dialog>
      <label>
        <id>ID</id>
        <name>Vardas</name>
        <lifecycle>Gyvenimo ciklas</lifecycle>
        <homePath>Namų kelias</homePath>
        <targetWorkspace>Tikslinė darbo sritis</targetWorkspace>
        <targetPath>Tikslo kelias</targetPath>
      </label>
    </dialog>
  </ScriptAction>
  <UIDialogForm>
   <form>
   <DialogFormException>Negalima išsaugoti šio dokumento. Gali būti problema susijusi su prieš-išsaugojimo perėmeju.</DialogFormException>
   </form>
  </UIDialogForm>

