      <label>
        <isDeep>Yra giliai</isDeep>
        <nodetypes>Mazgo tipai</nodetypes>
        <affectedNodeTypeNames>Įtakoti mazgo tipai</affectedNodeTypeNames>
      </label>
    </dialog>
  </ScriptAction>

  <AddMetadata>
    <dialog>
      <label>
        <isDeep>Yra giliai</isDeep>
        <nodetypes>Mazgo tipai</nodetypes>
        <affectedNodeTypeNames>Įtakoti mazgo tipai</affectedNodeTypeNames>
      </label>
    </dialog>
  </AddMetadata>

  <EnableVersioning>
    <dialog>
      <label>
        <isDeep>Yra giliai</isDeep>
        <nodetypes>Mazgo tipai</nodetypes>
        <affectedNodeTypeNames>Įtakoti mazgo tipai</affectedNodeTypeNames>
      </label>
    </dialog>
  </EnableVersioning>
  
  <AutoVersioning>
    <dialog>
      <label>
        <isDeep>Yra giliai</isDeep>
        <nodetypes>Mazgo tipai</nodetypes>
        <affectedNodeTypeNames>Įtakoti mazgo tipai</affectedNodeTypeNames>
      </label>
    </dialog>
  </AutoVersioning>

  <AccessibleMedia>
  <dialog>
  <label>
  <captions>Nuorodinės antraštės</captions>
<audioDescription>Garso aprašymas</audioDescription>
<alternativeText>Alternatyvus tekstas</alternativeText>
</label>
</dialog>
<help>
<CaptionsHelp>Nuorodinės antraštės turi pateikti dialogo tekstą ir svarbius garsus</CaptionsHelp>
<AudioDescriptionHelp>Garso aprašymas yra video įgarsinimo takelis</AudioDescriptionHelp>
<AlternativeTextHelp>Pateikti prieinamą, alternatyvų informacijos pateikimo būdą</AlternativeTextHelp>
</help>
