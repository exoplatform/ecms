<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Pangalan</name>
      <action>Dagdag</action>
    </header>
    <info>
      <child-not-found>Hindi matagpuan ang bata!</child-not-found>
    </info>
    <action>
      <title>
        <select>Piliin</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Pangalan ng Kategorya</categoryName>
      <categoryPath>Kategorya sa landas</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Mga kategorya ay hindi na-save.</cannot-save>
      <have-saved>Mga kategorya ay tagumpay na na-save</have-saved>
      <non-categories>Ang mga kategorya ay mali. Pakiusap na pumili ng wasto na mga kategorya.</non-categories>
      <confirm-delete>Nais mo ba na burahin ang kategorya na ito?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Pumili ng Gumagamit</UIPermissionUser>
      <UIPermissionMembership>Pumili ng Papel na Ginagampanan</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Pumili ng Papel na Ginagampanan</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Pumili ng Gumagamit</selectUser>
      <selectGroup>Mag-browse at pumili ng pangkat</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Pumili ng Papel na Ginagampanan</selectUser>
      <selectGroup>Mag-browse at pumili ng pangkat</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Pumili ng grupo</selectGroup>
      <selectMembership>Pumili ng Papel na Ginagampanan</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Tagapili ng pahina</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Magdagdag ng Ugat</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Kahit anong Permiso</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
