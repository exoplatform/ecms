<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################### global word for ECM dialogs resoucebundle ####
  -->
  <Ecm>
    <dialog>
      <label>
        <end-time>Tapusin ang Oras</end-time>
        <start-time>Umpisa ng Oras</start-time>
        <lifecycle>Ang Ikot ng Buhay</lifecycle>
        <mimeType>Ang Uri ng Mime</mimeType>
        <repeat-count>Ulitin ang Pagbibilang</repeat-count>
        <time-interval>Ang Pagitan sa Oras</time-interval>
        <query-items>Katanungan sa mga Aytem</query-items>
