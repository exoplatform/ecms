        <remove>Alisin ang aytem</remove>
        <add>Magdagdag ng aytem</add>
        <expand>Ang Palawakin Ito</expand>
      </label>
    </dialog>
  </Ecm>
  
  <UIDocumentForm>
    <label>
      <title>#{word.title}</title>
      <link>Ang Link</link>
      <contentHtml>#{word.content}</contentHtml>
      <content>#{word.content}</content>
      <contentPlain>#{word.content}</contentPlain>
      <name>Ang pangalan</name>
    <categories>Mga kategorya</categories>
    </label>
  </UIDocumentForm>
 
  <UICommentForm>
    <label>
      <placeholder>Use @ to mention someone in your comment</placeholder>
    </label>
  </UICommentForm>
 
  <!--
  ###################### resoucebundle for HTMLFile dialogs template ####
  -->
  <HtmlFile>
    <dialog>
      <label>
        <name>Pangalan</name>
        <content>Ang Nilalaman</content>
      </label>
    </dialog>
  </HtmlFile>

  <!--
  ###################### resoucebundle for nt:file dialogs template ####
  -->
  <File>
    <dialog>
      <label>
        <name>#{word.name}</name>
        <categories>Mga Kategorya</categories>
        <mimeType>#{Ecm.dialog.label.mimeType}</mimeType>
        <width>Ang Lapad</width>
        <height>Ang Taas</height>
        <content>#{word.content}</content>
        <re-upload>Ang Payl ay umiiral na. Maari mong burahin at i-upload ang ibang payl.</re-upload>
      </label>
    </dialog>
  </File>

  <UIActionForm>
    <label>
      <description>#{word.description}</description>
      <port>Ang Port</port>
      <userName>#{word.userName}</userName>
      <password>Password</password>
      <link>Ang Link</link>
      <to>Ipadala ang Liham</to>
      <actionName>#{word.name}</actionName>
    </label>
  </UIActionForm>
  <EditFormAction>
    <label>
      <description>#{word.description}</description>
      <port>Port</port>
      <userName>#{word.userName}</userName>
      <password>Password</password>
      <link>Ang Link</link>
      <to>Send Mail To</to>
      <actionName>#{word.name}</actionName>
    </label>
  </EditFormAction>

  <!--
  ###################### resoucebundle for exo:addMetadataAction dialogs template ####
  -->
  <AddMetadata>
    <dialog>
      <label>
        <id>Ang Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
