<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Όνομα</name>
      <action>Add</action>
    </header>
    <info>
      <child-not-found>Δεν βρέθηκαν θυγατρικοί κόμβοι!</child-not-found>
    </info>
    <action>
      <title>
        <select>Επιλογή</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Όνομα κατηγορίας</categoryName>
      <categoryPath>Διαδρομή Κατηγορίας</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Οι Κατηγορίες δεν αποθηκεύτηκαν.</cannot-save>
      <have-saved>Οι Κατηγορίες αποθηκεύτηκαν με επιτυχία</have-saved>
      <non-categories>Οι κατηγορίες είναι λάθος. Παρακαλώ επιλέξτε τις κατάλληλες κατηγορίες.</non-categories>
      <confirm-delete>Θέλετε να διαγράψετε αυτήν την κατηγορία;</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Επιλογή χρήστη</UIPermissionUser>
      <UIPermissionMembership>Επιλογή ρόλου</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Επιλογή ρόλου</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Επιλογή χρήστη</selectUser>
      <selectGroup>Περιηγηθείτε και επιλέξτε μια ομάδα</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Επιλογή ρόλου</selectUser>
      <selectGroup>Περιηγηθείτε και επιλέξτε μια ομάδα</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Επιλογή ομάδας</selectGroup>
      <selectMembership>Επιλογή ρόλου</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Επιλογέας σελίδας</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Προσθήκη κόμβου-ρίζα</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Όλοι</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
