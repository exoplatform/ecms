  <ScriptAction>
    <dialog>
      <label>
        <isDeep>Podrobneje</isDeep>
        <nodetypes>Vrste vozlišč</nodetypes>
        <affectedNodeTypeNames>Vpliva na vrste vozlišč</affectedNodeTypeNames>
      </label>
    </dialog>
  </ScriptAction>

  <AddMetadata>
    <dialog>
      <label>
        <isDeep>Podrobneje</isDeep>
        <nodetypes>Vrste vozlišč</nodetypes>
        <affectedNodeTypeNames>Vpliva na vrste vozlišč</affectedNodeTypeNames>
      </label>
    </dialog>
  </AddMetadata>

  <EnableVersioning>
    <dialog>
      <label>
        <isDeep>Podrobneje</isDeep>
        <nodetypes>Vrste vozlišč</nodetypes>
        <affectedNodeTypeNames>Vpliva na vrste vozlišč</affectedNodeTypeNames>
      </label>
    </dialog>
  </EnableVersioning>
  
  <AutoVersioning>
    <dialog>
      <label>
        <isDeep>Podrobneje</isDeep>
        <nodetypes>Vrste vozlišč</nodetypes>
        <affectedNodeTypeNames>Vpliva na vrste vozlišč</affectedNodeTypeNames>
      </label>
    </dialog>
  </AutoVersioning>

  <AccessibleMedia>
  <dialog>
  <label>
  <captions>Napisi</captions>
<audioDescription>Avdio opis</audioDescription>
<alternativeText>Nadomestno besedilo</alternativeText>
</label>
</dialog>
<help>
<CaptionsHelp>Napisi bi naj zagotovili besedilo pogovornh oken in pomembnih zvokov</CaptionsHelp>
<AudioDescriptionHelp>Zvočni opis je govorjena sled videa</AudioDescriptionHelp>
<AlternativeTextHelp>Zagotoviti dostopen, alternativni način predstavitve informacij</AlternativeTextHelp>
</help>
