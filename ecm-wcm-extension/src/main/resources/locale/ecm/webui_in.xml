<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Nama</name>
      <action>Tambah</action>
    </header>
    <info>
      <child-not-found>Anak tidak ditemukan!</child-not-found>
    </info>
    <action>
      <title>
        <select>Memilih</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Nama Kategori</categoryName>
      <categoryPath>Path kategori</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Kategori tidak tersimpan.</cannot-save>
      <have-saved>Kategori telah berhasil disimpan</have-saved>
      <non-categories>Kategori adalah salah. Silakan pilih kategori yang sesuai.</non-categories>
      <confirm-delete>Apakah Kamu ingin untuk menghapus kategori ini?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Pilih Pengguna</UIPermissionUser>
      <UIPermissionMembership>Select Role</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Select Role</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Pilih Pengguna</selectUser>
      <selectGroup>Jelajahi dan pilih grup</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Select Role</selectUser>
      <selectGroup>Jelajahi dan pilih grup</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Pilih Grup</selectGroup>
      <selectMembership>Select Role</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Pemilih halaman</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Tambahkan root</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Semua izin</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
