<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Название</name>
      <action>Добавить</action>
    </header>
    <info>
      <child-not-found>Дочерний элемент не найден!</child-not-found>
    </info>
    <action>
      <title>
        <select>Выбрать</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Название категории</categoryName>
      <categoryPath>Путь к категории</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Категории не были сохранены.</cannot-save>
      <have-saved>Категории были успешно сохранены</have-saved>
      <non-categories>Неверные категории. Пожалуйста, выберите соответствующие категории.</non-categories>
      <confirm-delete>Вы хотите удалить эту категорию?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Выберите пользователя</UIPermissionUser>
      <UIPermissionMembership>Select Role</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Select Role</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Выберите пользователя</selectUser>
      <selectGroup>Просмотреть и выбрать группу</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Select Role</selectUser>
      <selectGroup>Просмотреть и выбрать группу</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Выбрать группу</selectGroup>
      <selectMembership>Select Role</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Указатель страницы</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Добавить корневой узел</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Любое разрешение</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
