<configuration
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.exoplatform.org/xml/ns/kernel_1_2.xsd http://www.exoplatform.org/xml/ns/kernel_1_2.xsd"
    xmlns="http://www.exoplatform.org/xml/ns/kernel_1_2.xsd">

  <external-component-plugins>
    <target-component>org.exoplatform.portal.config.NavigationCategoryService</target-component>
    <component-plugin>
      <name>NavigationCategoryPlugin</name>
      <set-method>addPlugin</set-method>
      <type>org.exoplatform.portal.config.NavigationCategoryPlugin</type>
      <init-params>
        <value-param>
          <name>category.order</name>
          <value>50</value>
        </value-param>
        <value-param>
          <name>uri.order</name>
          <value>20</value>
        </value-param>
        <value-param>
          <name>category</name>
          <value>content</value>
        </value-param>
        <value-param>
          <name>uri</name>
          <value>siteExplorer</value>
        </value-param>
      </init-params>
    </component-plugin>
    <component-plugin>
      <name>NavigationCategoryPlugin</name>
      <set-method>addPlugin</set-method>
      <type>org.exoplatform.portal.config.NavigationCategoryPlugin</type>
      <init-params>
        <value-param>
          <name>category.order</name>
          <value>50</value>
        </value-param>
        <value-param>
          <name>uri.order</name>
          <value>30</value>
        </value-param>
        <value-param>
          <name>category</name>
          <value>content</value>
        </value-param>
        <value-param>
          <name>uri</name>
          <value>wcmAdmin</value>
        </value-param>
      </init-params>
    </component-plugin>
  </external-component-plugins>

</configuration>
