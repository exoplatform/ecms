<bundle>
  <UIExoStart>
    <tooltip>
      <homepage>Homepage</homepage>
    </tooltip>
  </UIExoStart>

  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Do you want to delete ?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>

  <Drives>
    <label>
      <ManagedSites>Sites Management</ManagedSites>
      <SharedSite>Shared Site</SharedSite>
    </label>
  </Drives>

  <Views>
    <label>
      <wcm-view>WCM View</wcm-view>
      <admin-view>Admin View</admin-view>
      <icon-view>Icon View</icon-view>
      <thumbails-view>Thumbnails View</thumbails-view>
      <cover-flow>Cover Flow View</cover-flow>
    </label>
  </Views>

  <UIActionBar>
    <obtion>
      <Publication>Publication</Publication>
      <System>System</System>
    </obtion>
  </UIActionBar>

  <UIPageSelectorPanel>
    <header>
      <path>Path</path>
      <action>Action</action>
    </header>
    <msg>
      <child-not-found>No page found</child-not-found>
    </msg>
  </UIPageSelectorPanel>

  <UIContentDialogFormPopupWindow>
    <title>
      <UIContentDialogPreference>Webcontent preference</UIContentDialogPreference>
    </title>
  </UIContentDialogFormPopupWindow>

  <UITabPane>
    <title>
      <UIContentBrowsePanelOne>Content browser</UIContentBrowsePanelOne>
      <UIContentBrowsePanelMulti>Content browser</UIContentBrowsePanelMulti>
      <UIContentBrowsePanelFolder>Folder browser</UIContentBrowsePanelFolder>
      <UIContentSearchForm>Content search form</UIContentSearchForm>
      <UIContentSearchResult>Search result</UIContentSearchResult>
      <UIContentViewer>Preview content</UIContentViewer>
      <WebContentMetadataPopup>Property selector</WebContentMetadataPopup>
      <WebContentNodeTypePopup>Nodetype selector</WebContentNodeTypePopup>
      <UIPermissionManager>Permission manager</UIPermissionManager>
    </title>
  </UITabPane>

  <UIContentBrowsePanelOne>
    <selectBoxValues>
      <FilterBy>Filter by</FilterBy>
      <WebContent>Web Content</WebContent>
      <DMSDocument>Document</DMSDocument>
      <Media>Media</Media>
    </selectBoxValues>
    <msg>
      <node-checkout>Node is checked out. Please check in node before do this function</node-checkout>
    </msg>
  </UIContentBrowsePanelOne>

  <UIContentBrowsePanelMulti>
    <selectBoxValues>
      <FilterBy>Filter by</FilterBy>
      <WebContent>Web Content</WebContent>
      <DMSDocument>Document</DMSDocument>
      <Media>Media</Media>
    </selectBoxValues>
  </UIContentBrowsePanelMulti>

  <UIContentBrowsePanelOne>
    <label>
      <LeftColumn>Select a folder to display its children as content</LeftColumn>
      <RightColumn>Below is the possible content can be selected</RightColumn>
    </label>
  </UIContentBrowsePanelOne>

  <UIContentBrowsePanelMulti>
    <label>
      <LeftColumn>Select a folder to display its children as content</LeftColumn>
      <RightColumn>Below is the possible content can be selected</RightColumn>
    </label>
  </UIContentBrowsePanelMulti>

  <UIContentBrowsePanelFolder>
    <label>
      <LeftColumn>Select a folder to display its children as content</LeftColumn>
      <RightColumn>Below is the possible folder can be selected</RightColumn>
    </label>
  </UIContentBrowsePanelFolder>

  <UISelectedContentGrid>
    <label>
      <name>Name</name>
      <add>Add</add>
      <delete>Delete</delete>
    </label>
    <msg>
      <no-content-found>No content found</no-content-found>
      <non-content>You didn't select any content</non-content>
    </msg>
  </UISelectedContentGrid>

  <UICLVContentSelectedGrid>
    <action>
      <SaveCategories>Save</SaveCategories>
      <Cancel>Cancel</Cancel>
    </action>
  </UICLVContentSelectedGrid>

  <UIContentBrowsePathSelector>
    <header>
      <name>Name</name>
      <publicationstate>Publication state</publicationstate>
      <action>Action</action>
    </header>
    <msg>
      <child-not-found>No content found</child-not-found>
    </msg>
  </UIContentBrowsePathSelector>

  <UIContentBrowsePathSelectorFolder>
    <header>
      <name>Name</name>
      <publicationstate>Publication state</publicationstate>
      <action>Action</action>
    </header>
    <msg>
      <child-not-found>No folder found</child-not-found>
    </msg>
  </UIContentBrowsePathSelectorFolder>

  <UIContentSearchForm>
    <title>
      <Location>Site Name</Location>
      <SearchByName>Search by name</SearchByName>
      <Fulltext>A word or phrase in context</Fulltext>
      <Property>Property</Property>
      <Contain>Contain</Contain>
      <FromDate>from date</FromDate>
      <ToDate>to date</ToDate>
      <DocumentType>Document Type</DocumentType>
    </title>
    <label>
      <option>
        <CREATED>Created</CREATED>
        <MODIFIED>Modified</MODIFIED>
      </option>
    </label>
    <action>
      <SearchWebContent>Search</SearchWebContent>
    </action>
    <msg>
      <empty-field>Please input valid keyword for search</empty-field>
      <invalid-format>Invalid format datetime in field '{0}'</invalid-format>
      <invalid-date>"To Date" have to greater than "From Date"</invalid-date>
      <invalid-nodeType>Node type "{0}" is invalid! Please check again.</invalid-nodeType>
      <name-not-allowed>There are some invalid characters in this field. Type another value, please! </name-not-allowed>
    </msg>
  </UIContentSearchForm>

  <WebContentMetadataPopup>
    <title>
      <UIContentPropertySelector>Property Selector</UIContentPropertySelector>
    </title>
  </WebContentMetadataPopup>

  <UIContentPropertySelector>
    <action>
      <Add>Add</Add>
      <Cancel>Cancel</Cancel>
    </action>
    <label>
      <metadataType>Metadata type</metadataType>
      <property_select>Property</property_select>
    </label>
  </UIContentPropertySelector>

  <WebContentNodeTypePopup>
    <title>
      <UIContentNodeTypeSelector>Nodetype Selector</UIContentNodeTypeSelector>
    </title>
  </WebContentNodeTypePopup>

  <UIContentNodeTypeSelector>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
  </UIContentNodeTypeSelector>

  <UIContentSearchResult>
    <header>
      <title>Title</title>
      <score>Score</score>
      <publicationstate>Publication state</publicationstate>
      <action>Action</action>
    </header>
    <title>
      <Select>Select</Select>
      <View>View</View>
    </title>
  </UIContentSearchResult>

  <UIContentDialogForm>
    <action>
      <SaveDraft>Save Draft</SaveDraft>
      <FastPublish>Fast Publish</FastPublish>
      <Preferences>Preferences</Preferences>
      <Close>Close</Close>
    </action>
    <msg>
      <access-denied>
        You don't have permission add categories for this document.
   	</access-denied>
      <exception>
        Have any exception raising while perform add categories for this document.
   	</exception>
      <non-categories>Categories are wrong. Please select good categories.</non-categories>
    </msg>
  </UIContentDialogForm>

  <UIPermissionManager>
    <label>
      <UIPermissionInputSetWithAction>Permission</UIPermissionInputSetWithAction>
      <UIPermissionStringInput></UIPermissionStringInput>
      <UIAccessibleCheckboxInput>Access Right</UIAccessibleCheckboxInput>
      <UIEditableCheckboxInput>Edit Right</UIEditableCheckboxInput>
    </label>
    <action>
      <Save>Save</Save>
      <Clear>Clear</Clear>
      <title>
        <Edit>Edit</Edit>
        <Delete>Delete</Delete>
      </title>
    </action>
  </UIPermissionManager>

  <UIPermissionManagerGrid>
    <header>
      <owner>Permission</owner>
      <accessible>Accessible</accessible>
      <editable>Editable</editable>
      <action>Action</action>
    </header>
    <msg>
      <confirm-delete-permission>Are you sure to delete ?</confirm-delete-permission>
      <no-permission-remove>Don't have permission.</no-permission-remove>
      <access-denied>Access denied.</access-denied>
      <node-locked>The path element (0) or one of its parents is locked.</node-locked>
      <userOrGroup-required>Require select user or group.</userOrGroup-required>
      <checkbox-require>You should set right.</checkbox-require>
      <node-locked>The item with path {0} or upper level is locked.</node-locked>
      <node-checkedin>You must check out first</node-checkedin>
    </msg>
  </UIPermissionManagerGrid>

  <UIPermissionInputSetWithAction>
    <title>
      <SelectUser>Select user</SelectUser>
      <SelectMember>Select member</SelectMember>
      <AddAny>Add any permission</AddAny>
    </title>
  </UIPermissionInputSetWithAction>

  <UIUserSelectorPopupWindow>
    <title>
      <UIUserContainer>User selector</UIUserContainer>
    </title>
  </UIUserSelectorPopupWindow>

  <UIGroupSelectorPopupWindow>
    <title>
      <UIGroupMemberSelector>Group selector</UIGroupMemberSelector>
    </title>
  </UIGroupSelectorPopupWindow>

  <UIGroupMemberSelector>
    <label>
      <selectGroup>Select group</selectGroup>
      <selectMembership>Select membership</selectMembership>
    </label>
  </UIGroupMemberSelector>

  <UIContentDialogPreference>
    <action>
      <Back>Back</Back>
    </action>
  </UIContentDialogPreference>

  <ZeroValidator>
    <msg>
      <Invalid-number>Zero is not accepted in field '{0}'</Invalid-number>
    </msg>
  </ZeroValidator>

  <UIDropDownPageTemp>
    <item>
      <WCMPageConfigs>WCM page configs</WCMPageConfigs>
      <ColumnPageConfigs>Column page configs</ColumnPageConfigs>
      <RowPageConfigs>Row page configs</RowPageConfigs>
      <TabsPageConfigs>Tabs page configs</TabsPageConfigs>
      <MixPageConfigs>Mix page config</MixPageConfigs>
    </item>
  </UIDropDownPageTemp>

  <UIWizardPageSelectLayoutForm>
    <label>
      <SingleContentViewer>Single content viewer</SingleContentViewer>
      <ContentListViewer>Content list viewer</ContentListViewer>
      <Search>Search</Search>
      <Sitemap>Sitemap</Sitemap>
      <TwoColumnsLayout>Two columns layout</TwoColumnsLayout>
      <ThreeColumnsLayout>Three columns layout</ThreeColumnsLayout>
      <TwoRowsLayout>Two rows layout</TwoRowsLayout>
      <ThreeRowsLayout>Three rows layout</ThreeRowsLayout>
      <TwoTabsLayout>Two tabs layout</TwoTabsLayout>
      <ThreeTabsLayout>Three tabs layout</ThreeTabsLayout>
      <TwoColumnsOneRowLayout>Two columns One row layout</TwoColumnsOneRowLayout>
      <OneRowTwoColumnsLayout>One row Two columns layout</OneRowTwoColumnsLayout>
      <ThreeRowsTwoColumnsLayout>Three rows Two columns layout </ThreeRowsTwoColumnsLayout>
    </label>
  </UIWizardPageSelectLayoutForm>

  <Signin>
    <welcome>Welcome</welcome>
    <login>Login</login>
    <logout>Logout</logout>
    <language>Change language</language>
    <register>Register</register>
  </Signin>
  <UIDriveForm>
	<label>
		<wcm-category-view>Category View</wcm-category-view>	
		<wcm-view>WCM content View</wcm-view>
	</label>
  </UIDriveForm>
  
  <ContentSelector>
    <title>
      <PersonalDrives>Personal drives</PersonalDrives>
      <GeneralDrives>General drives</GeneralDrives>
      <GroupDrives>Group drives</GroupDrives>
      <Private>Private</Private>
      <Public>Public</Public>
      <BackupCenter>Backup Center</BackupCenter>
      <CategorizedDocumentsCenter>Categorized Documents Center</CategorizedDocumentsCenter>
      <CollaborationCenter>Collaboration Center</CollaborationCenter>
      <DigitalAssetsManagementCenter>Digital Assets Management Center</DigitalAssetsManagementCenter>
      <DMSAdministration>DMS Administration</DMSAdministration>
      <DocumentsManagementCenter>Documents Management Center</DocumentsManagementCenter>
      <LiveDocumentsCenter>Live Documents Center</LiveDocumentsCenter>
      <PendingDocumentsCenter>Pending Documents Center</PendingDocumentsCenter>
      <RecordsManagementCenter>Records Management Center</RecordsManagementCenter>
      <SharedUsersSpace>Shared Users Space</SharedUsersSpace>
      <Systemfiles>System files</Systemfiles>
      <ValidationRequestsDocumentsCenter>Validation Requests Documents Center</ValidationRequestsDocumentsCenter>
      <ManagedSites>Sites Management</ManagedSites>
      <SharedSite>Shared Site</SharedSite>
    </title>
  </ContentSelector>
  
  <UICategoryNavigationTargetPathPopupWindow>
    <title>
      <UIPageSelector>Page selector</UIPageSelector>
    </title>
  </UICategoryNavigationTargetPathPopupWindow>
  
  <UIDocumentInfo>
    <msg>
      <access-denied-exception>Access denied. You may not have the privileges on reading or node removed or changed</access-denied-exception>
    </msg>
  </UIDocumentInfo>
  <UIActionForm>
    <msg>
      <xss-vulnerability-character>Field '{0}' contains XSS vulnerability characters</xss-vulnerability-character>
    </msg>
  </UIActionForm>  
