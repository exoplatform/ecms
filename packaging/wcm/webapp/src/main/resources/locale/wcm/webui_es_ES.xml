      <DocumentsManagementCenter>Centro de Gestión de Documentos</DocumentsManagementCenter>
      <LiveDocumentsCenter>centro de documentación en vivo</LiveDocumentsCenter>
      <PendingDocumentsCenter>Centro de Documentos Pendientes</PendingDocumentsCenter>
      <RecordsManagementCenter>Centro de gestión de registros</RecordsManagementCenter>
      <SharedUsersSpace>Espacio de Usuarios compartidos</SharedUsersSpace>
      <Systemfiles>Archivos del Sistema</Systemfiles>
      <ValidationRequestsDocumentsCenter>Centro de Pedido de Validación de Documentos</ValidationRequestsDocumentsCenter>
      <ManagedSites>Gestión de Sitios</ManagedSites>
      <SharedSite>Sitio compartido</SharedSite>
    </title>
  </ContentSelector>
  
  <UICategoryNavigationTargetPathPopupWindow>
    <title>
      <UIPageSelector>Selector Página</UIPageSelector>
    </title>
  </UICategoryNavigationTargetPathPopupWindow>
  
  <UIDocumentInfo>
    <msg>
      <access-denied-exception>Acceso denegado. Tal vez no ha leído permiso, o el nodo puede haber sido eliminado o modificado.</access-denied-exception>
    </msg>
  </UIDocumentInfo>
  <UIPortalApplication>
    <title>
      <abort>Anular</abort>
