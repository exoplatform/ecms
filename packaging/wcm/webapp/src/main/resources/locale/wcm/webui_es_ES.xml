    <register>Registro</register>
  </Signin>
  <UIDriveForm>
<label>
<wcm-category-view>Vista de Categoría</wcm-category-view>
<wcm-view>Vista del contenido WCM</wcm-view>
</label>
  </UIDriveForm>
  
  <ContentSelector>
    <title>
      <PersonalDrives>Drives personales</PersonalDrives>
      <GeneralDrives>Drives generales</GeneralDrives>
