<bundle>
  <UIExoStart>
    <tooltip>
      <homepage>Página de inicio</homepage>
    </tooltip>
  </UIExoStart>

  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>¿Está seguro que desea eliminar este elemento?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>

  <Drives>
    <label>
      <ManagedSites>Gestión de Sitios</ManagedSites>
      <SharedSite>Sitio compartido</SharedSite>
    </label>
  </Drives>

  <Views>
    <label>
      <wcm-view>Web</wcm-view>
      <admin-view>Administrador</admin-view>
      <icon-view>Iconos</icon-view>
      <thumbails-view>Miniaturas</thumbails-view>
      <list-view>Lista</list-view>
      <authoring-view>Autoría</authoring-view>
      <wcm-category-view>Categorías</wcm-category-view>
      <file-view>Vista de la gestión de archivos</file-view>
    </label>
  </Views>

  <UIActionBar>
    <obtion>
      <Publication>Publicación</Publication>
      <System>Sistema</System>
    </obtion>
  </UIActionBar>

  <UIPageSelectorPanel>
    <header>
      <path>Ruta</path>
      <action>#{word.action}</action>
    </header>
    <msg>
      <child-not-found>Página no encontrada.</child-not-found>
    </msg>
    <title>
      <select>Seleccionar</select>
    </title>
  </UIPageSelectorPanel>

  <UIContentDialogFormPopupWindow>
    <title>
      <UIContentDialogPreference>Preferencia de contenido web</UIContentDialogPreference>
    </title>
  </UIContentDialogFormPopupWindow>

  <UITabPane>
    <title>
      <UIContentBrowsePanelOne>Navegador de contenido</UIContentBrowsePanelOne>
      <UIContentBrowsePanelMulti>Navegador de contenido</UIContentBrowsePanelMulti>
      <UIContentBrowsePanelFolder>Explorador de carpetas</UIContentBrowsePanelFolder>
      <UIContentSearchForm>Formulario de búsqueda de contenido</UIContentSearchForm>
      <UIContentSearchResult>Resultado de Búsqueda</UIContentSearchResult>
      <UIContentViewer>Vista previa de contenido</UIContentViewer>
