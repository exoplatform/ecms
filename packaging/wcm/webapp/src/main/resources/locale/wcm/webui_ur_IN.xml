    </title>
  </UIGroupSelectorPopupWindow>

  <UIGroupMemberSelector>
    <label>
      <selectGroup>Select Group</selectGroup>
      <selectMembership>Select Membership</selectMembership>
    </label>
  </UIGroupMemberSelector>

  <UIContentDialogPreference>
    <action>
      <Back>Back</Back>
    </action>
  </UIContentDialogPreference>

  <ZeroValidator>
    <msg>
