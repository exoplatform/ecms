    <item>
      <WCMPageConfigs>WCM Page Configurations</WCMPageConfigs>
      <ColumnPageConfigs>Column Page Configurations</ColumnPageConfigs>
      <RowPageConfigs>Row Page Configurations</RowPageConfigs>
      <TabsPageConfigs>Tabs Page Configurations</TabsPageConfigs>
      <MixPageConfigs>Mixed</MixPageConfigs>
    </item>
  </UIDropDownPageTemp>

