<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################### global word for ECM dialogs resoucebundle ####
  -->
  <Ecm>
    <dialog>
      <label>
        <end-time>Hora de finalización</end-time>
        <start-time>Hora de inicio</start-time>
        <lifecycle>Ciclo de vida</lifecycle>
        <mimeType>Tipo Mime</mimeType>
        <repeat-count>Repita el conteo</repeat-count>
        <time-interval>Intervalo de tiempo</time-interval>
        <query-items>Elementos de Consulta</query-items>
        <schedule-type>Tipo de Programa</schedule-type>
        <remove>Eliminar Item</remove>
        <add>Añadir Item</add>
        <expand>Extender este</expand>
      </label>
    </dialog>
  </Ecm>
  
  <UIDocumentForm>
    <label>
      <title>#{word.title}</title>
      <link>Enlace</link>
      <contentHtml>#{word.content}</contentHtml>
      <content>#{word.content}</content>
      <contentPlain>#{word.content}</contentPlain>
      <name>Nombre</name>
    <categories>Categorías</categories>
    </label>
  </UIDocumentForm>
 
  <!--
  ###################### resoucebundle for HTMLFile dialogs template ####
  -->
  <HtmlFile>
    <dialog>
      <label>
        <name>Nombre</name>
        <content>Contenido</content>
      </label>
    </dialog>
  </HtmlFile>

  <!--
  ###################### resoucebundle for nt:file dialogs template ####
  -->
  <File>
    <dialog>
      <label>
        <name>#{word.name}</name>
        <categories>Categorías</categories>
        <mimeType>#{Ecm.dialog.label.mimeType}</mimeType>
        <width>Anchura</width>
        <height>Altura</height>
        <content>#{word.content}</content>
        <re-upload>El archivo ya existe. Puede eliminarlo y cargar otro archivo.</re-upload>
      </label>
    </dialog>
  </File>

  <UIActionForm>
    <label>
      <description>#{word.description}</description>
      <port>Puerto</port>
      <userName>#{word.userName}</userName>
      <password>Contraseña</password>
      <link>Enlace</link>
      <to>Enviar correo a</to>
      <actionName>#{word.name}</actionName>
    </label>
  </UIActionForm>
  <EditFormAction>
    <label>
      <description>#{word.description}</description>
      <port>Puerto</port>
      <userName>#{word.userName}</userName>
      <password>Contraseña</password>
      <link>Enlace</link>
      <to>Enviar correo a</to>
      <actionName>#{word.name}</actionName>
    </label>
  </EditFormAction>
 
  <!--
  ###################### resoucebundle for exo:addMetadataAction dialogs template ####
  -->
  <AddMetadata>
    <dialog>
      <label>
        <id>ID</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <metadata>Metadatos</metadata>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AddMetadata>

  <!--
  ###################### resoucebundle for exo:autoVersioning dialogs template ####
  -->
  <AutoVersioning>
    <dialog>
      <label>
        <id>ID</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Expresión de cron</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AutoVersioning>
  <!--
  ###################### resoucebundle for exo:enableVersiong dialogs template ####
  -->
  <EnableVersioning>
    <dialog>
      <label>
        <id>ID</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Cron Expression</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </EnableVersioning>
  <!--
  ###################### resoucebundle for dc:elementSet dialogs template ####
  -->
  <ElementSet>
    <dialog>
      <label>
        <title>#{word.title}:</title>
        <creator>Creador:</creator>
        <subject>#{word.subject}:</subject>
        <source>Fuente:</source>
        <contributor>Contribuyente:</contributor>
        <resource-type>Tipo de Recursos:</resource-type>
        <date>#{word.date}:</date>
        <publisher>Editorial:</publisher>
        <description>#{word.description}:</description>
        <rights>Derechos:</rights>
        <relation>Relación:</relation>
        <format>#{word.format}:</format>
        <coverage>Cobertura:</coverage>
        <identifier>Identificador:</identifier>
        <language>idioma:</language>
      </label>
    </dialog>
  </ElementSet>

  <!--
  ###################### resoucebundle for exo:actionable dialogs template ####
  -->
  <Actionable>
    <dialog>
      <label>
        <primary-type>Tipo primario</primary-type>
        <mixin-types>Tipos Mixin</mixin-types>
      </label>
    </dialog>
  </Actionable>

  <!--
  ###################### resoucebundle for exo:move dialogs template ####
  -->
  <Move>
    <dialog>
      <label>
        <destination-workpsace>Destinación del espacio de trabajo</destination-workpsace>
        <destination-path>Ruta de destino</destination-path>
        <primary-type>Tipo primario</primary-type>
        <mixin-types>Tipos Mixin</mixin-types>
      </label>
    </dialog>
  </Move>
  <!--
  ###################### resoucebundle for exo:published dialogs template ####
  -->
  <Published>
    <dialog>
      <label>
        <start-publication>Comenzar la publicación</start-publication>
        <end-publication>Publicación final</end-publication>
      </label>
    </dialog>
  </Published>
 
  <!--
  ###################### resoucebundle mix:referenceable dialogs template ####
  -->
  <Referenceable>
    <dialog>
      <label>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Referenceable>
  <!--
  ###################### resoucebundle exo:relationable dialogs template ####
  -->
  <Relationable>
    <dialog>
      <label>
        <relation>Relación</relation>
      </label>
    </dialog>
  </Relationable>
  <!--
  ###################### resoucebundle exo:rss-enable dialogs template ####
  -->
  <RssEnable>
    <dialog>
      <label>
        <title>Título</title>
        <summary>#{word.summary}</summary>
      </label>
    </dialog>
  </RssEnable>

  <!--
  ###################### resoucebundle mix:versionable dialogs template ####
  -->
  <Versionable>
    <dialog>
      <label>
        <version-history>Historia de la versión</version-history>
        <base-version>Versión base</base-version>
        <is-checked-out>Está retirado</is-checked-out>
        <predecessors>Antecesores</predecessors>
        <merge-failed>La unión falló</merge-failed>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Versionable>

  <!--
  ##################### resoucebundle for resouce container dialogs template#####
  -->
  <ResouceContainer>
    <dialog>
      <label>
        <name>Nombre</name>
        <autoload>Carga automática</autoload>
        <minetype>Tipo MIME</minetype>
        <content>Contenido</content>
      </label>
    </dialog>
  </ResouceContainer>
  <!--
  ###################### resoucebundle for validator ####
  -->
  <EmailAddressValidator>
    <msg>
      <Invalid-input>El correo electrónico es inválido.</Invalid-input>
    </msg>
  </EmailAddressValidator>
  <EmptyFieldValidator>
    <msg>
      <empty-input>El campo '{0}' es necesario.  Por favor introduzca su valor.</empty-input>
    </msg>
  </EmptyFieldValidator>
  <NumberFormatValidator>
    <msg>
      <Invalid-number>El campo '{0}' es inválido.</Invalid-number>
    </msg>
  </NumberFormatValidator>
  <!--
  #####Since version 2.4#######

  ###################### resoucebundle exo:taxonomyAction dialogs template ####
  -->
  <ScriptAction>
    <dialog>
      <label>
        <id>ID</id>
        <name>Nombre</name>
        <lifecycle>Ciclo de vida</lifecycle>
        <homePath>Ruta de Inicio</homePath>
        <targetWorkspace>El espacio de trabajo meta</targetWorkspace>
        <targetPath>Ruta de destino</targetPath>
      </label>
    </dialog>
  </ScriptAction>
  <UIDialogForm>
   <form>
   <DialogFormException>No se puede guardar este documento. Puede haber un problema con el interceptor ahorro previo.</DialogFormException>
   </form>
  </UIDialogForm>

  <!--
  #####Since version 2.6#######

  ###################### resoucebundle exo:taxonomyAction dialogs template ####
  -->
<no>No</no>
<yes>Si</yes>
<true>Cierto</true>
<false>Falso</false>
<Electronic>Electrónico</Electronic>
<Paper>Papel</Paper>
<NONE>Ninguno</NONE>
<CLASSIFIED>Clasificado</CLASSIFIED>
<UNCLASSIFIED>Sin clasificar</UNCLASSIFIED>
<NOCONTRACT>Ningún Contrato</NOCONTRACT>
<one-minute>Un Minuto</one-minute>
<hourly>Por hora</hourly>
<daily>Diario</daily>
<quarterly>Trimestral</quarterly>
<yearly>Anualmente</yearly>
<ten-years>Diez Años</ten-years>
<read>Acción de Usuario</read>
  <schedule>Planificador</schedule>
  <node_added>Adición del Contenido</node_added>
  <node_removed>Eliminación del Contenido</node_removed>
  <property_added>Añadir Propiedades</property_added>
  <property_removed>Eliminación de la propiedad</property_removed>
  <property_changed>Modificación de Propiedades</property_changed>

  <ScriptAction>
    <dialog>
      <label>
        <isDeep>Es profundo</isDeep>
        <nodetypes>Tipos de nodo</nodetypes>
        <affectedNodeTypeNames>Tipos de nodo afectados</affectedNodeTypeNames>
      </label>
    </dialog>
  </ScriptAction>

  <AddMetadata>
    <dialog>
      <label>
        <isDeep>Es profundo</isDeep>
        <nodetypes>Tipos de nodo</nodetypes>
        <affectedNodeTypeNames>Tipos de nodo afectados</affectedNodeTypeNames>
      </label>
    </dialog>
  </AddMetadata>

  <EnableVersioning>
    <dialog>
      <label>
        <isDeep>Es profundo</isDeep>
        <nodetypes>Tipos de nodo</nodetypes>
        <affectedNodeTypeNames>Tipos de nodo afectados</affectedNodeTypeNames>
      </label>
    </dialog>
  </EnableVersioning>
  
  <AutoVersioning>
    <dialog>
      <label>
        <isDeep>Es profundo</isDeep>
        <nodetypes>Tipos de nodo</nodetypes>
        <affectedNodeTypeNames>Tipos de nodo afectados</affectedNodeTypeNames>
      </label>
    </dialog>
  </AutoVersioning>

  <AccessibleMedia>
  <dialog>
  <label>
  <captions>Leyendas</captions>
<audioDescription>Descripción audio</audioDescription>
<alternativeText>Texto alternativo</alternativeText>
</label>
</dialog>
<help>
<CaptionsHelp>Las leyendas deben proporcionar el texto del cuadro de diálogo y los sonidos importantes</CaptionsHelp>
<AudioDescriptionHelp>Una descripción audio es una pista narrada del vídeo</AudioDescriptionHelp>
<AlternativeTextHelp>Proporcionar una alternativa accesible de presentar la información</AlternativeTextHelp>
</help>
</AccessibleMedia>
  
</bundle>
