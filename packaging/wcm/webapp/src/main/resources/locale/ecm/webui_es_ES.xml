<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Nombre</name>
      <action>Añadir</action>
    </header>
    <info>
      <child-not-found>¡El subnodo no fué encontrado!</child-not-found>
    </info>
    <action>
      <title>
        <select>Seleccionar</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Nombre de la Categoría</categoryName>
      <categoryPath>Ruta de la Categoría</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Las categorías no fueron guardadas.</cannot-save>
      <have-saved>Las categorías fueron guardadas exitosamente</have-saved>
      <non-categories>Las categorías están equivocadas. Por favor, seleccione las categorías apropiadas.</non-categories>
      <confirm-delete>¿Quiere eliminar esta categoría?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Seleccionar Usuario</UIPermissionUser>
      <UIPermissionMembership>Seleccione la afiliación</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Seleccione la afiliación</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Seleccionar Usuario</selectUser>
      <selectGroup>Navega y selecciona un Grupo</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Seleccione la afiliación</selectUser>
      <selectGroup>Navega y selecciona un Grupo</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Seleccionar Grupo</selectGroup>
      <selectMembership>Seleccione la afiliación</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Selector de página</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Añadir Carpeta raíz</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Cualquier Permiso</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
