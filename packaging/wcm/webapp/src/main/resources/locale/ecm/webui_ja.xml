<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>名前</name>
      <action>追加</action>
    </header>
    <info>
      <child-not-found>子要素が見つかりません！</child-not-found>
    </info>
    <action>
      <title>
        <select>選択</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>カテゴリ名</categoryName>
      <categoryPath>カテゴリパス</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>カテゴリは保存されていません。</cannot-save>
      <have-saved>カテゴリが保存されました。</have-saved>
      <non-categories>カテゴリが間違っています。正しいカテゴリを選んでください。</non-categories>
      <confirm-delete>カテゴリを削除しますか</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>ユーザの選択</UIPermissionUser>
      <UIPermissionMembership>メンバシップの選択</UIPermissionMembership>
      <TaxoSelectUserOrGroup>メンバシップの選択</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>ユーザの選択</selectUser>
      <selectGroup>グループの閲覧と選択</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>メンバシップの選択</selectUser>
      <selectGroup>グループの閲覧と選択</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>グループの選択</selectGroup>
      <selectMembership>メンバシップの選択</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>ページ選択</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>ルートノードの追加</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>権限</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
