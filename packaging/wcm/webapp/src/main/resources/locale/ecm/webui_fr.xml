<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Nom</name>
      <action>Ajouter</action>
    </header>
    <info>
      <child-not-found>Pas de sous-noeud.</child-not-found>
    </info>
    <action>
      <title>
        <select>Sélectionner</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Nom de catégorie</categoryName>
      <categoryPath>Chemin de catégorie</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Les catégories n'ont pas été enregistrées.</cannot-save>
      <have-saved>Les catégories ont été enregistrées avec succès.</have-saved>
      <non-categories>Veuillez sélectionner des catégories valides.</non-categories>
      <confirm-delete>Etes-vous sûr de vouloir supprimer cette catégorie ?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  #	 org.exoplatform.ecm.webui.selector.UIPermissionSelector		       #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Sélectionner un Utilisateur</UIPermissionUser>
      <UIPermissionMembership>Rôle</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Sélectionnez un rôle</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Sélectionner un Utilisateur</selectUser>
      <selectGroup>Groupe</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Rôle</selectUser>
      <selectGroup>Groupe</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Groupe</selectGroup>
      <selectMembership>Sélectionnez un rôle</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Sélecteur de Page</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Ajouter le Noeud Racine</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Toute Personne</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
