      <name>الاسم</name>
    <categories>الفئات</categories>
    </label>
  </UIDocumentForm>
 
  <UICommentForm>
    <label>
      <placeholder>Use @ to mention someone in your comment</placeholder>
    </label>
  </UICommentForm>
 
  <!--
