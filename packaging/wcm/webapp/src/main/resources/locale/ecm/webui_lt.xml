<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Pavadinimas</name>
      <action>Pridėti</action>
    </header>
    <info>
      <child-not-found>Atšaka nerasta!</child-not-found>
    </info>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Kategorijos pavadinimas</categoryName>
      <categoryPath>Kategorijos kelias</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Kategorijos neišsaugotos.</cannot-save>
      <have-saved>Kategorijos sėkmingai išsaugotos</have-saved>
      <non-categories>Kategorijos yra neteisingos. Prašome pasirinkti atitinkamas kategorijas.</non-categories>
      <confirm-delete>Ar norite ištrinti šią kategoriją?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Pasirinkti vartotoją</UIPermissionUser>
      <UIPermissionMembership>Pasirinkti narystę</UIPermissionMembership>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Pasirinkti vartotoją</selectUser>
      <selectGroup>Naršyti ir pasirinkti grupę</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Pasirinkti narystę</selectUser>
      <selectGroup>Naršyti ir pasirinkti grupę</selectGroup>
    </label>
  </UIPermissionMembership>
</bundle>
