<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################### global word for ECM dialogs resoucebundle ####
  -->
  <Ecm>
    <dialog>
      <label>
        <end-time>Heure de fin</end-time>
        <start-time>Heure de début</start-time>
        <lifecycle>Cycle de vie</lifecycle>
        <mimeType>Type Mime</mimeType>
        <repeat-count>Nombre d'essais</repeat-count>
        <time-interval>Intervalle entre 2 essais</time-interval>
        <query-items>Eléments de la requête</query-items>
        <schedule-type>Type de planification</schedule-type>
        <remove>Supprimer</remove>
        <add>Ajouter</add>
        <expand>Étendre</expand>
      </label>
    </dialog>
  </Ecm>
 
  <UIDocumentForm>
    <label>
      <title>#{word.title}</title>
      <link>Lien</link>
      <contentHtml>#{word.content}</contentHtml>
      <content>#{word.content}</content>
      <contentPlain>#{word.content}</contentPlain>
      <name>Nom</name>
	    <categories>Catégories</categories>
    </label>
  </UIDocumentForm>
 
  <!--
  ###################### resoucebundle for HTMLFile dialogs template ####
  -->
  <HtmlFile>
    <dialog>
      <label>
        <name>Nom</name>
        <content>Contenu</content>
      </label>
    </dialog>
  </HtmlFile>

  <!--
  ###################### resoucebundle for nt:file dialogs template ####
  -->
  <File>
    <dialog>
      <label>
        <name>#{word.name}</name>
        <categories>Catégories</categories>
        <mimeType>#{Ecm.dialog.label.mimeType}</mimeType>
        <width>Largeur</width>
        <height>Hauteur</height>
        <content>#{word.content}</content>
        <re-upload>Un fichier existe déjà. Vous pouvez le supprimer et envoyer un autre fichier à partir d'ici.</re-upload>
      </label>
    </dialog>
  </File>

  <UIActionForm>
    <label>
      <description>#{word.description}</description>
      <port>Port</port>
      <userName>#{word.userName}</userName>
      <password>Mot de Passe</password>
      <link>Lien</link>
      <to>Envoyer un Email à</to>
      <actionName>#{word.name}</actionName>
    </label>
  </UIActionForm>
  <EditFormAction>
    <label>
      <description>#{word.description}</description>
      <port>Port</port>
      <userName>#{word.userName}</userName>
      <password>Mot de Passe</password>
      <link>Lien</link>
      <to>Envoyer un Email à</to>
      <actionName>#{word.name}</actionName>
    </label>
  </EditFormAction>
 
  <!--
  ###################### resoucebundle for exo:addMetadataAction dialogs template ####
  -->
  <AddMetadata>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <metadata>Metadonnée</metadata>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AddMetadata>

  <!--
  ###################### resoucebundle for exo:autoVersioning dialogs template ####
  -->
  <AutoVersioning>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Commande Cron</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AutoVersioning>
  <!--
  ###################### resoucebundle for exo:enableVersiong dialogs template ####
  -->
  <EnableVersioning>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Commande Cron</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </EnableVersioning>
  <!--
  ###################### resoucebundle for dc:elementSet dialogs template ####
  -->
  <ElementSet>
    <dialog>
      <label>
        <title>Titre</title>
        <creator>Créateur</creator>
        <subject>#{word.subject}</subject>
        <source>Source</source>
        <contributor>Contributeur</contributor>
        <resource-type>Type de Ressource</resource-type>
        <date>#{word.date}</date>
        <publisher>Editeur</publisher>
        <description>#{word.description}</description>
        <rights>Droits</rights>
        <relation>Relation</relation>
        <format>#{word.format}</format>
        <coverage>Couverture</coverage>
        <identifier>Identifiant</identifier>
        <language>Langue</language>
      </label>
    </dialog>
  </ElementSet>

  <!--
  ###################### resoucebundle for exo:actionable dialogs template ####
  -->
  <Actionable>
    <dialog>
      <label>
        <primary-type>Type Primaire</primary-type>
        <mixin-types>Types Mixin</mixin-types>
      </label>
    </dialog>
  </Actionable>

  <!--
  ###################### resoucebundle for exo:move dialogs template ####
  -->
  <Move>
    <dialog>
      <label>
        <destination-workpsace>Espace de stockage cible</destination-workpsace>
        <destination-path>Emplacement</destination-path>
        <primary-type>Type Primaire</primary-type>
        <mixin-types>Types Mixin</mixin-types>
      </label>
    </dialog>
  </Move>
  <!--
  ###################### resoucebundle for exo:published dialogs template ####
  -->
  <Published>
    <dialog>
      <label>
        <start-publication>Début de publication</start-publication>
        <end-publication>Fin de publication</end-publication>
      </label>
    </dialog>
  </Published>
 
  <!--
  ###################### resoucebundle mix:referenceable dialogs template ####
  -->
  <Referenceable>
    <dialog>
      <label>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Referenceable>
  <!--
  ###################### resoucebundle exo:relationable dialogs template ####
  -->
  <Relationable>
    <dialog>
      <label>
        <relation>Relation</relation>
      </label>
    </dialog>
  </Relationable>
  <!--
  ###################### resoucebundle exo:rss-enable dialogs template ####
  -->
  <RssEnable>
    <dialog>
      <label>
        <title>Titre</title>
        <summary>#{word.summary}</summary>
      </label>
    </dialog>
  </RssEnable>

  <!--
  ###################### resoucebundle mix:versionable dialogs template ####
  -->
  <Versionable>
    <dialog>
      <label>
        <version-history>L'historique des versions</version-history>
        <base-version>Version de base</base-version>
        <is-checked-out>Est extrait</is-checked-out>
        <predecessors>Prédécesseurs</predecessors>
        <merge-failed>Echec de la fusion</merge-failed>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Versionable>

  <!--
  ##################### resoucebundle for resouce container dialogs template#####
  -->
  <ResouceContainer>
    <dialog>
      <label>
        <name>Nom</name>
        <autoload>Chargement automatique</autoload>
        <minetype>Type MIME</minetype>
        <content>Contenu</content>
      </label>
    </dialog>
  </ResouceContainer>
  <!--
  ###################### resoucebundle for validator ####
  -->
  <EmailAddressValidator>
    <msg>
      <Invalid-input>L'adresse Email est invalide.</Invalid-input>
    </msg>
  </EmailAddressValidator>
  <EmptyFieldValidator>
    <msg>
      <empty-input>Le champ '{0}' est requis. Veuillez saisir une valeur.</empty-input>
    </msg>
  </EmptyFieldValidator>
  <NumberFormatValidator>
    <msg>
      <Invalid-number>Le champs '{0}' est invalid.</Invalid-number>
    </msg>
  </NumberFormatValidator>
  <!--
  #####Since version 2.4#######

  ###################### resoucebundle exo:taxonomyAction dialogs template ####
  -->
  <ScriptAction>
    <dialog>
      <label>
        <id>Id</id>
        <name>Nom</name>
        <lifecycle>Cycle de Vie</lifecycle>
        <homePath>Chemin Racine</homePath>
        <targetWorkspace>Espace de stockage cible</targetWorkspace>
        <targetPath>Chemin Cible</targetPath>
      </label>
    </dialog>
  </ScriptAction>
  <UIDialogForm>
   <form>
   <DialogFormException>Can not save this document. Maybe there is a problem with pre save interceptor.</DialogFormException>
   </form>
  </UIDialogForm>

  <!--
  #####Since version 2.6#######

  ###################### resoucebundle exo:taxonomyAction dialogs template ####
  -->
  <no>Non</no>
	<yes>Oui</yes>
	<true>Vrai</true>
	<false>Faux</false>
	<Electronic>Électronique</Electronic>
	<Paper>Papier</Paper>
	<NONE>Aucun</NONE>
	<CLASSIFIED>Classé</CLASSIFIED>
	<UNCLASSIFIED>Non classé</UNCLASSIFIED>
	<NOCONTRACT>Aucun contrat</NOCONTRACT>
	<one-minute>Une fois par Minute</one-minute>
	<hourly>Une fois par heure</hourly>
<daily>Quotidien</daily>
	<quarterly>Trimestriel</quarterly>
	<yearly>Annuel</yearly>
	<ten-years>Tous les 10 ans</ten-years>
	<read>Action Utilisateur</read>
	    <schedule>Planificateur</schedule>
  <node_added>Ajout de Contenu</node_added>
  <node_removed>Suppression de Contenu</node_removed>
  <property_added>Ajout de Propriété</property_added>
  <property_removed>Suppression de Propriété</property_removed>
  <property_changed>Modification de Propriété</property_changed>
  
  <ScriptAction>
   <dialog>
     <label>
							<isDeep>Récursif</isDeep>
        <nodetypes>Types de noeud</nodetypes>
        <affectedNodeTypeNames>Types de noeud affectés</affectedNodeTypeNames>
     </label>
   </dialog>
  </ScriptAction>

  <AddMetadata>
   <dialog>
     <label>
     		<isDeep>Récursif</isDeep>
        <nodetypes>Types de noeud</nodetypes>
        <affectedNodeTypeNames>Types de noeud affectés</affectedNodeTypeNames>
     </label>
   </dialog>
  </AddMetadata>

  <EnableVersioning>
   <dialog>
     <label>
     		<isDeep>Récursif</isDeep>
        <nodetypes>Types de Noeud</nodetypes>
        <affectedNodeTypeNames>Types de Noeud Affectés</affectedNodeTypeNames>
     </label>
   </dialog>
  </EnableVersioning>
  
  <AutoVersioning>
   <dialog>
     <label>
     		<isDeep>Récursif</isDeep>
        <nodetypes>Types de Noeud</nodetypes>
        <affectedNodeTypeNames>Types de Noeud Affectés</affectedNodeTypeNames>
     </label>
   </dialog>
  </AutoVersioning>
		
  <AccessibleMedia>
  	<dialog>
  		<label>
  			<captions>Sous-titres</captions>
				<audioDescription>Description audio</audioDescription>
				<alternativeText>Texte alternatif</alternativeText>
			</label>
		</dialog>
		<help>
			<CaptionsHelp>Les sous-titres communiquent le contenu des dialogues mais aussi toutes informations audio importantes.</CaptionsHelp>
			<AudioDescriptionHelp>Narration ajoutée à une piste sonore pour décrire les détails visuels importants.</AudioDescriptionHelp>
			<AlternativeTextHelp>Fournir une alternative accessible pour présenter l'information</AlternativeTextHelp>
		</help>
	</AccessibleMedia>
  
</bundle>
