<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Nome</name>
      <action>Aggiungi</action>
    </header>
    <info>
      <child-not-found>Figlio non trovato!</child-not-found>
    </info>
    <action>
      <title>
        <select>Selezionare</select>
      </title>
    </action>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Nome categoria</categoryName>
      <categoryPath>Path Categoria</categoryPath>
      <action>#{word.action}</action>
    </header>
    <action>
      <SaveCategories>#{word.save}</SaveCategories>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <cannot-save>Categorie non sono state salvate.</cannot-save>
      <have-saved>Categorie salvate con successo.</have-saved>
      <non-categories>Categorie non sono corrette. Prego seleziona categorie buone.</non-categories>
      <confirm-delete>Vuoi davvero cancellare questa categoria?</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Seleziona Utente</UIPermissionUser>
      <UIPermissionMembership>Seleziona Membership</UIPermissionMembership>
      <TaxoSelectUserOrGroup>Seleziona Membership</TaxoSelectUserOrGroup>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Seleziona Utente</selectUser>
      <selectGroup>Sfoglia e scegli un gruppo</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Seleziona Membership</selectUser>
      <selectGroup>Sfoglia e scegli un gruppo</selectGroup>
    </label>
  </UIPermissionMembership>
  <TaxoSelectUserOrGroup>
    <label>
      <selectGroup>Seleziona il Gruppo</selectGroup>
      <selectMembership>Seleziona Membership</selectMembership>
    </label>
  </TaxoSelectUserOrGroup>
  <!--
  ######################################################################
  # org.exoplatform.wcm.webui.clv.UICLVConfig        #
  ######################################################################
  -->
  <UICLVConfigTargetPageSelectorPopupWindow>
    <title>
      <UIPageSelector>Selettore Pagine</UIPageSelector>
    </title>
  </UICLVConfigTargetPageSelectorPopupWindow>
  <!--
  ###################################################################################
  # Repository                                        #
  ###################################################################################
  -->
  <rootNodeInfo>
    <title>
      <AddRootNode>Aggiungi Nodo Root</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  ################################################################################
  #             org.exoplatform.ecm.webui.component.admin.UIMetadataList         #
  ################################################################################
  -->
  <UIMetadataAnyPermission>
    <label>
      <anyPermission>Tutti i permessi</anyPermission>
    </label>
  </UIMetadataAnyPermission>
</bundle>
