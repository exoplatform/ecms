<?xml version="1.0" encoding="UTF-8"?>
<bundle>
 
  <View>
    <label>
      <languages>
        <en>Englisch</en>
        <fr>Französisch</fr>
        <ru>Russisch</ru>
        <vi>Vietnamesisch</vi>
        <nl>Holländisch</nl>
        <de>Deutsch</de>
        <uk>Ukrainisch</uk>
        <ar>Arabisch</ar>
