        <webDAV>WebDAV Ansicht</webDAV>
        <download>Herunterladen</download>
        <htmlview>Als HTML anzeigen</htmlview>
        <plaintextview>Als nur-Text anzeigen</plaintextview>
      </label>
    </view>
  </File>
 
  <!--
  ################resoucebundle FOR MAIL:DOCUMENT VIEW TEMPLATE##############
  -->
  <Mail>
    <view>
      <label>
        <from>Von:</from>
        <to>An:</to>
        <cc>Cc:</cc>
        <date>Datum:</date>
        <subject>Betreff:</subject>
        <attachment>Anhänge:</attachment>
      </label>
    </view>
  </Mail>
  <!--
  ################resoucebundle FOR EXO:VOTE VIEW TEMPLATE##############
  -->
  <Vote>
    <view>
      <label>
        <avg-rating>Durschnittliche Bewertung</avg-rating>
        <subscription>Stimmen</subscription>
      </label>
    </view>
  </Vote>
  <!--
  ################resoucebundle FOR LISTVIEW VIEW TEMPLATE##############
  -->
  <ListView>
    <view>
      <label>
        <name>Name</name>
        <versionable>Versionierbar</versionable>
        <exo_owner>Eigentümer</exo_owner>
        <exo_dateCreated>Erstellungsdatum</exo_dateCreated>
        <exo_dateModified>Bearbeitungsdatum</exo_dateModified>
        <auditing>Audit</auditing>
      </label>
    </view>
  </ListView>

  <!--
  ###################### resoucebundle for exo:addMetadataAction view template ####
  -->
  <AddMetadata>
    <view>
      <label>
        <action-name>Aktionname</action-name>
        <lifecycle>Lebenszyklus</lifecycle>
        <metadata>Metadaten</metadata>
        <description>Beschreibung</description>
      </label>
    </view>
  </AddMetadata>

  <!--
  ###################### resoucebundle for exo:autoVersioning view template ####
  -->
  <AutoVersioning>
    <view>
      <label>
        <action-name>Aktionname</action-name>
        <lifecycle>Lebenszyklus</lifecycle>
        <description>Beschreibung</description>
      </label>
    </view>
  </AutoVersioning>
  <!--
  ###################### resoucebundle for exo:enableVersiong view template ####
  -->
  <EnableVersioning>
    <view>
      <label>
        <action-name>Name</action-name>
        <lifecycle>Lebenszyklus</lifecycle>
        <description>Beschreibung</description>
      </label>
    </view>
  </EnableVersioning>
