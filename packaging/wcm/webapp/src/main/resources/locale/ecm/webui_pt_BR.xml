<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #####################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UICategoriesSelectPanel #
  #####################################################################
  -->
  <UITreeList>
    <header>
      <name>Nome</name>
      <action>Adicionar</action>
    </header>
    <action/>
    <info>
      <child-not-found>Sub-nó não encontrado!</child-not-found>
    </info>
  </UITreeList>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.tree.selectmany.UISelectedCategoriesGrid #
  ######################################################################
  -->
  <UISelectedCategoriesGrid>
    <header>
      <categoryName>Nome da Categoria</categoryName>
      <categoryPath>Caminho da Categoria</categoryPath>
      <action>Ação</action>
    </header>
    <action>
      <SaveCategories>Salvar</SaveCategories>
      <Cancel>Cancelar</Cancel>
    </action>
    <msg>
      <cannot-save>As categorias não foram salvas.</cannot-save>
      <have-saved>As categorias foram salvas com sucesso</have-saved>
      <non-categories>Por favor, selecione categorias válidas.</non-categories>
      <confirm-delete>Você quer deletar esta categoria</confirm-delete>
    </msg>
  </UISelectedCategoriesGrid>
  <!--
  ######################################################################
  # org.exoplatform.ecm.webui.selector.UIPermissionSelector           #
  ######################################################################
  -->
  <SelectUserOrGroup>
    <title>
      <UIPermissionUser>Selecionar Usuário</UIPermissionUser>
      <UIPermissionMembership>Selecionar associação</UIPermissionMembership>
    </title>
  </SelectUserOrGroup>
  <UIPermissionUser>
    <label>
      <selectUser>Selecionar Usuário</selectUser>
      <selectGroup>Selecionar grupo</selectGroup>
    </label>
  </UIPermissionUser>
  <UIPermissionMembership>
    <label>
      <selectUser>Selecionar associação</selectUser>
      <selectGroup>Selecionar grupo</selectGroup>
    </label>
  </UIPermissionMembership>
</bundle>
