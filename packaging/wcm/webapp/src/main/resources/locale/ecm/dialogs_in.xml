<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################### global word for ECM dialogs resoucebundle ####
  -->
  <Ecm>
    <dialog>
      <label>
        <end-time>Waktu berakhir</end-time>
        <start-time>Waktu mulai</start-time>
        <lifecycle>Lingkaran kehidupan</lifecycle>
        <mimeType>Jenis Mime</mimeType>
        <repeat-count>Menghitung ulang</repeat-count>
        <time-interval>Jarak waktu</time-interval>
        <query-items>Item Kueri</query-items>
        <schedule-type>Jenis Jadwal</schedule-type>
        <remove>Hapus Item</remove>
        <add>Tambah Item</add>
        <expand>Perluas ini</expand>
      </label>
    </dialog>
  </Ecm>
  
  <UIDocumentForm>
    <label>
      <title>#{word.title}</title>
      <link>Tautan</link>
      <contentHtml>#{word.content}</contentHtml>
      <content>#{word.content}</content>
      <contentPlain>#{word.content}</contentPlain>
      <name>Nama</name>
    <categories>Kategori</categories>
    </label>
  </UIDocumentForm>
 
  <UICommentForm>
    <label>
      <placeholder>Gunakan @ untuk menyebutkan seseorang dalam komentar anda</placeholder>
    </label>
  </UICommentForm>
 
  <!--
  ###################### resoucebundle for HTMLFile dialogs template ####
  -->
  <HtmlFile>
    <dialog>
      <label>
        <name>Nama</name>
        <content>Isi</content>
      </label>
    </dialog>
  </HtmlFile>

  <!--
  ###################### resoucebundle for nt:file dialogs template ####
  -->
  <File>
    <dialog>
      <label>
        <name>#{word.name}</name>
        <categories>Kategori</categories>
        <mimeType>#{Ecm.dialog.label.mimeType}</mimeType>
        <width>Lebar</width>
        <height>Tinggi</height>
        <content>#{word.content}</content>
        <re-upload>Berkasnya sudah ada. anda dapat menghapus dan mengupload berkas lain.</re-upload>
      </label>
    </dialog>
  </File>

  <UIActionForm>
    <label>
      <description>#{word.description}</description>
      <port>Pelabuhan</port>
      <userName>#{word.userName}</userName>
      <password>Kata Sandi</password>
      <link>Tautan</link>
      <to>Kirim Surat ke</to>
      <actionName>#{word.name}</actionName>
    </label>
  </UIActionForm>
  <EditFormAction>
    <label>
      <description>#{word.description}</description>
      <port>Pelabuhan</port>
      <userName>#{word.userName}</userName>
      <password>Kata Sandi</password>
      <link>Tautan</link>
      <to>Kirim Surat ke</to>
      <actionName>#{word.name}</actionName>
    </label>
  </EditFormAction>

  <!--
  ###################### resoucebundle for exo:addMetadataAction dialogs template ####
  -->
  <AddMetadata>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <metadata>Metadata</metadata>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AddMetadata>

  <!--
  ###################### resoucebundle for exo:autoVersioning dialogs template ####
  -->
  <AutoVersioning>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Cron ekspresi</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </AutoVersioning>
  <!--
  ###################### resoucebundle for exo:enableVersiong dialogs template ####
  -->
  <EnableVersioning>
    <dialog>
      <label>
        <id>Id</id>
        <name>#{word.name}</name>
        <lifecycle>#{Ecm.dialog.label.lifecycle}</lifecycle>
        <schedule-type>#{Ecm.dialog.label.schedule-type}</schedule-type>
        <start-time>#{Ecm.dialog.label.start-time}</start-time>
        <end-time>#{Ecm.dialog.label.end-time}</end-time>
        <repeat-count>#{Ecm.dialog.label.repeat-count}</repeat-count>
        <time-inteval>#{Ecm.dialog.label.time-interval}</time-inteval>
        <cron-expression>Cron Expression</cron-expression>
        <description>#{word.description}</description>
      </label>
    </dialog>
  </EnableVersioning>
  <!--
  ###################### resoucebundle for dc:elementSet dialogs template ####
  -->
  <ElementSet>
    <dialog>
      <label>
        <title>#{word.title}:</title>
        <creator>Pencipta:</creator>
        <subject>#{word.subject}:</subject>
        <source>Sumber:</source>
        <contributor>Penyumbang:</contributor>
        <resource-type>Jenis Sumber Daya:</resource-type>
        <date>#{word.date}:</date>
        <publisher>Penerbit:</publisher>
        <description>#{word.description}:</description>
        <rights>Hak:</rights>
        <relation>Hubungan:</relation>
        <format>#{word.format}:</format>
        <coverage>Cakupan:</coverage>
        <identifier>Pengenal:</identifier>
        <language>Language:</language>
      </label>
    </dialog>
  </ElementSet>

  <!--
  ###################### resoucebundle for exo:actionable dialogs template ####
  -->
  <Actionable>
    <dialog>
      <label>
        <primary-type>Jenis utama</primary-type>
        <mixin-types>Jenis Campuran</mixin-types>
      </label>
    </dialog>
  </Actionable>

  <!--
  ###################### resoucebundle for exo:move dialogs template ####
  -->
  <Move>
    <dialog>
      <label>
        <destination-workpsace>Tujuan kerja</destination-workpsace>
        <destination-path>Jalur Tujuan</destination-path>
        <primary-type>Tipe utama</primary-type>
        <mixin-types>Jenis Campuran</mixin-types>
      </label>
    </dialog>
  </Move>
  <!--
  ###################### resoucebundle for exo:published dialogs template ####
  -->
  <Published>
    <dialog>
      <label>
        <start-publication>Mulai Publikasi</start-publication>
        <end-publication>Publikasi Akhir</end-publication>
      </label>
    </dialog>
  </Published>
 
  <!--
  ###################### resoucebundle mix:referenceable dialogs template ####
  -->
  <Referenceable>
    <dialog>
      <label>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Referenceable>
  <!--
  ###################### resoucebundle exo:relationable dialogs template ####
  -->
  <Relationable>
    <dialog>
      <label>
        <relation>Hubungan</relation>
      </label>
    </dialog>
  </Relationable>
  <!--
  ###################### resoucebundle exo:rss-enable dialogs template ####
  -->
  <RssEnable>
    <dialog>
      <label>
        <title>Judul</title>
        <summary>#{word.summary}</summary>
      </label>
    </dialog>
  </RssEnable>

  <!--
  ###################### resoucebundle mix:versionable dialogs template ####
  -->
  <Versionable>
    <dialog>
      <label>
        <version-history>Sejarah Versi</version-history>
        <base-version>Versi dasar</base-version>
        <is-checked-out>Apakah periksa</is-checked-out>
        <predecessors>Pendahulu</predecessors>
        <merge-failed>Gabung Gagal</merge-failed>
        <uuid>UUID</uuid>
      </label>
    </dialog>
  </Versionable>

  <!--
  ##################### resoucebundle for resouce container dialogs template#####
  -->
  <ResouceContainer>
    <dialog>
      <label>
        <name>Nama</name>
        <autoload>Beban otomatis</autoload>
        <minetype>Jenis MIME</minetype>
        <content>Konten</content>
      </label>
    </dialog>
  </ResouceContainer>
  <!--
  ###################### resoucebundle for validator ####
  -->
  <EmailAddressValidator>
    <msg>
      <Invalid-input>Alamat emailnya tidak valid.</Invalid-input>
    </msg>
  </EmailAddressValidator>
  <EmptyFieldValidator>
    <msg>
      <empty-input>Bidangnya '{0}' ialah diperlukan. Silahkan masukkan nilai anda.</empty-input>
    </msg>
  </EmptyFieldValidator>
  <NumberFormatValidator>
    <msg>
      <Invalid-number>Bidangnya '{0}' tidak valid.</Invalid-number>
    </msg>
  </NumberFormatValidator>
  <!--
  #####Since version 2.4#######

  ###################### resoucebundle exo:taxonomyAction dialogs template ####
  -->
  <ScriptAction>
    <dialog>
      <label>
        <id>Id</id>
        <name>Nama</name>
        <lifecycle>Lingkaran kehidupan</lifecycle>
        <homePath>Jalur rumah</homePath>
        <targetWorkspace>Sasaran ruang kerja</targetWorkspace>
        <targetPath>Jalur Sasaran</targetPath>
      </label>
    </dialog>
  </ScriptAction>
  <UIDialogForm>
   <form>
   <DialogFormException>Tidak dapat menyimpan dokumen ini. Mungkin ada masalah dengan sebuah pencegat pra-simpan.</DialogFormException>
   </form>
  </UIDialogForm>

