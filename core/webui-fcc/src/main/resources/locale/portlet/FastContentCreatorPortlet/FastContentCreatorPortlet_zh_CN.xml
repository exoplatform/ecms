  <UIFCCConfig>
    <label>
      <UIFCCSaveLocationField>保存位置</UIFCCSaveLocationField>
      <UIFCCWorkspaceFormSelectBox>选择工作区</UIFCCWorkspaceFormSelectBox>
      <UIFCCTemplateField>模板选项</UIFCCTemplateField>
      <UIFCCLocationFormInputAction>要保存的位置</UIFCCLocationFormInputAction>
      <UIFCCTemplateFormSelectBox>选择模板</UIFCCTemplateFormSelectBox>
