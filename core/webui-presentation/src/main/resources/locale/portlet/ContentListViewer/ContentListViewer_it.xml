      <category-back>Vai al precedente</category-back>
    </msg>
  </UICLVContainer>
  <UIViewerManagementPopupWindow>
    <title>
      <UICLVConfig>Preferenze Lista Contenuti</UICLVConfig>
    </title>
  </UIViewerManagementPopupWindow>
  <UICLVConfig>
