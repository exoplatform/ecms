  <UICLVConfigTargetPageFormInputSet>
    <title>
      <SelectBasePath>Välj rotkatalog</SelectBasePath>
      <SelectTargetPage>Välj rotkatalog</SelectTargetPage>
    </title>
  </UICLVConfigTargetPageFormInputSet>
  <UICLVBasePathPopupWindow>
    <title>
      <UIPageSelector>Sidväljare</UIPageSelector>
    </title>
  </UICLVBasePathPopupWindow>
  <UICLVPresentation>
    <msg>
      <confirm-delete>Are you sure you want to delete this content?</confirm-delete>
      <delete-content-successfully>Content deleted successfully.</delete-content-successfully>
    </msg>
    <label>
      <readmore>Läs mer</readmore>
      <rssFeed>RSS-flöde</rssFeed>
    </label>
    <action>
      <refresh>Uppdatera</refresh>
      <edit>Redigera innehåll i utforskaren</edit>
      <delete>Ta bort</delete>
      <publish>Publicera</publish>
    </action>
    <label>
      <draft>Utkast</draft>
    </label>
  </UICLVPresentation>
  <CorrectContentSelectorPopupWindow>
    <title>
      <UIContentSelectorMulti>Multiple Content Selector Panel</UIContentSelectorMulti>
    </title>
  </CorrectContentSelectorPopupWindow>
  <UIContentDialogFormPopupWindow>
    <title>
      <UIContentDialogForm>Formulär för webb Innehåll</UIContentDialogForm>
    </title>
  </UIContentDialogFormPopupWindow>
  <UICLVConfigContextualFolderRadioBoxInput>
    <label>
      <contextualEnable>Aktiverad</contextualEnable>
      <contextualDisable>Inaktiverad</contextualDisable>
    </label>
  </UICLVConfigContextualFolderRadioBoxInput>
  <PublicationStates>
    <archived>Arkiverad</archived>
