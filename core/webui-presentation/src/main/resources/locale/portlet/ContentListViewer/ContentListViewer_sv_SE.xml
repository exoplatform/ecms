  <UICLVConfigCacheEnableRadioBoxInput>
    <label>
      <ENABLE>Begränsad för autentisering</ENABLE>
      <DISABLE>Begränsad för användarroll</DISABLE>
    </label>
  </UICLVConfigCacheEnableRadioBoxInput>
