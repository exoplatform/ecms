<bundle>
  <UITabForm>
    <label>
      <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
      <showConnectCloudDrive>Bulut Belgelerini Bağlayın</showConnectCloudDrive>
    </label>
  </UITabForm>
  <UIPopupWindow>
    <title>
      <ConnectCloudDriveForm>Bulut Belgelerini Bağlayın</ConnectCloudDriveForm>
      <UISharingManager>Bulut dosya paylaşımı</UISharingManager>
    </title>
  </UIPopupWindow>
  <UIActionBar>
    <tooltip>
      <ShowConnectCloudDrive>Bulut Belgelerini Bağlayın</ShowConnectCloudDrive>
    </tooltip>
  </UIActionBar>
  <ECMContextMenu>
    <event>
      <OpenCloudFile>Şurada aç</OpenCloudFile>
      <SyncingFile>Dosya eşitleniyor...</SyncingFile>
      <PushCloudFile>Şuraya gönder</PushCloudFile>
      <RefreshCloudDrive>Şununla eşitle</RefreshCloudDrive>
      <RefreshView>Görünümü yenile</RefreshView>
    </event>
  </ECMContextMenu>
  <CloudFileViewer>
    <event>
      <OpenFileOn>$serviceName üzerinde $fileUrl dosyasını açın</OpenFileOn>
      <OpenOn>Şurada aç</OpenOn>
      <EditFile>Düzenle</EditFile>
      <ViewFile>Görünüm</ViewFile>
    </event>
    <label>
      <sourceview>Kaynak kodunu görüntüle</sourceview>
      <webview>Web sayfası olarak görüntüle</webview>
    </label>
  </CloudFileViewer>
  <CloudFile>
    <msg>
      <MoveToAnotherDriveNotSupported>Dosya başka bir bulut sürücüsüne taşınamıyor</MoveToAnotherDriveNotSupported>
      <MoveToOutsideDriveNotSupported>Dosya kendi bulut sürücüsünün dışına taşınamıyor</MoveToOutsideDriveNotSupported>
      <MoveBetweenWorkspacesNotSupported>Bulut dosyası farklı çalışma alanına taşınamıyor</MoveBetweenWorkspacesNotSupported>
      <CloudDriveCopyMoveNotSupported>Bulut sürücüsü klasörü kopyalanamıyor veya taşınamıyor</CloudDriveCopyMoveNotSupported>
      <DestinationNotWrittable>Hedef klasöre yazılamıyor</DestinationNotWrittable>
      <DestinationNotDefined>Hedef klasör tanımlanmadı</DestinationNotDefined>
      <SourceNotDefined>Kaynak dosya tanımlanmadı</SourceNotDefined>
      <ErrorMoveToDestination>Paylaşılan bağlantı hedefe taşınırken hata oluştu</ErrorMoveToDestination>
      <DestinationItemNameExists>Bu öğe adı hedef konumda zaten var</DestinationItemNameExists>
      <LinkCreated>{0} bağlantısı oluşturuldu</LinkCreated>
      <FileLinksCreated>{0} dosyası {1} ile bağlantılı</FileLinksCreated>
      <FileLinksSharedInSpace>Dosya bağlantısı {0}, {1} / {2} alanda paylaşıldı</FileLinksSharedInSpace>
      <FileLinksSharedInGroup>Dosya bağlantısı {0}, {1} / {2} grupta paylaşıldı</FileLinksSharedInGroup>
      <FileLinksSharedWithUser>Dosya bağlantısı {0}, {2} kullanıcısıyla {1} klasöründe paylaşıldı</FileLinksSharedWithUser>
      <FileLinksNotSupportedToThisDestination>Bu hedefe dosya bağlama desteklenmiyor</FileLinksNotSupportedToThisDestination>
      <ErrorReadingDrives>Belge sürücülerini okuma hatası</ErrorReadingDrives>
      <CannotFindUser>{0} kullanıcısı bulunamıyor</CannotFindUser>
      <ErrorReadingFileLinks>Dosya bağlantılarını okuma hatası</ErrorReadingFileLinks>
      <ErrorReadingUserDrive>Kullanıcı sürücüsünü okuma hatası</ErrorReadingUserDrive>
    </msg>
  </CloudFile>
  <ConnectCloudDriveForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </ConnectCloudDriveForm>
  <cloudStorage>
    <label>
      <title>Cloud Storage</title>
      <cloudDrives>Cloud Drives</cloudDrives>
      <provider>Sağlaycı</provider>
      <description>Açıklama</description>
      <active>Etkin</active>
      <ConnectionToCloud>Connection to Cloud drives</ConnectionToCloud>
    </label>
  </cloudStorage>
  <cloudDriveSettings>
    <label>
      <title>Belgeler</title>
      <subtitle>Connect your personal drives</subtitle>
      <description>Access your personal drives from your Digital Wokrplace</description>
    </label>
    <drawer>
      <title>Connect your personal drives</title>
      <button>
        <connect>Bağlan</connect>
      </button>
      <noConnector>No personal drive connector available</noConnector>
    </drawer>
    <alert>
      <successMessage>Your cloud drive is connected correctly</successMessage>
      <errorMessage>Failed connection</errorMessage>
    </alert>
  </cloudDriveSettings>
</bundle>
