<bundle>
  <UITabForm>
    <label>
      <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
      <showConnectCloudDrive>Cloud-Dokumente verbinden</showConnectCloudDrive>
    </label>
  </UITabForm>
  <UIPopupWindow>
    <title>
      <ConnectCloudDriveForm>Cloud-Dokumente verbinden</ConnectCloudDriveForm>
      <UISharingManager>Cloud-Dateifreigabe</UISharingManager>
    </title>
  </UIPopupWindow>
  <UIActionBar>
    <tooltip>
      <ShowConnectCloudDrive>Cloud-Dokumente verbinden</ShowConnectCloudDrive>
    </tooltip>
  </UIActionBar>
  <ECMContextMenu>
    <event>
      <OpenCloudFile>Öffnen auf</OpenCloudFile>
      <SyncingFile>Datei synchronisieren …</SyncingFile>
      <PushCloudFile>Verschieben nach</PushCloudFile>
      <RefreshCloudDrive>Synchronisieren mit</RefreshCloudDrive>
      <RefreshView>Ansicht aktualisieren</RefreshView>
    </event>
  </ECMContextMenu>
  <CloudFileViewer>
    <event>
      <OpenFileOn>Öffnen Sie $fileUrl unter $serviceName</OpenFileOn>
      <OpenOn>Öffnen auf</OpenOn>
      <EditFile>Bearbeiten</EditFile>
      <ViewFile>Einstellung</ViewFile>
    </event>
    <label>
      <sourceview>Quellcode anzeigen</sourceview>
      <webview>Als Webseite anzeigen</webview>
    </label>
  </CloudFileViewer>
  <CloudFile>
    <msg>
      <MoveToAnotherDriveNotSupported>Datei kann nicht auf einen anderen Cloud Drive verschoben werden</MoveToAnotherDriveNotSupported>
      <MoveToOutsideDriveNotSupported>Datei kann nicht außerhalb des Cloud Drives verschoben werden</MoveToOutsideDriveNotSupported>
      <MoveBetweenWorkspacesNotSupported>Die Cloud-Datei kann nicht in einen anderen Arbeitsbereich verschoben werden</MoveBetweenWorkspacesNotSupported>
      <CloudDriveCopyMoveNotSupported>Der Ordner des Cloud Drives kann nicht kopiert oder verschoben werden</CloudDriveCopyMoveNotSupported>
      <DestinationNotWrittable>Kann nicht im Zielordner schreiben</DestinationNotWrittable>
      <DestinationNotDefined>Zielordner nicht definiert</DestinationNotDefined>
      <SourceNotDefined>Quelldatei nicht definiert</SourceNotDefined>
      <ErrorMoveToDestination>Fehler beim Verschieben des freigegebenen Links zum Ziel</ErrorMoveToDestination>
      <DestinationItemNameExists>Ein solcher Artikel-Name existiert bereits am Zielort</DestinationItemNameExists>
      <LinkCreated>{0} Link erstellt</LinkCreated>
      <FileLinksCreated>Datei {0} in {1} verknüpft</FileLinksCreated>
      <FileLinksSharedInSpace>Dateilink {0} wird im Raum {1} von {2} geteilt</FileLinksSharedInSpace>
      <FileLinksSharedInGroup>Dateilink {0} in {1} der Gruppe {2} geteilt</FileLinksSharedInGroup>
      <FileLinksSharedWithUser>Dateilink {0} mit Benutzer {2} in seinem Ordner {1} geteilt</FileLinksSharedWithUser>
      <FileLinksNotSupportedToThisDestination>Dateiverknüpfung zu diesem Zielort wird nicht unterstützt</FileLinksNotSupportedToThisDestination>
      <ErrorReadingDrives>Fehler beim Lesen des Dokument Drives</ErrorReadingDrives>
      <CannotFindUser>Nutzer {0} kann nicht gefunden werden</CannotFindUser>
      <ErrorReadingFileLinks>Fehler beim Lesen der Datei-Links</ErrorReadingFileLinks>
      <ErrorReadingUserDrive>Fehler beim Lesen des Benutzerlaufwerks</ErrorReadingUserDrive>
    </msg>
  </CloudFile>
  <ConnectCloudDriveForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </ConnectCloudDriveForm>
  <cloudStorage>
    <label>
      <title>Cloud-Speicher</title>
      <cloudDrives>Cloud Drives</cloudDrives>
      <provider>Anbieter</provider>
      <description>Beschreibung</description>
      <active>Aktiv</active>
      <ConnectionToCloud>Connection to Cloud drives</ConnectionToCloud>
    </label>
  </cloudStorage>
  <cloudDriveSettings>
    <label>
      <title>Dokumente</title>
      <subtitle>Connect your personal drives</subtitle>
      <description>Access your personal drives from your Digital Workplace</description>
    </label>
    <drawer>
      <title>Connect your personal drives</title>
      <button>
        <connect>Verbinden</connect>
      </button>
      <button>
        <disconnect>Trennen</disconnect>
      </button>
      <noConnector>No personal drive connector available</noConnector>
      <connectedAccountWith>Sie sind verbunden mit</connectedAccountWith>
    </drawer>
    <alert>
      <successMessage>Your cloud drive is connected correctly</successMessage>
      <errorMessage>Failed connection</errorMessage>
      <errorSaveUserSettings>Cannot save connected drive account</errorSaveUserSettings>
      <disconnectSuccessMessage>Your cloud drive is disconnected correctly</disconnectSuccessMessage>
      <disconnectErrorMessage>Failed to disconnect.</disconnectErrorMessage>
    </alert>
  </cloudDriveSettings>
</bundle>
