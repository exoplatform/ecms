<bundle>
  <UITabForm>
    <label>
      <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
      <showConnectCloudDrive>Connetti Documenti del Cloud</showConnectCloudDrive>
    </label>
  </UITabForm>
  <UIPopupWindow>
    <title>
      <ConnectCloudDriveForm>Connetti Documenti del Cloud</ConnectCloudDriveForm>
      <ConnectYour>Connetti il tuo</ConnectYour>
      <UISharingManager>Condivisione file del cloud</UISharingManager>
    </title>
  </UIPopupWindow>
  <UIActionBar>
    <tooltip>
      <ShowConnectCloudDrive>Connetti Documenti del Cloud</ShowConnectCloudDrive>
    </tooltip>
  </UIActionBar>
  <ECMContextMenu>
    <event>
      <OpenCloudFile>Apri su</OpenCloudFile>
      <SyncingFile>Sincronizzazione file...</SyncingFile>
      <PushCloudFile>Invia a</PushCloudFile>
      <RefreshCloudDrive>Sincronizza con</RefreshCloudDrive>
      <RefreshView>Aggiorna visualizzazione</RefreshView>
    </event>
  </ECMContextMenu>
  <CloudFileViewer>
    <event>
      <OpenFileOn>Apri $fileUrl su $serviceName</OpenFileOn>
      <OpenOn>Apri su</OpenOn>
      <EditFile>Modificare</EditFile>
      <ViewFile>Vista</ViewFile>
    </event>
    <label>
      <sourceview>Visualizza codice sorgente</sourceview>
      <webview>Visualizza come pagina web</webview>
    </label>
  </CloudFileViewer>
  <CloudFile>
    <msg>
      <MoveToAnotherDriveNotSupported>Impossibile spostare il file su un'altra unità cloud</MoveToAnotherDriveNotSupported>
      <MoveToOutsideDriveNotSupported>Impossibile muovere il file al di fuori della sua unità cloud</MoveToOutsideDriveNotSupported>
      <MoveBetweenWorkspacesNotSupported>Impossibile muovere il file del cloud ad uno spazio di lavoro differente</MoveBetweenWorkspacesNotSupported>
      <CloudDriveCopyMoveNotSupported>Impossibile copiare o spostare la cartella dell'unità del cloud</CloudDriveCopyMoveNotSupported>
      <DestinationNotWrittable>Impossibile scrivere nella cartella di destinazione</DestinationNotWrittable>
      <DestinationNotDefined>La cartella di destinazione non è definita</DestinationNotDefined>
      <SourceNotDefined>Il file sorgente non è definito</SourceNotDefined>
      <ErrorMoveToDestination>Errore durante lo spostamento del link condiviso alla destinazione</ErrorMoveToDestination>
      <DestinationItemNameExists>Tale nome dell'elemento esiste già alla destinazione</DestinationItemNameExists>
      <LinkCreated>Link {0} creato</LinkCreated>
      <FileLinksCreated>File {0} collegato in {1}</FileLinksCreated>
      <FileLinksSharedInSpace>Link del file {0} condiviso in {1} dello spazio {2}</FileLinksSharedInSpace>
      <FileLinksSharedInGroup>Link del file {0} condiviso in {1} del gruppo {2}</FileLinksSharedInGroup>
      <FileLinksSharedWithUser>Link del file {0} condiviso con l'utente {2} nella sua cartella {1}</FileLinksSharedWithUser>
      <FileLinksNotSupportedToThisDestination>Il collegamento del file non è supportato a questa destinazione</FileLinksNotSupportedToThisDestination>
      <ErrorReadingDrives>Errore nella lettura delle unità del documento</ErrorReadingDrives>
      <CannotFindUser>Impossibile trovare l'utente {0}</CannotFindUser>
      <ErrorReadingFileLinks>Errore durante la lettura dei link del file</ErrorReadingFileLinks>
      <ErrorReadingUserDrive>Errore durante la lettura dell'unità dell'utente</ErrorReadingUserDrive>
    </msg>
  </CloudFile>
  <ConnectCloudDriveForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </ConnectCloudDriveForm>
  <ConnectDriveDrawer>
    <title>
      <ConnectYourService>Connetti il tuo servizio</ConnectYourService>
    </title>
  </ConnectDriveDrawer>
  <cloudStorage>
    <label>
      <title>Cloud Storage</title>
      <cloudDrives>Cloud Drives</cloudDrives>
      <provider>Fornitore</provider>
      <description>Descrizione</description>
      <active>Attiva</active>
      <ConnectionToCloud>Connection to Cloud drives</ConnectionToCloud>
    </label>
  </cloudStorage>
</bundle>
