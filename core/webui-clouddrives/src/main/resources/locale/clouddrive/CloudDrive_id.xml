<bundle>
  <UITabForm>
    <label>
      <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
      <showConnectCloudDrive>Hubungkan Dokumen Cloud</showConnectCloudDrive>
    </label>
  </UITabForm>
  <UIPopupWindow>
    <title>
      <ConnectCloudDriveForm>Hubungkan Dokumen Cloud</ConnectCloudDriveForm>
      <UISharingManager>Berbagi Berkas di Cloud</UISharingManager>
    </title>
  </UIPopupWindow>
  <UIActionBar>
    <tooltip>
      <ShowConnectCloudDrive>Hubungkan Dokumen Cloud</ShowConnectCloudDrive>
    </tooltip>
  </UIActionBar>
  <ECMContextMenu>
    <event>
      <OpenCloudFile>Buka pada</OpenCloudFile>
      <SyncingFile>Menyinkronkan berkas</SyncingFile>
      <PushCloudFile>Kirim ke</PushCloudFile>
      <RefreshCloudDrive>Sinkronkan dengan</RefreshCloudDrive>
      <RefreshView>Perbarui tampilan</RefreshView>
    </event>
  </ECMContextMenu>
  <CloudFileViewer>
    <event>
      <OpenFileOn>Buka $fileUrl pada $serviceName</OpenFileOn>
      <OpenOn>Buka pada</OpenOn>
      <EditFile>Edit</EditFile>
      <ViewFile>Tampilan</ViewFile>
    </event>
    <label>
      <sourceview>Tampilkan kode sumber</sourceview>
      <webview>Tampilkan sebagai halaman web</webview>
    </label>
  </CloudFileViewer>
  <CloudFile>
    <msg>
      <MoveToAnotherDriveNotSupported>Tidak dapat memindahkan berkas ke cloud drive lain.</MoveToAnotherDriveNotSupported>
      <MoveToOutsideDriveNotSupported>Tidak dapat memindahkan berkas ke luar cloud drive-nya.</MoveToOutsideDriveNotSupported>
      <MoveBetweenWorkspacesNotSupported>Tidak dapat memindahkan berkas cloud ke ruang kerja yang berbeda.</MoveBetweenWorkspacesNotSupported>
      <CloudDriveCopyMoveNotSupported>Tidak dapat menyalin atau memindahkan folder cloud drive.</CloudDriveCopyMoveNotSupported>
      <DestinationNotWrittable>Tidak dapat menulis di folder tujuan.</DestinationNotWrittable>
      <DestinationNotDefined>Folder tujuan tidak didefinisikan.</DestinationNotDefined>
      <SourceNotDefined>Sumber file tidak didefinisikan.</SourceNotDefined>
      <ErrorMoveToDestination>Kesalahan saat memindahkan tautan bersama ke tujuan.</ErrorMoveToDestination>
      <DestinationItemNameExists>Nama item seperti itu sudah ada di destinasi</DestinationItemNameExists>
      <LinkCreated>Link {0} telah dibuat.</LinkCreated>
      <FileLinksCreated>File {0} dihubungkan di {1}</FileLinksCreated>
      <FileLinksSharedInSpace>Link file {0} dibagikan di {1} dari ruang {2}.</FileLinksSharedInSpace>
      <FileLinksSharedInGroup>File link {0} shared in {1} of {2} group</FileLinksSharedInGroup>
      <FileLinksSharedWithUser>File link {0} shared with user {2} in his {1} folder</FileLinksSharedWithUser>
      <FileLinksNotSupportedToThisDestination>File linking not supported to this destination</FileLinksNotSupportedToThisDestination>
      <ErrorReadingDrives>Error reading document drives</ErrorReadingDrives>
      <CannotFindUser>Cannot find user {0}</CannotFindUser>
      <ErrorReadingFileLinks>Error reading file links</ErrorReadingFileLinks>
      <ErrorReadingUserDrive>Error reading user drive</ErrorReadingUserDrive>
    </msg>
  </CloudFile>
  <ConnectCloudDriveForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </ConnectCloudDriveForm>
  <cloudStorage>
    <label>
      <title>Cloud Storage</title>
      <cloudDrives>Cloud Drives</cloudDrives>
      <provider>Provider</provider>
      <description>Description</description>
      <active>Active</active>
      <ConnectionToCloud>Connection to Cloud drives</ConnectionToCloud>
    </label>
  </cloudStorage>
  <cloudDriveSettings>
    <label>
      <title>Documents</title>
      <subtitle>Connect your personal drives</subtitle>
      <description>Access your personal drives from your Digital Workplace</description>
    </label>
    <drawer>
      <title>Connect your personal drives</title>
      <button>
        <connect>Connect</connect>
      </button>
      <button>
        <disconnect>Disconnect</disconnect>
      </button>
      <noConnector>No personal drive connector available</noConnector>
      <connectedAccountWith>You are connected with</connectedAccountWith>
    </drawer>
    <alert>
      <successMessage>Your cloud drive is connected correctly</successMessage>
      <errorMessage>Failed connection</errorMessage>
      <errorSaveUserSettings>Cannot save connected drive account</errorSaveUserSettings>
      <disconnectSuccessMessage>Your cloud drive is disconnected correctly</disconnectSuccessMessage>
      <disconnectErrorMessage>Failed to disconnect.</disconnectErrorMessage>
    </alert>
  </cloudDriveSettings>
</bundle>
