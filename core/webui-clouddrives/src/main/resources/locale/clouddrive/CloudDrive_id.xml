<bundle>
  <UITabForm>
    <label>
      <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
      <showConnectCloudDrive>Hubungkan Dokumen Cloud</showConnectCloudDrive>
    </label>
  </UITabForm>
  <UIPopupWindow>
    <title>
      <ConnectCloudDriveForm>Hubungkan Dokumen Cloud</ConnectCloudDriveForm>
      <UISharingManager>Berbagi Berkas di Cloud</UISharingManager>
    </title>
  </UIPopupWindow>
  <UIActionBar>
    <tooltip>
      <ShowConnectCloudDrive>Hubungkan Dokumen Cloud</ShowConnectCloudDrive>
    </tooltip>
  </UIActionBar>
  <ECMContextMenu>
    <event>
      <OpenCloudFile>Buka pada</OpenCloudFile>
      <SyncingFile>Menyinkronkan berkas</SyncingFile>
      <PushCloudFile>Kirim ke</PushCloudFile>
      <RefreshCloudDrive>Sinkronkan dengan</RefreshCloudDrive>
      <RefreshView>Perbarui tampilan</RefreshView>
    </event>
  </ECMContextMenu>
  <CloudFileViewer>
    <event>
      <OpenFileOn>Buka $fileUrl pada $serviceName</OpenFileOn>
      <OpenOn>Buka pada</OpenOn>
      <EditFile>Edit</EditFile>
      <ViewFile>Tampilan</ViewFile>
    </event>
    <label>
      <sourceview>Tampilkan kode sumber</sourceview>
      <webview>Tampilkan sebagai halaman web</webview>
    </label>
  </CloudFileViewer>
  <CloudFile>
    <msg>
      <MoveToAnotherDriveNotSupported>Tidak dapat memindahkan berkas ke cloud drive lain.</MoveToAnotherDriveNotSupported>
      <MoveToOutsideDriveNotSupported>Tidak dapat memindahkan berkas ke luar cloud drive-nya.</MoveToOutsideDriveNotSupported>
      <MoveBetweenWorkspacesNotSupported>Tidak dapat memindahkan berkas cloud ke ruang kerja yang berbeda.</MoveBetweenWorkspacesNotSupported>
      <CloudDriveCopyMoveNotSupported>Tidak dapat menyalin atau memindahkan folder cloud drive.</CloudDriveCopyMoveNotSupported>
      <DestinationNotWrittable>Tidak dapat menulis di folder tujuan.</DestinationNotWrittable>
      <DestinationNotDefined>Folder tujuan tidak didefinisikan.</DestinationNotDefined>
      <SourceNotDefined>Sumber file tidak didefinisikan.</SourceNotDefined>
      <ErrorMoveToDestination>Kesalahan saat memindahkan tautan bersama ke tujuan.</ErrorMoveToDestination>
      <DestinationItemNameExists>Nama item seperti itu sudah ada di destinasi</DestinationItemNameExists>
      <LinkCreated>Link {0} telah dibuat.</LinkCreated>
      <FileLinksCreated>File {0} dihubungkan di {1}</FileLinksCreated>
      <FileLinksSharedInSpace>Link file {0} dibagikan di {1} dari ruang {2}.</FileLinksSharedInSpace>
      <FileLinksSharedInGroup>File link {0} dibagikan di {1} dari grup {2}.</FileLinksSharedInGroup>
      <FileLinksSharedWithUser>File link {0} dibagikan dengan pengguna {2} di folder {1} miliknya.</FileLinksSharedWithUser>
      <FileLinksNotSupportedToThisDestination>Pengaitan berkas tidak didukung ke tujuan ini.</FileLinksNotSupportedToThisDestination>
      <ErrorReadingDrives>Kesalahan membaca drive dokumen.</ErrorReadingDrives>
      <CannotFindUser>Tidak dapat menemukan pengguna {0}.</CannotFindUser>
      <ErrorReadingFileLinks>Kesalahan membaca tautan berkas.</ErrorReadingFileLinks>
      <ErrorReadingUserDrive>Kesalahan membaca drive pengguna.</ErrorReadingUserDrive>
    </msg>
  </CloudFile>
  <ConnectCloudDriveForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </ConnectCloudDriveForm>
  <cloudStorage>
    <label>
      <title>Penyimpanan Cloud</title>
      <cloudDrives>Cloud Drive</cloudDrives>
      <provider>Penyedia</provider>
      <description>Deskripsi</description>
      <active>Aktif</active>
      <ConnectionToCloud>Koneksi ke Drive Cloud</ConnectionToCloud>
    </label>
  </cloudStorage>
  <cloudDriveSettings>
    <label>
      <title>Dokumen</title>
      <subtitle>Hubungkan drive pribadi Anda</subtitle>
      <description>Akses drive pribadi Anda dari Lingkungan Kerja Digital Anda</description>
    </label>
    <drawer>
      <title>Hubungkan drive pribadi Anda</title>
      <button>
        <connect>Hubungkan</connect>
      </button>
      <button>
        <disconnect>Putuskan Koneksi</disconnect>
      </button>
      <noConnector>Tidak ada konektor drive pribadi yang tersedia.</noConnector>
      <connectedAccountWith>Anda terhubung dengan</connectedAccountWith>
    </drawer>
    <alert>
      <successMessage>Drive cloud Anda telah terhubung dengan benar.</successMessage>
      <errorMessage>Koneksi gagal</errorMessage>
      <errorSaveUserSettings>Tidak dapat menyimpan akun drive yang terhubung.</errorSaveUserSettings>
      <disconnectSuccessMessage>Drive cloud Anda telah terputus dengan benar.</disconnectSuccessMessage>
      <disconnectErrorMessage>Gagal memutuskan koneksi.</disconnectErrorMessage>
    </alert>
  </cloudDriveSettings>
</bundle>
