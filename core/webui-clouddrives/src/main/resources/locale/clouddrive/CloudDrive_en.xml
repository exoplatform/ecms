<bundle>
  <UITabForm>
    <label>
      <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
      <showConnectCloudDrive>Connect Cloud Documents</showConnectCloudDrive>
    </label>
  </UITabForm>
  <UIPopupWindow>
    <title>
      <ConnectCloudDriveForm>Connect Cloud Documents</ConnectCloudDriveForm>
      <ConnectYour>Connect your</ConnectYour>
      <UISharingManager>Cloud file sharing</UISharingManager>
    </title>
  </UIPopupWindow>
  <UIActionBar>
    <tooltip>
      <ShowConnectCloudDrive>Connect Cloud Documents</ShowConnectCloudDrive>
    </tooltip>
  </UIActionBar>
  <ECMContextMenu>
    <event>
      <OpenCloudFile>Open on</OpenCloudFile>
      <SyncingFile>Synchronizing file...</SyncingFile>
      <PushCloudFile>Push to</PushCloudFile>
      <RefreshCloudDrive>Sync with </RefreshCloudDrive>
      <RefreshView>Refresh view</RefreshView>
    </event>
  </ECMContextMenu>
  <CloudFileViewer>
    <event>
      <OpenFileOn>Open $fileUrl on $serviceName</OpenFileOn>
      <OpenOn>Open on</OpenOn>
      <EditFile>Edit</EditFile>
      <ViewFile>View</ViewFile>
    </event>
    <label>
      <sourceview>View source code</sourceview>
      <webview>View as Web page</webview>
    </label>
  </CloudFileViewer>
  <CloudFile>
    <msg>
      <MoveToAnotherDriveNotSupported>Cannot move file to another cloud drive</MoveToAnotherDriveNotSupported>
      <MoveToOutsideDriveNotSupported>Cannot move file to outside its cloud drive</MoveToOutsideDriveNotSupported>
      <MoveBetweenWorkspacesNotSupported>Cannot move cloud file to different workspace</MoveBetweenWorkspacesNotSupported>
      <CloudDriveCopyMoveNotSupported>Cannot copy or move cloud drive folder</CloudDriveCopyMoveNotSupported>
      <DestinationNotWrittable>Cannot write in destination folder</DestinationNotWrittable>
      <DestinationNotDefined>Destination folder not defined</DestinationNotDefined>
      <SourceNotDefined>Source file not defined</SourceNotDefined>
      <ErrorMoveToDestination>Error moving shared link to destination</ErrorMoveToDestination>
      <DestinationItemNameExists>Such item name already exists at destination</DestinationItemNameExists>
      <LinkCreated>Link{0} created</LinkCreated>
      <FileLinksCreated>File{0} linked in {1}</FileLinksCreated>
      <FileLinksSharedInSpace>File link{0} shared in {1} of {2} space</FileLinksSharedInSpace>
      <FileLinksSharedInGroup>File link{0} shared in {1} of {2} group</FileLinksSharedInGroup>
      <FileLinksSharedWithUser>File link{0} shared with user {2} in his {1} folder</FileLinksSharedWithUser>
      <FileLinksNotSupportedToThisDestination>File linking not supported to this destination</FileLinksNotSupportedToThisDestination>
      <ErrorReadingDrives>Error reading document drives</ErrorReadingDrives>
      <CannotFindUser>Cannot find user {0}</CannotFindUser>
      <ErrorReadingFileLinks>Error reading file links</ErrorReadingFileLinks>
      <ErrorReadingUserDrive>Error reading user drive</ErrorReadingUserDrive>
    </msg>
  </CloudFile>
  <ConnectCloudDriveForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </ConnectCloudDriveForm>
  <ConnectDriveDrawer>
    <title>
      <ConnectYourService>Connect your service</ConnectYourService>
    </title>
  </ConnectDriveDrawer>
</bundle>