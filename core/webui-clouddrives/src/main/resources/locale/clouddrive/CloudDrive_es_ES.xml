<bundle>
  <UITabForm>
    <label>
      <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
      <showConnectCloudDrive>Conectar documentos en la nube</showConnectCloudDrive>
    </label>
  </UITabForm>
  <UIPopupWindow>
    <title>
      <ConnectCloudDriveForm>Conectar documentos en la nube</ConnectCloudDriveForm>
      <ConnectYour>Conecta tu</ConnectYour>
      <UISharingManager>Compartir archivos en la nube</UISharingManager>
    </title>
  </UIPopupWindow>
  <UIActionBar>
    <tooltip>
      <ShowConnectCloudDrive>Conectar documentos en la nube</ShowConnectCloudDrive>
    </tooltip>
  </UIActionBar>
  <ECMContextMenu>
    <event>
      <OpenCloudFile>Abrir en</OpenCloudFile>
      <SyncingFile>Sincronizando archivo...</SyncingFile>
      <PushCloudFile>Llevar a</PushCloudFile>
      <RefreshCloudDrive>Sincronizar con</RefreshCloudDrive>
      <RefreshView>Actualizar vista</RefreshView>
    </event>
  </ECMContextMenu>
  <CloudFileViewer>
    <event>
      <OpenFileOn>Abrir $fileUrl en $serviceName</OpenFileOn>
      <OpenOn>Abrir en</OpenOn>
      <EditFile>Editar</EditFile>
      <ViewFile>Ajuste</ViewFile>
    </event>
    <label>
      <sourceview>Ver código fuente</sourceview>
      <webview>Ver como página web</webview>
    </label>
  </CloudFileViewer>
  <CloudFile>
    <msg>
      <MoveToAnotherDriveNotSupported>Error al mover el archivo a otra unidad en la nube</MoveToAnotherDriveNotSupported>
      <MoveToOutsideDriveNotSupported>Error al extraer el archivo de su unidad en la nube</MoveToOutsideDriveNotSupported>
      <MoveBetweenWorkspacesNotSupported>Error al mover el archivo en la nube a otro espacio de trabajo</MoveBetweenWorkspacesNotSupported>
      <CloudDriveCopyMoveNotSupported>Error al copiar o mover la carpeta de la unidad en la nube</CloudDriveCopyMoveNotSupported>
      <DestinationNotWrittable>Error al escribir en la carpeta de destino</DestinationNotWrittable>
      <DestinationNotDefined>La carpeta de destino no está definida</DestinationNotDefined>
      <SourceNotDefined>El archivo de origen no está definido</SourceNotDefined>
      <ErrorMoveToDestination>Error al mover el enlace compartido al destino</ErrorMoveToDestination>
      <DestinationItemNameExists>Ya existe un elemento con ese nombre en el destino</DestinationItemNameExists>
      <LinkCreated>Se ha creado el enlace {0}</LinkCreated>
      <FileLinksCreated>Archivo {0} vinculado en {1}</FileLinksCreated>
      <FileLinksSharedInSpace>Enlace del archivo {0} compartido en {1} del espacio {2}</FileLinksSharedInSpace>
      <FileLinksSharedInGroup>Enlace del archivo {0} compartido en {1} del grupo {2}</FileLinksSharedInGroup>
      <FileLinksSharedWithUser>Enlace del archivo {0} compartido con {2} en su carpeta {1}</FileLinksSharedWithUser>
      <FileLinksNotSupportedToThisDestination>Error al vincular el archivo a este destino</FileLinksNotSupportedToThisDestination>
      <ErrorReadingDrives>Error al leer unidades de documentos</ErrorReadingDrives>
      <CannotFindUser>No se ha encontrado el usuario {0}</CannotFindUser>
      <ErrorReadingFileLinks>Error al leer enlaces del archivo</ErrorReadingFileLinks>
      <ErrorReadingUserDrive>Error al leer la unidad del usuario</ErrorReadingUserDrive>
    </msg>
  </CloudFile>
  <ConnectCloudDriveForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </ConnectCloudDriveForm>
  <ConnectDriveDrawer>
    <title>
      <ConnectYourService>Conecta tu servicio</ConnectYourService>
    </title>
  </ConnectDriveDrawer>
  <cloudStorage>
    <label>
      <title>Almacenamiento en la nube</title>
      <cloudDrives>Unidades en la nube</cloudDrives>
      <provider>Proveedor</provider>
      <description>Descripción</description>
      <active>Activo</active>
      <ConnectionToCloud>Conexión con unidades en la nube</ConnectionToCloud>
    </label>
  </cloudStorage>
</bundle>
