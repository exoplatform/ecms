<head>
  <style>
    .tabNormal {
      float: left; /* orientation=lt */
      float: right; /* orientation=rt */
      background: url('/eXoDMSResources/skin/images/metadata/Metadata.gif') repeat-x left top;
      height: 20px; width: 120px; line-height: 20px; text-align: center;
      border: solid 1px #a3abb6;
      font-size: 12px; font-weight: bold;
    }
    
    .tabSelected {
      float: left; /* orientation=lt */
      float: right; /* orientation=rt */
      background: url('/eXoDMSResources/skin/images/metadata/Metadata.gif') repeat-x left -20px;
      height: 20px; width: 120px; line-height: 20px; text-align: center;
      border: solid 1px #a3abb6;
      font-size: 12px; font-weight: bold;
    }
    
    .ContentBlock {
      margin: 15px;
    }
    
    .LabelMetadata {
    	border-bottom: 1px dotted #666666;
    	padding-bottom: 5px;
      height:14px;
      font-weight: bold;
      font-size: 12px;
      font-style: Verdana;
      text-align: left; /* orientation=lt */
      text-align: right; /* orientation=rt */
      color: #f68822;
    }
    
    .Header {
      font-weight: bold;
      font-size: 14px;
      font-style: Verdana;
      text-align: left; /* orientation=lt */
      text-align: right; /* orientation=rt */
    }
  </style>   
</head>
<%
	public void renderDCValues(String name) {
	  def node = uicomponent.getViewNode("dc:elementSet") ;
		def values = uicomponent.getMultiValues(node, name) ;
		if(values.size() == 0) { %>
	    <div style="overflow:hidden; width:99.5%; margin-top:5px; margin-left:5px">
	      <div style="float:left"><img style="height:4px; vertical-align:middle; margin-right:3px" alt="" src='/eXoSkin/skin/ecm/portlet/explorer/images/metadata/rect.gif'/></div>     
	      <div style="text-align: left"><%=_ctx.appRes("ElementSet.view.label.not-avaliable")%></div>
	    </div>    		
	<%} else {
			for(value in values) { %>
	      <div style="overflow:hidden; width:99.5%; margin-top:5px; margin-left:5px">
	        <div style="float:left"><img style="height:4px; vertical-align:middle; margin-right:3px" alt="" src='/eXoSkin/skin/ecm/portlet/explorer/images/metadata/rect.gif'/></div>     
	        <div style="font-size:12px; text-align: left ">$value</div>
	      </div>    			
	<%	}
		}
	}
%>

