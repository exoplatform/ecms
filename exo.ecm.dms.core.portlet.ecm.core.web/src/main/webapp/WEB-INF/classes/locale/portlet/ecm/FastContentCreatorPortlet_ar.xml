<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################################################################################
  #Edit mode																		  #
  ###################################################################################
  -->
  <UIEditModeConfiguration>
    <label>
      <selectTemplate>اختيار القالب</selectTemplate>
      <select-repository>اختيار مستودع البيانات</select-repository>
      <actionInput>UIEditModeConfiguration</actionInput>
      <workspaceName>اختيار فضاء العمل</workspaceName>
      <savedPath>موقع الحفظ</savedPath>
      <repositoryName>الرجاء اختيار مستودع البيانات</repositoryName>
    </label>
    <msg>
      <save-successfully>عملية تسجيل افضليات المدخل قد تمت بنجاح</save-successfully>
      <item-not-found>مسار المجلد غير صحيح</item-not-found>
