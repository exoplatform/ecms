  ###################################################################################
  # org.exoplatform.ecm.jcr.SearchValidator																			#
  ###################################################################################
  -->
  <SearchValidator>
    <msg>
      <empty-input>De waarde van dit veld is leeg of leeg !</empty-input>
      <Invalid-char>Er zijn een aantal ongeldige tekens in dit veld. Typ een andere waarde, alsjeblieft !</Invalid-char>
      <Invalid-char2>Aantal moet worden na de char '~'</Invalid-char2>
      <Invalid-char1>Aantal volg de char '~' moet tussen 0 en 1</Invalid-char1>
      <Invalid-char3>Aantal moet worden na het teken '^'</Invalid-char3>
      <Invalid-char4>Geen ruimte is toegestaan in de buurt van de char '~'</Invalid-char4>
      <Invalid-char5>Geen ruimte is toegestaan in de buurt van het teken '^'</Invalid-char5>
    </msg>
  </SearchValidator>
  <!--
  ######################################################################################
  # Since ECM 2.3
  ######################################################################################
  -->
  <UIPathConfig>
    <label>
      <searchEnable>Inschakelen zoek locatie</searchEnable>
      <searchLocation>Zoek locatie</searchLocation>
      <searchPath>UIPathConfig</searchPath>
      <explain></explain>
    </label>
    <msg>
      <enter-search-location>Je moet de locatie zoeken</enter-search-location>
      <explain>Als het pad in Dadaïsten inhoud komt overeen met een hiërarchie van de categorieën, is het nodig om het pad waar de documenten zijn opgeslagen, zodat ze kunnen worden gevonden door de zoekfunctie van de portletrepository.</explain>
    </msg>
  </UIPathConfig>
  
  <UIDocumentForm>
  	<msg>
  		<not-support>Kan niet laden sjabloon van '{0}'.</not-support>
  		<do-not-permission>U heeft geen toestemming voor het laden van de sjabloon '(0)'.</do-not-permission>
  	</msg>
  </UIDocumentForm>
  <!--
	  ######################################################################################
	  # Since ECM 2.6
	  ######################################################################################
	  -->
  <Comments>
	  <tooltip>
	    <remove-comment>Remove this comment</remove-comment>
	    <edit-comment>Edit this comment</edit-comment>
	  </tooltip>
		</Comments>
		<UIDocumentDetail>
		  <msg>
		    <confirm-deletecomment>Do you really want to delete this comment?</confirm-deletecomment>
		  </msg>
		</UIDocumentDetail>
		<UIPopupWindow>
			<title>
				<UICBCommentForm>Input comment</UICBCommentForm>
