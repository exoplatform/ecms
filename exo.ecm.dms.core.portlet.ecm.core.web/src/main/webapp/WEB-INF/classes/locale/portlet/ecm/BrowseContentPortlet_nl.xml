<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UIConfigTabPane #
  ###################################################################
  -->
  <UIConfigTabPane>
    <tab>
      <label>
        <UIPathConfig>Configuratie pad</UIPathConfig>
        <UINewConfigForm>Nieuwe</UINewConfigForm>
      </label>
    </tab>
  </UIConfigTabPane>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UIPathConfig #
  ###################################################################
  -->
  <!--
    ###################################################################
		#	    FROM VERSION DMS 2.3-SNAPSHOT, we add filterCategory        #
		#	    to display checkbox for filtering category tree             #
		#	    base on permission of user                                  #
    ###################################################################
   -->
  <UIPathConfig>
    <label>
      <categoryPath>Categories Pad</categoryPath>
      <workspace>Werkruimte</workspace>
      <repository>Bewaarplaats</repository>
      <path>UIPathConfig</path>
