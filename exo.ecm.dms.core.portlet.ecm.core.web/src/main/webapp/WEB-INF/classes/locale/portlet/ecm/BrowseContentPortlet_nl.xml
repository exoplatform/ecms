    <header>
      <name>Naam</name>
      <action>Actie</action>
    </header>
    <info>
      <child-not-found>Heeft geen child</child-not-found>
    </info>
  </UITreeList>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UICBCommentForm #
  ###################################################################
  -->
  <UICBCommentForm>
    <label>
      <title>commentaar toevoegen</title>
      <name>Naam</name>
      <email>E-mailadres</email>
      <website>Website</website>
      <comment>Commentaar</comment>
    </label>
    <action>
      <Cancel>#{word.cancel}</Cancel>
      <Save>#{word.save}</Save>
    </action>
    <msg>
      <locked-doc>Document is op slot</locked-doc>
      <versioning-doc>Document is versie</versioning-doc>
      <error-vote>Probleem bij het toevoegen van commentaar</error-vote>
      <comment-required>Commentaar is vereist</comment-required>
