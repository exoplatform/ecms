    <label>
      <workspaceName>Werkruimte</workspaceName>
      <rootNodeInfo>Root pad</rootNodeInfo>
      <rootPath>Root pad</rootPath>
    </label>
  </UIWorkspaceList>
  <!--
  ###################################################################################
  # UITagList																		  #
  ###################################################################################
  -->
  <UITagList>
    <label>
      <tagTitle>Tag Map</tagTitle>
    </label>
  </UITagList>
  <UICategoryTree>
    <label>
      <title>Categories</title>
    </label>
