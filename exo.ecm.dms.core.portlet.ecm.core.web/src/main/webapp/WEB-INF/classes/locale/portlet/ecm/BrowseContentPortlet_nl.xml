			<msg>
				<confirm-deleteattachment>Do you want to remove this attachment file??</confirm-deleteattachment>
			</msg>
		</RemoveAttachmentComponent>
  <UIDocumentDetail>
  	<tooltip>
  		<remove-attachment>Delete this attached file</remove-attachment>
  	</tooltip>
  </UIDocumentDetail>
  <DocumentDetail>
