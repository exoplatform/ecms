    <label>
      <itemsPerPageSelector>Elemente pe pagină</itemsPerPageSelector>
      <searchFormSelector>Șablon formular căutare</searchFormSelector>
      <searchResultSelector>Șablon rezultate căutare</searchResultSelector>
