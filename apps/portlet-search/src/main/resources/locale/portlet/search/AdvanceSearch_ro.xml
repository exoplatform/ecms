<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>Cuvânt cheie</keyword>
      <portal>Portal</portal>
      <Page>Pagină</Page>
      <Document>Document</Document>
    </label>
    <action>
      <Search>Căutare</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
    <label>
      <Page>Pagină</Page>
      <Document>Document</Document>
