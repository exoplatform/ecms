  <UISearchPageLayoutDefault>
    <label>
      <quickEditTitle>Editare rapidă</quickEditTitle>
    </label>
  </UISearchPageLayoutDefault>
  <searchOption>
    <label>
      <pageCheckBox>în paginile</pageCheckBox>
      <documentCheckBox>în conţinut</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>căutare în toate site-urile</all>
      </option>  
    </label>  
