      <ofAboutCaption>de aproximativ</ofAboutCaption>
      <timeUnitCaption>secunde</timeUnitCaption>
      <similar>Pagini în care apare același conținut</similar>
      <editor>Editor</editor>
      <date>Data</date>
      <relevancy>Relevanţa</relevancy>
      <title>Titlu</title>
      <sortBy>Sortează după:</sortBy>
    </label>
    <action>
      <edit>Modifică</edit>
    </action>
    <msg>
      <search-not-match-type>Căutarea {0} nu corespunde niciunui {1}.</search-not-match-type>
    </msg>
