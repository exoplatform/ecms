<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>Hakusana</keyword>
      <portal>Portaali</portal>
      <Page>Sivu</Page>
      <Document>Asiakirja</Document>
    </label>
    <action>
      <Search>Hae</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
    <label>
      <Page>Sivu</Page>
      <Document>Asiakirja</Document>
      <DocumentAndPage>Asiakirja ja sivu</DocumentAndPage>
      <documentandpage>Asiakirja ja sivu</documentandpage>
      <results>Results {0} to {1} for {2}</results>
      <oneResult>Only one result for {0}</oneResult>
      <ofAboutCaption>of about</ofAboutCaption>
      <timeUnitCaption>sekuntia</timeUnitCaption>
      <similar>Sivut, joilla sama sisältö esiintyy</similar>
      <editor>Editor</editor>
      <date>Päivämäärä</date>
      <relevancy>Relevance</relevancy>
      <title>Otsikko</title>
      <sortBy>Lajittele (jnkn mukaan):</sortBy>
    </label>
    <action>
      <edit>Muokkaa</edit>
    </action>
    <msg>
      <search-not-match-type>Hakusi {0} ei vastaa yhtään {1}.</search-not-match-type>
    </msg>
  </UIDefaultSearchResult>
  <UISearchPageLayoutManager>
    <label>
      <itemsPerPageSelector>Kohteita sivulla</itemsPerPageSelector>
      <searchFormSelector>Search Form Template</searchFormSelector>
      <searchResultSelector>Search Result Template</searchResultSelector>
      <searchPaginatorSelector>Search Paginator Template</searchPaginatorSelector>
      <searchPageLayoutSelector>Search Page Layout Template</searchPageLayoutSelector>
      <searchResultBasePathInputSetAction>Peruspolku</searchResultBasePathInputSetAction>
      <pageMode>Page Mode</pageMode>
      <option>
        <none>Ei mitään</none>
        <more>Lisää</more>
        <pagination>Sivutus</pagination>
      </option>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <searchResultBasePathInputSetAction>
    <title>
      <SelectBasePath>Valitse peruspolku</SelectBasePath>
    </title>
  </searchResultBasePathInputSetAction>  
  <UISearchForm>  
   <message>
     <keyword-not-support>The keyword is invalid. Some characters are not recognized.</keyword-not-support>
     <not-checked>Please select a search type.</not-checked>
     <keyword-not-empty>Please enter a keyword.</keyword-not-empty>
   </message>
  </UISearchForm>
  <UIPaginatorDefault>
    <label>
      <next>Seuraava</next>
      <previous>Edellinen</previous>
      <more>Lisää</more>
    </label>
  </UIPaginatorDefault>
  <UISearchPageLayoutDefault>
    <label>
      <quickEditTitle>Nopea muokkaus</quickEditTitle>
    </label>
  </UISearchPageLayoutDefault>
  <searchOption>
    <label>
      <pageCheckBox>in Pages</pageCheckBox>
      <documentCheckBox>in Content</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>hae kaikilta sivustoilta</all>
      </option>  
    </label>  
  </ViewMode>
</bundle>
