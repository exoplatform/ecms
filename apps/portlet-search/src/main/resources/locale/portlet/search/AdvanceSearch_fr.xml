<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>Mot-clé</keyword>
      <portal>Site</portal>
      <Page>Page</Page>
      <Document>Document</Document>
    </label>
    <action>
      <Search>Recherche</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
    <label>
      <Page>Page</Page>
      <Document>Document</Document>
      <DocumentAndPage>Document et Page</DocumentAndPage>
      <documentandpage>Document et Page</documentandpage>
      <results>Résultats {0} à {1} pour {2}</results>
      <oneResult>Un seul résultat pour {0}</oneResult>
      <ofAboutCaption>sur</ofAboutCaption>
      <timeUnitCaption>secondes</timeUnitCaption>
      <similar>Pages où ce contenu apparaît</similar>
      <editor>Editeur</editor>
      <date>Date</date>
      <relevancy>Pertinence</relevancy>
      <title>Titre</title>
      <sortBy>Trier Par:</sortBy>
    </label>
    <action>
      <edit>Modifier</edit>
    </action>
    <msg>
      <search-not-match-type>Votre recherche {0} ne correspond à aucun(e) {1}.</search-not-match-type>
    </msg>
  </UIDefaultSearchResult>
  <UISearchPageLayoutManager>
    <label>
      <itemsPerPageSelector>Articles par page</itemsPerPageSelector>
      <searchFormSelector>Page de Recherche</searchFormSelector>
      <searchResultSelector>Modèle de Résultats de Recherche</searchResultSelector>
      <searchPaginatorSelector>Pagination de Recherche</searchPaginatorSelector>
      <searchPageLayoutSelector>Modèle de Page de Recherche</searchPageLayoutSelector>
      <searchResultBasePathInputSetAction>Chemin de Base</searchResultBasePathInputSetAction>
      <pageMode>Mode de Page</pageMode>
      <option>
        <none>Aucun</none>
        <more>Plus</more>
        <pagination>Pagination</pagination>
      </option>      
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <searchResultBasePathInputSetAction>
    <title>
      <SelectBasePath>Sélectionnez le Chemin de Base</SelectBasePath>
    </title>
  </searchResultBasePathInputSetAction>
  <UISearchForm>
  	 <message>
  	    <keyword-not-support>Le mot clé est invalide. Certains caractères ne sont pas autorisés.</keyword-not-support>
        <not-checked>Le type de recherche n'est pas sélectionné. Veuillez en choisir un.</not-checked>
     <keyword-not-empty>Veuillez saisir un mot clé.</keyword-not-empty>
  	 </message>
  </UISearchForm>
  <UIPaginatorDefault>
    <label>
      <next>Suivant</next>
      <previous>Précédent</previous>
      <more>Plus</more>
    </label>
  </UIPaginatorDefault>
  <UISearchPageLayoutDefault>
    <label>
      <quickEditTitle>Modification Rapide</quickEditTitle>
    </label>
  </UISearchPageLayoutDefault>  
  <searchOption>
    <label>
      <pageCheckBox>dans les Pages</pageCheckBox>
      <documentCheckBox>dans les Contenus</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>recherchez dans tous les sites</all>
      </option>  
    </label>  
  </ViewMode>  
</bundle>