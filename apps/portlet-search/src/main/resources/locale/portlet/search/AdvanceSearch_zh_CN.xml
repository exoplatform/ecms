<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>关键字</keyword>
      <portal>门户</portal>
      <Page>页面</Page>
      <Document>文档</Document>
    </label>
    <action>
      <Search>搜索</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
    <label>
      <Page>页面</Page>
      <Document>文档</Document>
      <DocumentAndPage>文档和页面</DocumentAndPage>
      <documentandpage>文档和页面</documentandpage>
      <results>{2} 结果 {0} 到 {1}</results>
      <oneResult>只有一个结果为 {0}</oneResult>
      <ofAboutCaption>约</ofAboutCaption>
      <timeUnitCaption>秒</timeUnitCaption>
      <similar>网页相同的内容的显示位置</similar>
      <editor>编辑器</editor>
      <date>日期</date>
      <relevancy>相关性</relevancy>
      <title>标题</title>
      <sortBy>排序依据:</sortBy>
    </label>
    <action>
      <edit>编辑</edit>
    </action>
    <msg>
      <search-not-match-type>Your search {0} does not match any {1}.</search-not-match-type>
    </msg>
  </UIDefaultSearchResult>
  <UISearchPageLayoutManager>
    <label>
      <itemsPerPageSelector>每页显示</itemsPerPageSelector>
      <searchFormSelector>搜索表单模板</searchFormSelector>
      <searchResultSelector>搜索结果模板</searchResultSelector>
      <searchPaginatorSelector>搜索分页程序模板</searchPaginatorSelector>
      <searchPageLayoutSelector>搜索页面布局模板</searchPageLayoutSelector>
      <searchResultBasePathInputSetAction>基路径</searchResultBasePathInputSetAction>
      <pageMode>页面模式</pageMode>
      <option>
        <none>无</none>
        <more>更多</more>
        <pagination>分页</pagination>
      </option>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <searchResultBasePathInputSetAction>
    <title>
      <SelectBasePath>选择基路径</SelectBasePath>
    </title>
  </searchResultBasePathInputSetAction>  
  <UISearchForm>  
   <message>
     <keyword-not-support>关键字是无效的。某些字符不能识别。</keyword-not-support>
     <not-checked>请选择搜索类型。</not-checked>
     <keyword-not-empty>请输入一个关键字。</keyword-not-empty>
   </message>
  </UISearchForm>
  <UIPaginatorDefault>
    <label>
      <next>下一个</next>
      <previous>上一个</previous>
      <more>更多</more>
    </label>
  </UIPaginatorDefault>
  <UISearchPageLayoutDefault>
    <label>
      <quickEditTitle>快速编辑</quickEditTitle>
    </label>
  </UISearchPageLayoutDefault>
  <searchOption>
    <label>
      <pageCheckBox>在页中</pageCheckBox>
      <documentCheckBox>在内容中</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>搜索所有站点</all>
      </option>  
    </label>  
  </ViewMode>
</bundle>
