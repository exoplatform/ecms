<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>Sökord</keyword>
      <portal>Portal</portal>
      <Page>Sida</Page>
      <Document>Dokument</Document>
    </label>
    <action>
      <Search>Sök</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
    <label>
      <Page>Sida</Page>
      <Document>Dokument</Document>
      <DocumentAndPage>Dokument och sida</DocumentAndPage>
      <documentandpage>Dokument och sida</documentandpage>
      <results>Results {0} to {1} for {2}</results>
      <oneResult>Only one result for {0}</oneResult>
      <ofAboutCaption>om</ofAboutCaption>
      <timeUnitCaption>sekunder</timeUnitCaption>
      <similar>Sidor där samma innehåll visas</similar>
      <editor>Redigerare</editor>
      <date>Datum</date>
      <relevancy>Betydelse för</relevancy>
      <title>Titel</title>
      <sortBy>Sortera efter:</sortBy>
    </label>
    <action>
      <edit>Redigera</edit>
    </action>
    <msg>
      <search-not-match-type>Your search {0} does not match any {1}.</search-not-match-type>
    </msg>
  </UIDefaultSearchResult>
  <UISearchPageLayoutManager>
    <label>
      <itemsPerPageSelector>Poster per sida</itemsPerPageSelector>
      <searchFormSelector>Sök formulärmall</searchFormSelector>
      <searchResultSelector>Sök resultat</searchResultSelector>
      <searchPaginatorSelector>Sök sidnumrering</searchPaginatorSelector>
      <searchPageLayoutSelector>Sök sidlayout</searchPageLayoutSelector>
      <searchResultBasePathInputSetAction>Rotkatalog</searchResultBasePathInputSetAction>
      <pageMode>Sidinställning</pageMode>
      <option>
        <none>Ingen</none>
        <more>Mer</more>
        <pagination>Sidnumrering</pagination>
      </option>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <searchResultBasePathInputSetAction>
    <title>
      <SelectBasePath>Välj rotkatalog</SelectBasePath>
    </title>
  </searchResultBasePathInputSetAction>  
  <UISearchForm>  
   <message>
     <keyword-not-support>Sökord ogiltigt. Innehåller okända tecken.</keyword-not-support>
     <not-checked>Välj typ av sök.</not-checked>
     <keyword-not-empty>Skriv in ett sökord.</keyword-not-empty>
   </message>
  </UISearchForm>
  <UIPaginatorDefault>
    <label>
      <next>Nästa</next>
      <previous>Föregående</previous>
      <more>Mer</more>
    </label>
  </UIPaginatorDefault>
  <UISearchPageLayoutDefault>
    <label>
      <quickEditTitle>Snabbredigering</quickEditTitle>
    </label>
  </UISearchPageLayoutDefault>
  <searchOption>
    <label>
      <pageCheckBox>i sidor</pageCheckBox>
      <documentCheckBox>i innehåll</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>Sök alla platser</all>
      </option>  
    </label>  
  </ViewMode>
</bundle>
