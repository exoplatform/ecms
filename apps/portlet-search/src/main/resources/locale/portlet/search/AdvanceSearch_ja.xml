    <action>
      <Save>保存</Save>
      <Cancel>キャンセル</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <searchResultBasePathInputSetAction>
    <title>
      <SelectBasePath>ベースパスを選択</SelectBasePath>
    </title>
  </searchResultBasePathInputSetAction>  
  <UISearchForm>  
   <message>
