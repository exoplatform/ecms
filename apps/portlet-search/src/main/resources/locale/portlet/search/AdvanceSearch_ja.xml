<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>キーワード</keyword>
      <portal>ポータル</portal>
      <Page>ページ</Page>
      <Document>ドキュメント</Document>
    </label>
    <action>
      <Search>検索</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
