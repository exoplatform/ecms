  <searchOption>
    <label>
      <pageCheckBox>Sayfaları</pageCheckBox>
      <documentCheckBox>İçeriği</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>tüm sitelerde ara</all>
