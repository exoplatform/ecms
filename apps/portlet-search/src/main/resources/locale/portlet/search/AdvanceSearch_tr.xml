      <option>
        <none>Hiçbiri</none>
        <more>Daha</more>
        <pagination>Sayfalandırma</pagination>
      </option>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <searchResultBasePathInputSetAction>
    <title>
      <SelectBasePath>Ana Yolu Seçin</SelectBasePath>
    </title>
  </searchResultBasePathInputSetAction>  
