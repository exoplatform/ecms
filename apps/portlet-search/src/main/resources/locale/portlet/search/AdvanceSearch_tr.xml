      <timeUnitCaption>saniye</timeUnitCaption>
      <similar>Aynı içeriğin görüntülendiği sayfalar</similar>
      <editor>Düzenleyen</editor>
      <date>Tarih</date>
      <relevancy>İlgi</relevancy>
      <title>Başlık</title>
      <sortBy>Sıralama ölçütü:</sortBy>
    </label>
    <action>
      <edit>Düzenle</edit>
    </action>
    <msg>
      <search-not-match-type>{0} aramanız {1} ile eşleşmedi.</search-not-match-type>
    </msg>
