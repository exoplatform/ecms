<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>Anahtar Kelime</keyword>
      <portal>Portal</portal>
      <Page>Sayfa</Page>
      <Document>Belge</Document>
    </label>
    <action>
      <Search>Ara</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
