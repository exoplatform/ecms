<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>Ключевое слово</keyword>
      <portal>Портал</portal>
      <Page>Страница</Page>
      <Document>Документ</Document>
    </label>
    <action>
      <Search>Поиск</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
    <label>
      <Page>Страница</Page>
      <Document>Документ</Document>
      <DocumentAndPage>Документ и страница</DocumentAndPage>
      <documentandpage>Документ и страница</documentandpage>
      <resutlsCaption>Результаты</resutlsCaption>
      <ofAboutCaption>of about</ofAboutCaption>
      <forCaption>for</forCaption>
      <timeUnitCaption>секунд</timeUnitCaption>
      <similar>Страницы с тем же содержимым</similar>
      <editor>Редактор</editor>
      <date>Дата</date>
      <relevancy>Relevance</relevancy>
      <title>Заголовок</title>
      <sortBy>Sort By:</sortBy>
    </label>
    <action>
      <edit>Редактировать</edit>
    </action>
    <msg>
      <your-search>Ваш поиск</your-search>
      <did-not-match>совпадений не найдено</did-not-match>
      <new-suggestion>предложение</new-suggestion>
    </msg>
  </UIDefaultSearchResult>
  <UISearchPageLayoutManager>
    <label>
      <itemsPerPageSelector>Результатов на странице</itemsPerPageSelector>
      <searchFormSelector>Поиск по шаблону</searchFormSelector>
      <searchResultSelector>Шаблон результатов поиска</searchResultSelector>
      <searchPaginatorSelector>Search Paginator Template</searchPaginatorSelector>
      <searchPageLayoutSelector>Search Page Layout Template</searchPageLayoutSelector>
      <searchResultBasePathInputSetAction>Base Path</searchResultBasePathInputSetAction>
      <pageMode>Режим страницы</pageMode>
      <option>
        <none>None</none>
        <more>Еще</more>
        <pagination>Pagination</pagination>
      </option>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <searchResultBasePathInputSetAction>
    <title>
      <SelectBasePath>Select Base Path</SelectBasePath>
    </title>
  </searchResultBasePathInputSetAction>  
  <UISearchForm>  
   <message>
     <keyword-not-support>Неверное ключевое слово. Некоторые символы не распознаны.</keyword-not-support>
     <not-checked>Пожалуйста, выберите тип поиска.</not-checked>
     <keyword-not-empty>Пожалуйста, введите ключевое слово.</keyword-not-empty>
   </message>
  </UISearchForm>
  <UIPaginatorDefault>
    <label>
      <next>Далее</next>
      <previous>Предыдущий</previous>
      <more>Еще</more>
    </label>
  </UIPaginatorDefault>
  <UISearchPageLayoutDefault>
    <label>
      <quickEditTitle>Быстрое редактирование</quickEditTitle>
    </label>
  </UISearchPageLayoutDefault>
  <searchOption>
    <label>
      <pageCheckBox>in Pages</pageCheckBox>
      <documentCheckBox>in Content</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>search all sites</all>
      </option>  
    </label>  
  </ViewMode>
</bundle>
