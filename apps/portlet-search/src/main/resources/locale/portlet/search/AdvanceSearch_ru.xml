<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>Ключевое слово</keyword>
      <portal>Портал</portal>
      <Page>Страница</Page>
      <Document>Документ</Document>
    </label>
    <action>
      <Search>Поиск</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
    <label>
      <Page>Страница</Page>
      <Document>Документ</Document>
      <DocumentAndPage>Документ и страница</DocumentAndPage>
      <documentandpage>Документ и страница</documentandpage>
      <results>Результат с {0} по {1} для {2}</results>
      <oneResult>Только один результат для {0}</oneResult>
      <ofAboutCaption>около</ofAboutCaption>
      <timeUnitCaption>секунд</timeUnitCaption>
      <similar>Страницы с тем же содержимым</similar>
      <editor>Редактор</editor>
      <date>Дата</date>
      <relevancy>Релевантность</relevancy>
      <title>Заголовок</title>
      <sortBy>Сортировать по:</sortBy>
    </label>
    <action>
      <edit>Редактировать</edit>
    </action>
    <msg>
      <search-not-match-type>Ваш запрос {0} не соответствует ни одному из {1}.</search-not-match-type>
    </msg>
  </UIDefaultSearchResult>
  <UISearchPageLayoutManager>
    <label>
      <itemsPerPageSelector>Результатов на странице</itemsPerPageSelector>
      <searchFormSelector>Шаблон формы поиска</searchFormSelector>
      <searchResultSelector>Шаблон результатов поиска</searchResultSelector>
      <searchPaginatorSelector>Поиск шаблона разбивки на страницы</searchPaginatorSelector>
      <searchPageLayoutSelector>Поиск шаблона макета страницы</searchPageLayoutSelector>
      <searchResultBasePathInputSetAction>Базовый путь</searchResultBasePathInputSetAction>
      <pageMode>Режим страницы</pageMode>
      <option>
        <none>Нет</none>
        <more>Еще</more>
        <pagination>Разбивка на страницы</pagination>
      </option>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <searchResultBasePathInputSetAction>
    <title>
      <SelectBasePath>Выбрать базовый путь</SelectBasePath>
    </title>
  </searchResultBasePathInputSetAction>  
  <UISearchForm>  
   <message>
     <keyword-not-support>Неверное ключевое слово. Некоторые символы не распознаны.</keyword-not-support>
     <not-checked>Пожалуйста, выберите тип поиска.</not-checked>
     <keyword-not-empty>Пожалуйста, введите ключевое слово.</keyword-not-empty>
   </message>
  </UISearchForm>
  <UIPaginatorDefault>
    <label>
      <next>Вперед</next>
      <previous>Предыдущий</previous>
      <more>Еще</more>
    </label>
  </UIPaginatorDefault>
  <UISearchPageLayoutDefault>
    <label>
      <quickEditTitle>Быстрое редактирование</quickEditTitle>
    </label>
  </UISearchPageLayoutDefault>
  <searchOption>
    <label>
      <pageCheckBox>на страницах</pageCheckBox>
      <documentCheckBox>в содержании</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>поиск всех сайтов</all>
      </option>  
    </label>  
  </ViewMode>
</bundle>
