      <documentandpage>Dokumen dan Halaman</documentandpage>
      <results>Results {0} to {1} for {2}</results>
      <oneResult>Only one result for {0}</oneResult>
      <ofAboutCaption>of about</ofAboutCaption>
      <timeUnitCaption>seconds</timeUnitCaption>
      <similar>Halaman di mana kandungan yang sama muncul</similar>
      <editor>Editor</editor>
      <date>Tarikh</date>
      <relevancy>Relevan</relevancy>
