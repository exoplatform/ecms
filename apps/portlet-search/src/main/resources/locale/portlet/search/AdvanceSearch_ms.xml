<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>Keyword</keyword>
      <portal>Portal</portal>
      <Page>Page</Page>
      <Document>Document</Document>
    </label>
    <action>
      <Search>Carian</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
    <label>
      <Page>Page</Page>
      <Document>Document</Document>
