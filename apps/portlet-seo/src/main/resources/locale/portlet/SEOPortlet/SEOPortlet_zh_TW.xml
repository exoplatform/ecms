<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UISEOForm>
    <label>  
      <title>Title:</title>
      <description>Description:</description>
      <keywords>Keywords:</keywords>
      <language>Language:</language>
      <robots>Robots:</robots>
      <sitemap>Sitemap:</sitemap>
