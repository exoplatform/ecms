<bundle>
  <editors>
    <admin>
      <error>
        <ProviderNotRegistered>The editor provider is not registered.</ProviderNotRegistered>
        <EmptyRequest>The request should contain at least 1 form param (active/permissions).</EmptyRequest>
        <CannotGetProviders>Cannot get providers list.</CannotGetProviders>
        <PermissionNotValid>The specified permission is not valid. Probably the user or group doesn't exist.</PermissionNotValid>
        <CannotLoadIdentities>Cannot load identites by specified criteria</CannotLoadIdentities>
        <CannotSavePrefferedEditor>Cannot save selected editor as preffered one.</CannotSavePrefferedEditor>
      </error>
      <title>Document editors</title>
      <table>
        <Provider>Provider</Provider>
        <Description>Description</Description>
        <Active>Active</Active>
        <Permissions>Permissions</Permissions>
      </table>
      <modal>
        <title>Edit permissions</title>
        <SearchLabel>Enter user or group name</SearchLabel>
        <WithPermissions>Who has permission</WithPermissions>
        <None>None</None>
        <Everybody>Everybody</Everybody>
      </modal>
      <onlyoffice>
        <name>ONLYOFFICE</name>
        <description>ONLYOFFICE Online Document Editor</description>
      </onlyoffice>
      <officeonline>
        <name>Microsoft Office Online</name>
        <description>Microsoft Office Online editors for Word, Excel, Powerpoint</description>
      </officeonline>
      <buttons>
        <Save>Save</Save>
        <Cancel>Cancel</Cancel>
      </buttons>
    </admin>
  </editors>
</bundle>
