<bundle>
  <editors>
    <admin>
    <page>Editors Administration</page>
      <error>
        <ProviderNotRegistered>The editor provider is not registered.</ProviderNotRegistered>
        <EmptyRequest>The request should contain at least 1 form param (active/permissions).</EmptyRequest>
        <CannotGetProviders>Cannot get providers list.</CannotGetProviders>
        <PermissionNotValid>The specified permission is not valid. Probably the user or group doesn't exist.</PermissionNotValid>
        <CannotLoadIdentities>Cannot load identites by specified criteria</CannotLoadIdentities>
        <CannotSavePreferredEditor>Cannot save selected editor as preferred one.</CannotSavePreferredEditor>
        <UnableGetData>An error occurred during the data reading.</UnableGetData>
        <UnablePostData>An error occurred during the data saving.</UnablePostData>
      </error>
      <title>Document editors</title>
      <table>
        <Provider>مزود الخدمة</Provider>
        <Description>الوصف</Description>
        <Active>ناشط</Active>
        <Permissions>الصلاحيّات</Permissions>
      </table>
      <modal>
        <title>Edit permissions</title>
        <SearchLabel>Enter user or group name</SearchLabel>
        <WithPermissions>Who has permission</WithPermissions>
        <None>لا شَيْء</None>
        <Everybody>Everybody</Everybody>
      </modal>
      <buttons>
        <Save>حفظ</Save>
        <Cancel>الغاء</Cancel>
      </buttons>
    </admin>
    <buttons>
      <EditorButton>تغيير</EditorButton>
    </buttons>
    <idlePopup>
      <title>تحذير</title>
      <message>Seems you haven't been active for quite a long time. The editor page will be closed in %s seconds.</message>
    </idlePopup>
  </editors>
</bundle>
