<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>  
  <page>
  	<page-id>portal::empty::homepage</page-id>
    <owner-type>portal</owner-type>
    <owner-id>empty</owner-id>
    <name>homepage</name>
    <title>Home Page</title>
	  <access-permissions>Everyone</access-permissions>
	  <edit-permission>manager:/platform/web-contributors</edit-permission>
	  <container id="emptyHomepage" template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
	  	<application>
	      <instance-id>portal#empty:/presentation/SingleContentViewer/empty-homepage</instance-id>
	      <title>Homepage</title>
	    </application>
	  </container>
  </page>
  
  <page>
  	<page-id>portal::empty::search</page-id>
    <owner-type>portal</owner-type>
    <owner-id>empty</owner-id>
    <name>search</name>
    <title>Search</title>
	  <access-permissions>Everyone</access-permissions>
	  <edit-permission>manager:/platform/web-contributors</edit-permission>
    <application>
      <instance-id>portal#empty:/searches/WCMAdvanceSearchPortlet/empty-search</instance-id>
      <title>Search</title>
    </application>
  </page>
  
  <page>
  	<page-id>portal::empty::admintoolbar</page-id>
    <owner-type>portal</owner-type>
    <owner-id>empty</owner-id>
    <name>admintoolbar</name>
    <title>Toolbar</title>
	  <access-permissions>Everyone</access-permissions>
	  <edit-permission>manager:/platform/web-contributors</edit-permission>
    <application>
      <instance-id>portal#empty:/presentation/SiteAdministrationPortlet/empty-admintoolbar</instance-id>
      <title>AdminToolbar</title>
    </application>
  </page> 
  
  <page>
  	<page-id>portal::empty::contentviewer</page-id>
    <owner-type>portal</owner-type>
    <owner-id>empty</owner-id>
    <name>contentviewer</name>
    <title>ContentViewer</title>
	  <access-permissions>Everyone</access-permissions>
	  <edit-permission>manager:/platform/web-contributors</edit-permission>
    <application>
      <instance-id>portal#empty:/ecm/ContentViewerPortlet/empty-contentviewer</instance-id>
      <title>ContentViewer</title>
    </application>
  </page>
  
  <page>
  	<page-id>portal::empty::searchResult</page-id>
    <owner-type>portal</owner-type>
    <owner-id>empty</owner-id>
    <name>searchResult</name>
    <title>SearchResult</title>
	  <access-permissions>Everyone</access-permissions>
	  <edit-permission>manager:/platform/web-contributors</edit-permission>
    <application>
      <instance-id>portal#empty:/searches/WCMAdvanceSearchPortlet/empty-searchResult</instance-id>
      <title>SearchResult</title>
      <show-info-bar>false</show-info-bar>      
      <show-application-state>false</show-application-state>
      <show-application-mode>false</show-application-mode>
    </application>
  </page> 
  
  <page>
  	<page-id>portal::empty::parameterizedviewer</page-id>
    <owner-type>portal</owner-type>
    <owner-id>empty</owner-id>
    <name>parameterizedviewer</name>
    <title>ParameterizedViewer</title>
    <access-permissions>Everyone</access-permissions> 
    <edit-permission>manager:/platform/web-contributors</edit-permission>
    <application>
      <instance-id>portal#empty:/presentation/ParameterizedContentViewerPortlet/empty-parameterizedviewer</instance-id>
      <title>parameterizedviewer</title>
      <show-info-bar>false</show-info-bar>      
      <show-application-state>false</show-application-state>
      <show-application-mode>false</show-application-mode>
    </application>
  </page>
  <page>
  	<page-id>portal::empty::printviewer</page-id>
    <owner-type>portal</owner-type>
    <owner-id>empty</owner-id>
    <name>printviewer</name>
    <title>printviewer</title>
    <access-permissions>Everyone</access-permissions> 
    <edit-permission>manager:/platform/web-contributors</edit-permission>
    <show-max-window>true</show-max-window>
    <application>
      <instance-id>portal#empty:/presentation/ParameterizedContentViewerPortlet/empty-printviewer</instance-id>
      <title>printviewer</title>
      <show-info-bar>false</show-info-bar>      
      <show-application-state>false</show-application-state>
      <show-application-mode>false</show-application-mode>
    </application>
  </page>
</page-set>
