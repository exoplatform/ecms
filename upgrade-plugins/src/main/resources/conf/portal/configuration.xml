            <set-method>
                addUpgradePlugin
            </set-method>
            <type>
                org.exoplatform.ecms.upgrade.path.NodePathUpgradePlugin
            </type>
            <description>
                Upgrade nodes which path was changed
            </description>
            <init-params>
                <value-param>
                    <name>
                        product.group.id
                    </name>
                    <description>
                        The groupId of the product
                    </description>
                    <value>
                        org.exoplatform.ecms
